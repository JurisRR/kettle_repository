    <sql>select &#xd;&#xa;l.r_loan_id&#xd;&#xa;,l.amount_open_principal&#xd;&#xa;,l.amount_open_commission&#xd;&#xa;,l.amount_open_penalty&#xd;&#xa;,l.amount_open_principal &#x2b; l.amount_open_commission &#x2b; l.amount_open_penalty as open_total&#xd;&#xa;,&#x28;current_date - l.term_date&#x29; as current_delay&#xd;&#xa;&#xd;&#xa;,current_date as payment_extraction_date&#xd;&#xa;,&#x28;case when amount_repaid is null then 0 else amount_repaid end&#x29;&#x3a;&#x3a;double precision as amount_repaid&#xd;&#xa;,last_payment_date as last_payment_date&#xd;&#xa;--,ca.in_one_string as client_address&#xd;&#xa;--, case when l.status in &#x28;&#x27;Active&#x27;, &#x27;Completed&#x27;&#x29; then current_date else null end as task_end_date&#xd;&#xa;from &#xd;&#xa;r_loan l left outer join &#xd;&#xa;&#x28;select d.r_loan_id,  sum&#x28;d.amount&#x29; as amount_repaid , max&#x28;p.payment_date&#x29;&#x3a;&#x3a;date as last_payment_date&#xd;&#xa;	from r_payment_distr d join r_payment p on  &#x28;d.r_payment_id &#x3d; p.r_payment_id&#x29; &#xd;&#xa;where p.payment_date &#x3e;  &#x3f; &#xd;&#xa;and p.type &#x3d; &#x27;IN_Loan&#x27;&#xd;&#xa;group by d.r_loan_id&#x29;pp on &#x28;l.r_loan_id &#x3d; pp.r_loan_id&#x29;&#xd;&#xa;--left outer join r_client_address ca on &#x28;l.r_client_id &#x3d; ca.r_client_id and ca.type &#x3d; &#x27;Actual&#x27;&#x29;&#xd;&#xa;&#xd;&#xa;	&#xd;&#xa;-- &#xd;&#xa;&#xd;&#xa;--r_payment_distr d join r_payment p on  &#x28;d.r_payment_id &#x3d; p.r_payment_id&#x29; &#xd;&#xa;where 1&#x3d;1 &#xd;&#xa;	--and p.cl_bank_acct_id &#x3c;&#x3e; 18&#xd;&#xa;	--and p.type &#x3d; &#x27;IN_Loan&#x27;&#xd;&#xa;	--and p.cl_bank_acct_id &#x3c;&#x3e; 12&#xd;&#xa;	&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;and l.loan_no &#x3d;&#x3f;&#xd;&#xa;--and p.payment_date&#x3a;&#x3a;date &#x3e;&#x3d; &#x27;2017-01-17&#x27; &#xd;&#xa;--and p.payment_date&#x3a;&#x3a;date &#x3c; &#x3f;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;</sql>
