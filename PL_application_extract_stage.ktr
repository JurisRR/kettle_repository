    <connection>PL_Slave_Finotech</connection>
    <sql>with app_ranks as &#x28;SELECT a.id, seq_application_total, seq_application_confirm as confirmed_rank, seq_application_reject as rejected_rank&#xd;&#xa;,case when seq_application_reject &#x3e; 0 then seq_application_reject else seq_application_total- seq_application_confirm end as seq_application_reject&#xd;&#xa;,case when seq_application_confirm &#x3e; 0 then seq_application_confirm else seq_application_total- seq_application_reject end as seq_application_confirm&#xd;&#xa;,seq_application_per_loan&#xd;&#xa;  FROM lending.loan_application a join &#x28;select id, rank&#x28;&#x29; over &#x28;partition by client_id &#xd;&#xa;  order by client_id ASC, id ASC &#x29; as seq_application_total  from lending.loan_application &#x29;aa &#xd;&#xa;  on a.id &#x3d; aa.id&#xd;&#xa;&#xd;&#xa;  left outer join &#x28;select id, rank&#x28;&#x29; over &#x28;partition by client_id &#xd;&#xa;  order by client_id ASC, id ASC &#x29; as seq_application_confirm from lending.loan_application&#xd;&#xa;  where status &#x3d; &#x27;APPROVED&#x27;&#x29;ac &#xd;&#xa;  on a.id &#x3d; ac.id&#xd;&#xa;&#xd;&#xa;  left outer join &#x28;select id, rank&#x28;&#x29; over &#x28;partition by client_id &#xd;&#xa;  order by client_id ASC, id ASC &#x29; as seq_application_reject from lending.loan_application &#xd;&#xa;  where status &#x3d; &#x27;REJECTED&#x27;&#x29;ar &#xd;&#xa;  on a.id &#x3d; ar.id&#xd;&#xa;&#xd;&#xa;  left outer join &#x28;select id, rank&#x28;&#x29; over &#x28;partition by id, loans_paid&#xd;&#xa;  order by id ASC, loans_paid ASC, id ASC &#x29; as seq_application_per_loan from lending.loan_application &#xd;&#xa;  &#x29;aspl &#xd;&#xa;  on &#x28;a.id &#x3d; aspl.id&#x29;&#xd;&#xa;  &#xd;&#xa;  order by id, a.id&#x29; &#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;SELECT &#xd;&#xa;la.id as application_id&#xd;&#xa;, la.application_date&#x3a;&#x3a;date as app_date&#xd;&#xa;,extract&#x28;hour from la.created_at&#x29;&#x7c;&#x7c;&#x27;&#x3a;&#x27;&#x7c;&#x7c;extract&#x28;minutes from la.created_at&#x29; as application_time&#xd;&#xa;,extract&#x28;week from la.application_date&#x29; as app_iso_week&#xd;&#xa;,to_char&#x28;la.application_date, &#x27;day&#x27;&#x29; as app_day&#xd;&#xa;&#xd;&#xa;, la.client_id&#xd;&#xa;, la.offered_principal as amount&#xd;&#xa;, la.status&#xd;&#xa;, la.close_reason&#xd;&#xa;, 0 as score --&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23; add value&#x21;&#xd;&#xa;, la.loans_paid as finished_loan_cnt&#xd;&#xa;, la.loan_id&#xd;&#xa;, substring&#x28;l.loan_number from &#x27;..&#x24;&#x27;&#x29;&#x3a;&#x3a;numeric loan_seq&#xd;&#xa;, l.status as loan_status&#xd;&#xa;, la.offered_period_count&#xd;&#xa;, l.principal_disbursed&#xd;&#xa;, l.interest_applied&#xd;&#xa;, case when l.extensions&#x3e; 0 then 1 else 0 end as prolongated&#xd;&#xa;, lp.traffic_source as landing_page&#xd;&#xa;, pi.gender&#xd;&#xa;, extract&#x28;years from age&#x28;pi.date_of_birth&#x29;&#x29; as age  --&#x23;&#x23;&#x23; &#xd;&#xa;, div&#x28;extract&#x28;years from age&#x28;pi.date_of_birth&#x29;&#x29;&#x3a;&#x3a;int-1,5&#x29;&#x2a;5&#x2b;1 &#x7c;&#x7c;&#x27;-&#x27;&#x7c;&#x7c; div&#x28;extract&#x28;years from age&#x28;pi.date_of_birth&#x29;&#x29;&#x3a;&#x3a;int-1,5&#x29;&#x2a;5&#x2b;5 as age_group&#xd;&#xa;, &#x27;poland&#x27; as country&#xd;&#xa;, case when la.loans_paid &#x3d; 0 then &#x27;first&#x27; else &#x27;repeated&#x27; end as app_type&#xd;&#xa;&#xd;&#xa;, seq_application_total&#xd;&#xa;, seq_application_confirm&#xd;&#xa;, seq_application_reject&#xd;&#xa;, rejected_rank&#xd;&#xa;, confirmed_rank&#xd;&#xa;, seq_application_per_loan&#xd;&#xa;&#xd;&#xa;, ca.postalcode as postal&#xd;&#xa;, ca.city as powiat&#xd;&#xa;, ca.province as voivodship&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;,current_date as extraction_date&#xd;&#xa;,extract&#x28;hour from current_timestamp&#x29;&#x7c;&#x7c;&#x27;&#x3a;&#x27;&#x7c;&#x7c;extract&#x28;minutes from current_timestamp&#x29; as extraction_time&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;,case when now&#x28;&#x29; - la.created_at &#x3e; interval &#x27;24 hours&#x27; then &#x27;before&#x27; else &#x27;last 24 hours&#x27; end as last_24hours&#xd;&#xa;,case when now&#x28;&#x29; - la.created_at &#x3e; interval &#x27;7 days&#x27; then &#x27;before&#x27; else &#x27;last 7 days&#x27; end as last_7days&#xd;&#xa;,case when now&#x28;&#x29; - la.created_at &#x3e; interval &#x27;30 days&#x27; then &#x27;before&#x27; else &#x27;last 30 days&#x27; end as last_30days&#xd;&#xa;,extract&#x28;week from la.application_date&#x29; as app_isoweek&#xd;&#xa;,extract&#x28;month from la.application_date&#x29; as app_month&#xd;&#xa;,case when la.application_date&#x3a;&#x3a;date between current_date-7 and current_date-1 then &#x27;this week&#x27; &#xd;&#xa;	 when la.application_date&#x3a;&#x3a;date between current_date-14 and current_date-8 then &#x27;previous week&#x27; &#xd;&#xa;	else &#x27;history&#x27; end as week&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;,mt.utm_source&#xd;&#xa;,mt.utm_campaign&#xd;&#xa;, mt.referrer_domain&#xd;&#xa;, mt.event_date&#xd;&#xa;, mt.browser&#xd;&#xa;, mt.geo_location&#xd;&#xa;, mt.os&#xd;&#xa;, mt.device&#xd;&#xa;, mt.screen_size&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;--&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23; end of selected data &#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#xd;&#xa; from lending.loan_application la &#xd;&#xa;join lending.loan_application lla on &#x28;la.id &#x3d; lla.id&#x29;&#xd;&#xa;left outer join &#x28;&#xd;&#xa;&#xd;&#xa;SELECT client_id&#xd;&#xa;       , json_value&#x3a;&#x3a;json-&#x3e;&#x3e;&#x27;province&#x27; as province&#xd;&#xa;       , json_value&#x3a;&#x3a;json-&#x3e;&#x3e;&#x27;city&#x27; as city&#xd;&#xa;       , json_value&#x3a;&#x3a;json-&#x3e;&#x3e;&#x27;postalCode&#x27; as postalcode&#xd;&#xa;       , json_value&#x3a;&#x3a;json-&#x3e;&#x3e;&#x27;street&#x27; as street&#xd;&#xa;       , json_value&#x3a;&#x3a;json-&#x3e;&#x3e;&#x27;houseNumber&#x27; as housenumber&#xd;&#xa;       , json_value&#x3a;&#x3a;json-&#x3e;&#x3e;&#x27;apartmentNumber&#x27; as apartmentnumber&#xd;&#xa;    &#xd;&#xa;&#xd;&#xa;      FROM crm.client_address&#xd;&#xa;    where &#xd;&#xa;	type &#x3d; &#x27;ACTUAL&#x27;&#xd;&#xa;&#x29;ca  on &#x28;ca.client_id &#x3d; la.client_id&#x29;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;left outer join &#xd;&#xa;&#xd;&#xa;&#x28;select &#xd;&#xa;&#xd;&#xa;	workflow_id, number_value as app_id from workflow.workflow_param &#xd;&#xa;	where name &#x3d; &#x27;loanApplicationId&#x27;&#x29;app on &#x28;app.app_id &#x3d; la.id&#x29;&#xd;&#xa;	&#xd;&#xa;left outer join &#xd;&#xa;&#xd;&#xa;&#x28;select &#xd;&#xa;&#xd;&#xa;	workflow_id, text_value as resolution from workflow.workflow_param &#xd;&#xa;	where name &#x3d; &#x27;Resolution&#x27;&#x29;app2 on &#x28;app.workflow_id &#x3d; app2.workflow_id&#x29;&#xd;&#xa;&#xd;&#xa;left outer join crm.personal_info pi on &#x28;la.client_id &#x3d; pi.client_id&#x29;&#xd;&#xa;left outer join crm.email_login el on &#x28;la.client_id &#x3d; el.client_id&#x29;&#xd;&#xa;&#xd;&#xa;left outer join &#x28;SELECT &#xd;&#xa;&#xd;&#xa;	&#x2a;&#xd;&#xa;&#xd;&#xa;  FROM &#xd;&#xa;	&#x28;SELECT v.client_id, e.utm_source, e.utm_campaign, e.referrer_domain, e.created_at&#x3a;&#x3a;Date as event_date,&#xd;&#xa;	e.browser, e.geo_location, e.os, e.device, e.screen_size, la.id&#xd;&#xa;	,rank&#x28;&#x29; over &#x28;partition by la.id order by e.created_at&#x29; as rrow &#xd;&#xa;	from marketing_tracking.event e&#xd;&#xa;&#xd;&#xa;  join marketing_tracking.visitor v on &#x28;v.id &#x3d; e.visitor_id&#x29; join &#xd;&#xa;  --&#x28;select min&#x28;e.id&#x29; as first_event_id, v.client_id from marketing_tracking.event e join marketing_tracking.visitor v on &#x28;v.id &#x3d; e.visitor_id&#x29;  group by v.client_id&#x29; fe on &#x28;v.client_id &#x3d; fe.client_id&#x29;&#xd;&#xa;&#xd;&#xa;	lending.loan_application la on &#x28;la.client_id &#x3d; v.client_id and e.created_at&#x3a;&#x3a;Date &#x3d; la.application_date&#x3a;&#x3a;Date&#x29;&#x29;aa&#xd;&#xa;&#xd;&#xa;  where rrow&#x3d;1&#x29;mt on &#x28;mt.id &#x3d; la.id&#x29;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;left outer join &#x28;select distinct application_id, name as traffic_source from marketing_landing.event e join marketing_landing.landing_page lp on &#x28;e.landing_page_id&#x3d;lp.id&#x29;&#x29;lp on la.id &#x3d; lp.application_id&#xd;&#xa;	join app_ranks apr on &#x28;apr.id &#x3d; la.id&#x29;&#xd;&#xa;left outer join lending.loan l on &#x28;l.loan_application_id &#x3d; la.id&#x29;	&#xd;&#xa;&#xd;&#xa;--limit 100</sql>
