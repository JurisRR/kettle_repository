    <sql>select sum&#x28;available_amount&#x29; as total_available_amount &#xd;&#xa;from &#x28;select aa.user_id&#xd;&#xa;,amount_in&#xd;&#xa;&#x23;,amount_out&#xd;&#xa;,payout_req&#xd;&#xa;,active_investments&#xd;&#xa;,profit&#xd;&#xa;,reserved_amount&#xd;&#xa;,coalesce&#x28;amount_in,0&#x29;&#x2b;coalesce&#x28;profit,0&#x29;&#x2b;coalesce&#x28;withdrawal,0&#x29;-coalesce&#x28;payout_req,0&#x29;-&#xd;&#xa;coalesce&#x28;active_investments,0&#x29;-coalesce&#x28;reserved_amount,0&#x29; as available_amount&#xd;&#xa;,profit_acc-profit as accumulated_profit&#xd;&#xa;,case when u.confirmed is null then 0 else 1 end as confirmed&#xd;&#xa;,withdrawal as amount_out&#xd;&#xa;&#x23;,u.confirmed as uuu&#xd;&#xa; from &#x28;select user_id, sum&#x28;amount&#x29; as amount_in&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;from bank_transfers where action &#x3d; &#x27;credit&#x27;&#xd;&#xa;group by 1&#x29;aa &#xd;&#xa;left outer join &#xd;&#xa;&#xd;&#xa;&#x28;select user_id, sum&#x28;amount&#x29; as amount_out&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;from bank_transfers where action &#x3d; &#x27;debit&#x27;&#xd;&#xa;group by 1&#x29;dd on &#x28;aa.user_id &#x3d; dd.user_id&#x29;&#xd;&#xa;&#xd;&#xa;left outer join &#x28;select user_id, sum&#x28;amount&#x29; as payout_req&#xd;&#xa;&#xd;&#xa;from payout_requests where cancelled is null&#xd;&#xa;and paid is null&#xd;&#xa;group by 1&#x29;pp on &#x28;aa.user_id&#x3d;pp.user_id&#x29;&#xd;&#xa;&#xd;&#xa;left outer join &#x28;select user_id, sum&#x28;amount&#x29; as active_investments, sum&#x28;reserved_amount&#x29; as reserved_amount&#xd;&#xa;from investments where status &#x3c;&#x3e; &#x27;Completed&#x27;&#xd;&#xa;and status &#x3c;&#x3e; &#x27;Cancelled&#x27;&#xd;&#xa;group by 1&#x29;ii on &#x28;ii.user_id &#x3d; aa.user_id&#x29;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;left outer join &#x28;select user_id, sum&#x28;amount&#x29; as profit&#xd;&#xa;from transactions where type &#x3d; &#x27;profit&#x27;&#xd;&#xa;group by 1&#x29;pr on &#x28;pr.user_id&#x3d;aa.user_id&#x29;&#xd;&#xa;&#xd;&#xa;left outer join &#x28;select user_id, sum&#x28;amount&#x29; as profit_acc&#xd;&#xa;from profits &#xd;&#xa;group by 1&#x29;prr on &#x28;prr.user_id&#x3d;aa.user_id&#x29;&#xd;&#xa;&#xd;&#xa;left outer join users u on &#x28;u.id &#x3d; aa.user_id&#x29; &#xd;&#xa;&#xd;&#xa;left outer join &#x28;select user_id, sum&#x28;amount&#x29; as withdrawal&#xd;&#xa;from transactions where type &#x3d; &#x27;withdrawal&#x27;&#xd;&#xa;group by 1&#x29;wd on &#x28;wd.user_id&#x3d;aa.user_id&#x29;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;where aa.user_id is not null&#xd;&#xa;&#x23;limit 100&#xd;&#xa;&#x29;bb&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;</sql>
