on.transaction  where transaction_type&#x3d; &#x27;DISBURSEMENT&#x27; group by loan_id&#x29;tr&#xd;&#xa;	on &#x28;l.id&#x3d;tr.loan_id&#x29;&#xd;&#xa;&#xd;&#xa;left outer join &#x28;select min&#x28;l.created_at&#x29; as first_task_agent, min&#x28;t.activity_id&#x29; as activity_id, t.application_id&#xd;&#xa;	from task.log l join task.task t on &#x28;t.id &#x3d; l.task_id&#x29;&#xd;&#xa;		where l.operation &#x3d; &#x27;ASSIGNED&#x27; group by t.application_id&#x29;tt on &#x28;la.id &#x3d; tt.application_id&#x29;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;--where la.created_at&#x3a;&#x3a;date &#x3c;&#x27;2017-01-28&#x27;</sql>
