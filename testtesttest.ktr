    <sql>with app_ranks as &#x28;SELECT a.id, seq_application_total, seq_application_confirm as confirmed_rank, seq_application_reject as rejected_rank&#xd;&#xa;,case when seq_application_reject &#x3e; 0 then seq_application_reject else seq_application_total- seq_application_confirm end as seq_application_reject&#xd;&#xa;,case when seq_application_confirm &#x3e; 0 then seq_application_confirm else seq_application_total- seq_application_reject end as seq_application_confirm&#xd;&#xa;,seq_application_per_loan&#xd;&#xa;  FROM lending.loan_application a join &#x28;select id, rank&#x28;&#x29; over &#x28;partition by client_id &#xd;&#xa;  order by client_id ASC, id ASC &#x29; as seq_application_total  from lending.loan_application &#x29;aa &#xd;&#xa;  on a.id &#x3d; aa.id&#xd;&#xa;&#xd;&#xa;  left outer join &#x28;select id, rank&#x28;&#x29; over &#x28;partition by client_id &#xd;&#xa;  order by client_id ASC, id ASC &#x29; as seq_application_confirm from lending.loan_application&#xd;&#xa;  where status &#x3d; &#x27;APPROVED&#x27;&#x29;ac &#xd;&#xa;  on a.id &#x3d; ac.id&#xd;&#xa;&#xd;&#xa;  left outer join &#x28;select id, rank&#x28;&#x29; over &#x28;partition by client_id &#xd;&#xa;  order by client_id ASC, id ASC &#x29; as seq_application_reject from lending.loan_application &#xd;&#xa;  where status &#x3d; &#x27;REJECTED&#x27;&#x29;ar &#xd;&#xa;  on a.id &#x3d; ar.id&#xd;&#xa;&#xd;&#xa;  left outer join &#x28;select id, rank&#x28;&#x29; over &#x28;partition by client_id, loans_paid&#xd;&#xa;  order by client_id ASC, loans_paid ASC, id ASC &#x29; as seq_application_per_loan from lending.loan_application &#xd;&#xa;  &#x29;aspl &#xd;&#xa;  on &#x28;a.id &#x3d; aspl.id&#x29;&#xd;&#xa;  &#xd;&#xa;  order by id, a.id&#x29; &#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;SELECT &#xd;&#xa;distinct&#xd;&#xa;la.id, la.application_date&#x3a;&#x3a;date, la.close_date&#x3a;&#x3a;date, la.close_reason, la.created_at&#x3a;&#x3a;date, la.created_by, &#xd;&#xa;       la.loans_paid, la.application_number, la.offer_date&#x3a;&#x3a;date, la.offered_installments, &#xd;&#xa;       la.offered_interest, la.offered_period_count, la.offered_period_unit, &#xd;&#xa;       la.offered_principal, la.requested_installments, la.requested_period_count, &#xd;&#xa;       la.requested_period_unit, la.requested_principal, la.status, la.client_id, &#xd;&#xa;      &#xd;&#xa;	 lla.loan_id  -- some kind of error&#x3f; in loan_application_view&#x3f; changed to lending_loan_application&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;--,999999 as client_id&#xd;&#xa;,ca.province&#xd;&#xa;,ca.city&#xd;&#xa;,ca.subdistrict&#xd;&#xa;,ca.subsubdistrict&#xd;&#xa;,ca.rtrw&#xd;&#xa;,ca.streethouseapp&#xd;&#xa;,ca.postcode&#xd;&#xa;&#xd;&#xa;,app.workflow_id&#xd;&#xa;,app2.resolution&#xd;&#xa;,case when lla.created_by like &#x27;web&#x3a;&#x25;&#x27; then &#x27;web&#x27;&#xd;&#xa;	else lla.created_by end as created_by&#xd;&#xa;&#xd;&#xa;,ce.&#x2a;&#xd;&#xa;,cc.ektp&#xd;&#xa;--split the ektp&#xd;&#xa;,case when substring&#x28;cc.ektp, 7, 2&#x29;&#x3a;&#x3a;smallint &#x3e; 40 then &#x27;female&#x27; else &#x27;male&#x27; end as gender_ektp&#xd;&#xa;,pi.date_of_birth&#x3a;&#x3a;date&#xd;&#xa;,pi.gender&#xd;&#xa;,extract&#x28;years from age&#x28;pi.date_of_birth&#x3a;&#x3a;date&#x29;&#x29; as age&#xd;&#xa;,substring&#x28;el.email from position&#x28;&#x27;&#x40;&#x27; in el.email&#x29;&#x2b;1&#x29; as email&#xd;&#xa;,el.email as full_email&#xd;&#xa;,current_date&#x3a;&#x3a;date as extract_date&#xd;&#xa;,case when lla.requested_period_count &#x3d; 62 then &#x27;google&#x27; else &#x27;facebook&#x27; end as traffic_source&#xd;&#xa;&#xd;&#xa;,case when now&#x28;&#x29; - la.created_at &#x3e; interval &#x27;24 hours&#x27; then &#x27;before&#x27; else &#x27;last 24 hours&#x27; end as last_24hours&#xd;&#xa;,case when now&#x28;&#x29; - la.created_at &#x3e; interval &#x27;7 days&#x27; then &#x27;before&#x27; else &#x27;last 7 days&#x27; end as last_7days&#xd;&#xa;,case when now&#x28;&#x29; - la.created_at &#x3e; interval &#x27;30 days&#x27; then &#x27;before&#x27; else &#x27;last 30 days&#x27; end as last_30days&#xd;&#xa;,extract&#x28;week from la.application_date&#x29; as app_isoweek&#xd;&#xa;,extract&#x28;month from la.application_date&#x29; as app_month&#xd;&#xa;,case when la.application_date&#x3a;&#x3a;date between current_date-7 and current_date-1 then &#x27;this week&#x27; &#xd;&#xa;	 when la.application_date&#x3a;&#x3a;date between current_date-14 and current_date-8 then &#x27;previous week&#x27; &#xd;&#xa;	else &#x27;history&#x27; end as week&#xd;&#xa;,extract&#x28;isodow from la.application_date&#x29; as day_nr&#xd;&#xa;&#xd;&#xa;--,l.loans_paid&#x2b;1 as loan_seq&#xd;&#xa;,case when la.loans_paid &#x3d; 0 then &#x27;first&#x27; &#xd;&#xa;	when la.loans_paid &#x3e;0 then &#x27;repeated&#x27; &#xd;&#xa;	else null end as loan_type&#xd;&#xa;&#xd;&#xa;,extract&#x28;hour from la.created_at&#x29;&#x7c;&#x7c;&#x27;&#x3a;&#x27;&#x7c;&#x7c;extract&#x28;minutes from la.created_at&#x29; as application_time&#xd;&#xa;,tc.task_type as lc_task_type&#xd;&#xa;,tc.resolution as lc_resolution&#xd;&#xa;,tc.agent as lc_agent&#xd;&#xa;,tc.status as lc_status&#xd;&#xa;,t.task_type as t_task_type&#xd;&#xa;,t.resolution as t_resolution&#xd;&#xa;,t.agent as t_agent&#xd;&#xa;,t.status as t_status&#xd;&#xa;,t.times_postponed&#xd;&#xa;,t.times_reopened&#xd;&#xa;&#xd;&#xa;--,la.workflow_name&#xd;&#xa;--,la.next_activity&#xd;&#xa;&#xd;&#xa;,&#x28;lla.offered_principal &#x2f; lla.requested_principal&#x29; as offered_proportion&#xd;&#xa;&#xd;&#xa;,ec.resolution as emp_call_resolution&#xd;&#xa;,ec.status as emp_call_status&#xd;&#xa;,ec.times_postponed as emp_times_postponed&#xd;&#xa;,ec.times_reopened as emp_times_reopened&#xd;&#xa;&#xd;&#xa;,case when aok.auto_ok is null then &#x27;no auto&#x27; else &#x27;autocomplete triggered&#x27; end as auto&#xd;&#xa;&#xd;&#xa;,att.selfie_status&#xd;&#xa;&#xd;&#xa;--&#x21;&#x21; need to find difference in time between first submission of the applica