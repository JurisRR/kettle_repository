salary, salary_c&#x29; as salary&#xd;&#xa;,COALESCE&#x28;apt.postponed_count,0&#x29; as postponed_sales_task_count&#xd;&#xa;&#xd;&#xa;,current_timestamp as extraction_datetime&#xd;&#xa;&#xd;&#xa;,extract&#x28;epoch from &#x28;tr.created_at - la.created_at&#x29;&#x29;&#x2f;3600 as h_to_loan_disbursement&#xd;&#xa;,extract&#x28;epoch from &#x28;tt.first_task_agent - la.created_at&#x29;&#x29;&#x2f;3600 as h_to_agent&#xd;&#xa;,extract&#x28;epoch from &#x28;tr.created_at - tt.first_task_agent &#x29;&#x29;&#x2f;3600 as h_to_loan_since_agent&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;--&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23; end of selected data &#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#xd;&#xa; from lending.loan_application la &#xd;&#xa;join lending.loan_application lla on &#x28;la.id &#x3d; lla.id&#x29;&#xd;&#xa;left outer join &#x28;&#xd;&#xa;&#xd;&#xa;SELECT client_id&#xd;&#xa;       , json_value&#x3a;&#x3a;json-&#x3e;&#x3e;&#x27;province&#x27; as province&#xd;&#xa;       , json_value&#x3a;&#x3a;json-&#x3e;&#x3e;&#x27;city&#x27; as city&#xd;&#xa;       , json_value&#x3a;&#x3a;json-&#x3e;&#x3e;&#x27;subdistrict&#x27; as subdistrict&#xd;&#xa;       , json_value&#x3a;&#x3a;json-&#x3e;&#x3e;&#x27;subsubdistrict&#x27; as subsubdistrict&#xd;&#xa;       , json_value&#x3a;&#x3a;json-&#x3e;&#x3e;&#x27;rtrw&#x27; as rtrw&#xd;&#xa;       , json_value&#x3a;&#x3a;json-&#x3e;&#x3e;&#x27;streetHouseApp&#x27; as streethouseapp&#xd;&#xa;       , json_value&#x3a;&#x3a;json-&#x3e;&#x3e;&#x27;postCode&#x27; as postcode&#xd;&#xa;&#xd;&#xa;      FROM crm.client_attribute &#xd;&#xa;    where &#xd;&#xa;	key &#x3d; &#x27;addressData&#x27; &#xd;&#xa;&#x29;ca  on &#x28;ca.client_id &#x3d; la.client_id&#x29;&#xd;&#xa;&#xd;&#xa;left outer join &#x28;&#xd;&#xa;&#xd;&#xa;SELECT client_id&#xd;&#xa;       , json_value&#x3a;&#x3a;json-&#x3e;&#x3e;&#x27;ektp&#x27; as ektp&#xd;&#xa;       &#xd;&#xa;      FROM crm.client_attribute &#xd;&#xa;    where &#xd;&#xa;	key &#x3d; &#x27;personalData&#x27; &#xd;&#xa;&#x29;cc  on &#x28;cc.client_id &#x3d; la.client_id&#x29;&#xd;&#xa;&#xd;&#xa;left outer join &#x28;&#xd;&#xa;&#xd;&#xa;SELECT client_id&#xd;&#xa; , json_value&#x3a;&#x3a;json-&#x3e;&#x3e;&#x27;employmentStatus&#x27; as employmentStatus&#xd;&#xa; , &#x28;json_value&#x3a;&#x3a;json-&#x3e;&#x3e;&#x27;contractExpiryDate&#x27;&#x29;&#x3a;&#x3a;date as contractExpiryDate&#xd;&#xa; , &#x28;json_value&#x3a;&#x3a;json-&#x3e;&#x3e;&#x27;employmentDurationInMonths&#x27;&#x29;&#x3a;&#x3a;smallint as employmentDurationInMonths&#xd;&#xa; , json_value&#x3a;&#x3a;json-&#x3e;&#x3e;&#x27;jobPosition&#x27; as jobPosition&#xd;&#xa; , &#x28;json_value&#x3a;&#x3a;json-&#x3e;&#x3e;&#x27;salary&#x27;&#x29;&#x3a;&#x3a;bigint as salary_c    -- switched to application attribute salary&#xd;&#xa; , &#x28;json_value&#x3a;&#x3a;json-&#x3e;&#x3e;&#x27;otherLiabilities&#x27;&#x29;&#x3a;&#x3a;bigint as otherLiabilities&#xd;&#xa;    &#xd;&#xa;      FROM crm.client_attribute &#xd;&#xa;    where &#xd;&#xa;	key &#x3d; &#x27;employmentData&#x27; &#xd;&#xa;&#x29;ce  on &#x28;ce.client_id &#x3d; la.client_id&#x29;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;left outer join &#xd;&#xa;&#xd;&#xa;&#x28;select &#xd;&#xa;&#xd;&#xa;	workflow_id, number_value as app_id from workflow.workflow_param &#xd;&#xa;	where name &#x3d; &#x27;loanApplicationId&#x27;&#x29;app on &#x28;app.app_id &#x3d; la.id&#x29;&#xd;&#xa;	&#xd;&#xa;left outer join &#xd;&#xa;&#xd;&#xa;&#x28;select &#xd;&#xa;&#xd;&#xa;	workflow_id, text_value as resolution from workflow.workflow_param &#xd;&#xa;	where name &#x3d; &#x27;Resolution&#x27;&#x29;app2 on &#x28;app.workflow_id &#x3d; app2.workflow_id&#x29;&#xd;&#xa;&#xd;&#xa;left outer join crm.personal_info pi on &#x28;la.client_id &#x3d; pi.client_id&#x29;&#xd;&#xa;left outer join crm.email_login el on &#x28;la.client_id &#x3d; el.client_id&#x29;&#xd;&#xa;left outer join lending.loan l on &#x28;lla.id &#x3d; l.loan_application_id&#x29;&#xd;&#xa;left outer join &#x28;select application_id, max&#x28;id&#x29; as max_task_id from task.task where task_group &#x3c;&#x3e; &#x27;DebtCollection&#x27; group by application_id&#x29;mi on &#x28;la.id &#x3d; mi.application_id&#x29; --&#x23;&#x23; get max task id which to be joined&#xd;&#xa;left outer join task.task t on &#x28;t.id &#x3d; mi.max_task_id&#x29; --&#x23;&#x23; add info on last task&#xd;&#xa;left outer join &#x28;select application_id, max&#x28;id&#x29; as max_comp_task_id from task.task where status &#x3d; &#x27;COMPLETED&#x27; and task_group &#x3c;&#x3e; &#x27;DebtCollection&#x27; group by application_id&#x29;mci on &#x28;la.id &#x3d; mci.application_id&#x29; --&#x23;&#x23; get last completed task id which to be joined&#xd;&#xa;left outer join task.task tc on &#x28;tc.id &#x3d; mci.max_comp_task_id&#x29; --&#x23;&#x23; add info on last task&#xd;&#xa;&#xd;&#xa;left outer join &#x28;select a.attempts, a.resolution, a.status, w.application_id, t.times_postponed, t.times_reopened &#xd;&#xa;		from workflow.activity a join workflow.workflow w on &#x28;a.workflow_id &#x3d; w.id&#x29; left outer join task.task t on &#x28;a.id &#x3d; t.activity_id&#x29;&#xd;&#xa;		where a.name &#x3d; &#x27;EmployerCall&#x27;&#x29;ec on &#x28;lla.id &#x3d; ec.application_id&#x29; &#xd;&#xa;&#xd;&#xa;left outer join &#x28;select a.id as auto_ok, w.application_id from workflow.activity a join workflow.workflow w on &#x28;a.workflow_id&#x3d;w.id&#x29; &#xd;&#xa;	where 1&#x3d;1&#xd;&#xa;	and a.name &#x3d; &#x28; &#x27;AutoCompleteRegistration&#x27;&#x29;&#xd;&#xa;	and a.status &#x3d; &#x27;COMPLETED&#x27;&#xd;&#xa;&#xd;&#xa;	&#x29;aok on &#x28;aok.application_id &#x3d; lla.id&#x29;&#xd;&#xa;&#xd;&#xa;left outer join &#x28;select client_id,