    <sql>select&#xd;&#xa;&#xd;&#xa;crm_task_id, closed_date&#x3a;&#x3a;date, s.name as task_status, t.task_date&#x3a;&#x3a;date&#xd;&#xa;,t.r_loan_id&#xd;&#xa;,t.created&#x3a;&#x3a;date as task_create_date&#xd;&#xa;, &#x28;case --when s.name &#x3d; &#x27;Paid&#x27; then 0 &#xd;&#xa;	when t.closed_date is null then current_date - t.created&#x3a;&#x3a;date &#x2b;1 &#xd;&#xa;	else t.closed_date&#x3a;&#x3a;date-t.created&#x3a;&#x3a;date&#x2b;1 end&#x29;&#x3a;&#x3a;int as current_delay&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;, case when t.closed_date is null then current_date&#x2b;1 else t.closed_date&#x3a;&#x3a;date end as task_end_date&#xd;&#xa;,u.first_name &#x7c;&#x7c; &#x27; &#x27; &#x7c;&#x7c; u.last_name as responsible_person&#xd;&#xa; &#xd;&#xa;&#xd;&#xa;from crm_task t join crm_task_status s using &#x28;crm_task_status_id&#x29; join um_user u on &#x28;t.um_user_id &#x3d; u.um_user_id&#x29;&#xd;&#xa;&#xd;&#xa;where crm_task_id &#x3d;&#x3f; </sql>
