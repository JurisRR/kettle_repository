    <sql>select d.r_loan_id&#xd;&#xa;	, d.amount as amount_repaid &#xd;&#xa;	, p.payment_date&#x3a;&#x3a;date &#xd;&#xa;	, p.date_processed&#x3a;&#x3a;date&#xd;&#xa;	, &#x28;p.date_processed&#x3a;&#x3a;date - &#x3f; &#x2b;1&#x29;&#x3a;&#x3a;int as loan_delayed_day&#xd;&#xa;	--, p.date_processed&#x3a;&#x3a;date - &#x3f;&#x3a;&#x3a;date as delayed_day&#xd;&#xa;	&#xd;&#xa;	, case when &#x28;p.date_processed&#x3a;&#x3a;date - &#x3f; &#x2b;1&#x29;&#x3a;&#x3a;int &#x3c; 5 then &#x27;2-4&#x27; &#xd;&#xa;	when &#x28;p.date_processed&#x3a;&#x3a;date - &#x3f; &#x2b;1&#x29;&#x3a;&#x3a;int &#x3c; 24 then &#x27;5-24&#x27;&#xd;&#xa;	when &#x28;p.date_processed&#x3a;&#x3a;date - &#x3f; &#x2b;1&#x29;&#x3a;&#x3a;int &#x3c;41 then &#x27;25-40&#x27;&#xd;&#xa;	else &#x27;41&#x2b;&#x27; end as delayed_bucket&#xd;&#xa;	, extract&#x28;week from p.date_processed&#x29; as week_nr&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;	from r_payment_distr d join r_payment p on  &#x28;d.r_payment_id &#x3d; p.r_payment_id&#x29;-- join r_loan l on &#x28;d.r_loan_id &#x3d; l.r_loan_id&#x29;&#xd;&#xa;&#xd;&#xa;where 1&#x3d;1&#xd;&#xa;and p.type &#x3d; &#x27;IN_Loan&#x27;&#xd;&#xa;and d.r_loan_id &#x3d; &#x3f;&#xd;&#xa;and p.date_processed&#x3a;&#x3a;date between  &#x3f; and &#x3f;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;</sql>
