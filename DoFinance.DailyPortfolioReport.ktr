    <connection>DoFinance</connection>
    <sql>Select sum&#x28;c.amount&#x29; as portfolio_size_eur, current_timestamp as status_datetime, current_date as portfolio_date, pr.title as owner&#xd;&#xa;,coalesce&#x28;prr.profit,0&#x29; as finished_profit_yesterday&#xd;&#xa;&#xd;&#xa;from cessions c join loans l on &#x28;c.loan_id &#x3d; l.id&#x29; join providers pr on &#x28;pr.id &#x3d; l.provider_id&#x29; &#xd;&#xa;&#xd;&#xa;left outer join &#x28;select l.provider_id, round&#x28;sum&#x28;p.amount&#x29;,2&#x29; as profit from profits p join cessions c on &#x28;p.cession_id &#x3d; c.id&#x29; join loans l on &#x28;c.loan_id &#x3d; l.id&#x29;&#xd;&#xa;&#xd;&#xa;join investments i on &#x28;p.investment_id &#x3d; i.id&#x29; where cast&#x28;i.finished as date&#x29; &#x3d; current_date-1&#x29;prr on &#x28;prr.provider_id&#x3d;l.provider_id&#x29;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;where c.status in &#x28;&#x27;Active&#x27;, &#x27;Frozen&#x27;&#x29;&#xd;&#xa;&#xd;&#xa;group by 2,3,4</sql>
