    <sql>select&#xd;&#xa;&#xd;&#xa;a.id&#xd;&#xa;,a.created_at&#x3a;&#x3a;date as app_create_date&#xd;&#xa;,l.created_at&#x3a;&#x3a;Date as loan_create_date&#xd;&#xa;,extract&#x28;epoch from &#x28;l.created_at - a.created_at&#x29;&#x29;&#x2f;3600 as h_to_loan&#xd;&#xa;,a.status&#xd;&#xa;, case when a.created_at &#x3e; &#x27;2017-05-04&#x27; then 1 else 0 end as after_4th_may&#xd;&#xa;,a.loans_paid&#xd;&#xa;,case when a.loans_paid &#x3d; 0 then &#x27;first&#x27; else &#x27;repeated&#x27; end as loan_type&#xd;&#xa;,t.created_at&#x3a;&#x3a;date as loan_out_date&#xd;&#xa;,extract&#x28;epoch from &#x28;t.created_at - a.created_at&#x29;&#x29;&#x2f;3600 as h_to_loan_disbursement&#xd;&#xa;,extract&#x28;week from a.created_at&#x29; as app_isoweek&#xd;&#xa;,extract&#x28;epoch from &#x28;a.updated_at - a.created_at&#x29;&#x29;&#x2f;3600 as h_to_last_app_update&#xd;&#xa;,extract&#x28;epoch from &#x28;tt.first_task_agent - a.created_at&#x29;&#x29;&#x2f;3600 as h_to_agent&#xd;&#xa;,extract&#x28;epoch from &#x28;t.created_at - tt.first_task_agent &#x29;&#x29;&#x2f;3600 as h_to_loan_since_agent --&#x23;&#x23; loan time time changed to transaction&#xd;&#xa;,act.name as activity_name&#xd;&#xa;,w.name as workflow_name&#xd;&#xa;,act2.name as activity_name2&#xd;&#xa;,w2.name as workflow_name2&#xd;&#xa;&#xd;&#xa;--&#x23;&#x23;&#x23;&#x23; for repeated loans task times are not correct. &#xd;&#xa;&#xd;&#xa;&#xd;&#xa;from lending.loan_application a left outer join lending.loan l on &#x28;a.id &#x3d; l.loan_application_id&#x29;&#xd;&#xa;	left outer join transaction.transaction t on &#x28;l.id&#x3d;t.loan_id and t.transaction_type&#x3d; &#x27;DISBURSEMENT&#x27;&#x29;&#xd;&#xa;&#xd;&#xa;left outer join &#x28;select min&#x28;l.created_at&#x29; as first_task_agent, min&#x28;t.activity_id&#x29; as activity_id, t.application_id&#xd;&#xa;	from task.log l join task.task t on &#x28;t.id &#x3d; l.task_id&#x29;&#xd;&#xa;		where l.operation &#x3d; &#x27;ASSIGNED&#x27; group by t.application_id&#x29;tt on &#x28;a.id &#x3d; tt.application_id&#x29;&#xd;&#xa;&#xd;&#xa;left outer join &#x28;select  max&#x28;t.activity_id&#x29; as activity_id, t.application_id&#xd;&#xa;	from task.log l join task.task t on &#x28;t.id &#x3d; l.task_id&#x29;&#xd;&#xa;		 group by t.application_id&#x29;ttt on &#x28;a.id &#x3d; ttt.application_id&#x29;&#xd;&#xa;&#xd;&#xa;left outer join workflow.activity act on &#x28;act.id &#x3d; tt.activity_id&#x29; left outer join workflow.workflow w on &#x28;act.workflow_id &#x3d; w.id&#x29;	&#xd;&#xa;	&#xd;&#xa;left outer join workflow.activity act2 on &#x28;act2.id &#x3d; ttt.activity_id&#x29; left outer join workflow.workflow w2 on &#x28;act2.workflow_id &#x3d; w2.id&#x29;	&#xd;&#xa;--where  &#xd;&#xa;--limit 1000</sql>
