    <sql>SELECT c.change_date&#x3a;&#x3a;date&#xd;&#xa;,c.created&#x3a;&#x3a;date&#xd;&#xa;,c.recordid&#xd;&#xa;,l.loan_no&#xd;&#xa;,l.r_loan_id&#xd;&#xa;&#xd;&#xa;,l.status&#xd;&#xa;,c.createdby&#xd;&#xa;,c.updatedby&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;,first_name&#xd;&#xa;,last_name&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;,substring&#x28;c.updatestr from position&#x28;&#x27;AMOUNT_OPEN_PENALTY.NEW&#x3d;&#x27; in c.updatestr&#x29;&#x2b;24 &#xd;&#xa;	for position&#x28;&#x27;&#x3c;&#x3b;&#x3e;AMOUNT_OPEN_PENALTY.OLD&#x3d;&#x27; in c.updatestr&#x29;-&#x28;position&#x28;&#x27;AMOUNT_OPEN_PENALTY.NEW&#x3d;&#x27; in c.updatestr&#x29;&#x2b;24&#x29;&#x29;&#x3a;&#x3a;numeric as new_open_penalty&#xd;&#xa;&#xd;&#xa;,substring&#x28;substring&#x28;c.updatestr from position&#x28;&#x27;AMOUNT_OPEN_PENALTY.OLD&#x3d;&#x27; in c.updatestr&#x29;&#x2b;24&#x29; from 1 &#xd;&#xa;	for position&#x28;&#x27;&#x3c;&#x27; in substring&#x28;c.updatestr from position&#x28;&#x27;AMOUNT_OPEN_PENALTY.OLD&#x3d;&#x27; in c.updatestr&#x29;&#x2b;24&#x29;&#x29;-1&#x29;&#x3a;&#x3a;numeric as old_open_penalty&#xd;&#xa;&#xd;&#xa;,substring&#x28;substring&#x28;c.updatestr from position&#x28;&#x27;AMOUNT_OPEN_PENALTY.OLD&#x3d;&#x27; in c.updatestr&#x29;&#x2b;24&#x29; from 1 &#xd;&#xa;	for position&#x28;&#x27;&#x3c;&#x27; in substring&#x28;c.updatestr from position&#x28;&#x27;AMOUNT_OPEN_PENALTY.OLD&#x3d;&#x27; in c.updatestr&#x29;&#x2b;24&#x29;&#x29;-1&#x29;&#x3a;&#x3a;numeric -&#xd;&#xa;substring&#x28;c.updatestr from position&#x28;&#x27;AMOUNT_OPEN_PENALTY.NEW&#x3d;&#x27; in c.updatestr&#x29;&#x2b;24 &#xd;&#xa;	for position&#x28;&#x27;&#x3c;&#x3b;&#x3e;AMOUNT_OPEN_PENALTY.OLD&#x3d;&#x27; in c.updatestr&#x29;-&#x28;position&#x28;&#x27;AMOUNT_OPEN_PENALTY.NEW&#x3d;&#x27; in c.updatestr&#x29;&#x2b;24&#x29;&#x29;&#x3a;&#x3a;numeric as suspended_amount&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;--,c.updatestr&#xd;&#xa;,rank&#x28;&#x29; over &#x28;partition by c.recordid order by c.recordid asc, c.ad_changelog_id desc&#x29;as rank_chnglog&#xd;&#xa;  &#xd;&#xa;FROM ad_changelog c join r_loan l on &#x28;c.recordid &#x3d; l.r_loan_id&#x29; join um_user u on &#x28;c.updatedby&#x3d;u.um_user_id&#x29;&#xd;&#xa;&#xd;&#xa;  where tablename &#x3d; &#x27;R_LOAN&#x27;&#xd;&#xa;  &#xd;&#xa;  and updatestr like &#x27;&#x25;AMOUNT_OPEN_PENALTY.NEW&#x3d;&#x25;&#x27;&#xd;&#xa;  and updatestr like &#x27;&#x25;CANCELATION_DATE.NEW&#x3d;&#x25;&#x27;&#xd;&#xa;&#xd;&#xa;  and recordid &#x3d; &#x3f;&#xd;&#xa;&#xd;&#xa;	and u.um_user_id &#x3e; 17&#xd;&#xa;&#xd;&#xa;and  substring&#x28;c.updatestr from position&#x28;&#x27;AMOUNT_OPEN_PENALTY.NEW&#x3d;&#x27; in c.updatestr&#x29;&#x2b;24 &#xd;&#xa;	for position&#x28;&#x27;&#x3c;&#x3b;&#x3e;AMOUNT_OPEN_PENALTY.OLD&#x3d;&#x27; in c.updatestr&#x29;-&#x28;position&#x28;&#x27;AMOUNT_OPEN_PENALTY.NEW&#x3d;&#x27; in c.updatestr&#x29;&#x2b;24&#x29;&#x29;&#x3a;&#x3a;numeric &#xd;&#xa;&#x3c; substring&#x28;substring&#x28;c.updatestr from position&#x28;&#x27;AMOUNT_OPEN_PENALTY.OLD&#x3d;&#x27; in c.updatestr&#x29;&#x2b;24&#x29; from 1 &#xd;&#xa;	for position&#x28;&#x27;&#x3c;&#x27; in substring&#x28;c.updatestr from position&#x28;&#x27;AMOUNT_OPEN_PENALTY.OLD&#x3d;&#x27; in c.updatestr&#x29;&#x2b;24&#x29;&#x29;-1&#x29;&#x3a;&#x3a;numeric&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;  order by ad_changelog_id</sql>
