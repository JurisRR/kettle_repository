  <step>
    <name>add previous loan</name>
    <type>DBJoin</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>Georgia</connection>
    <rowlimit>0</rowlimit>
    <sql>-- &#x23;&#x23; Extract data on loan level including credit info, client, application data&#xd;&#xa;&#xd;&#xa;--&#x23;&#x23; aggregation of payments_distr table on loan level&#xd;&#xa;with dd as &#x28;select d.r_loan_id,&#xd;&#xa;sum&#x28;d.amount&#x29; as amount, sum&#x28;d.amount_principal&#x29; as amount_principal, &#xd;&#xa;       sum&#x28;d.amount_commission&#x29; as amount_commission, sum&#x28;d.amount_penalty&#x29; as amount_penalty, sum&#x28;d.amount_surplus&#x29; as amount_surplus,&#xd;&#xa;        sum&#x28;d.amount_prolongation&#x29; as amount_prolongation, sum&#x28;d.amount_fees&#x29; as amount_fees&#xd;&#xa;	&#xd;&#xa;from &#xd;&#xa;r_payment_distr d join r_payment p on  &#x28;d.r_payment_id &#x3d; p.r_payment_id&#x29; &#xd;&#xa;where 1&#x3d;1 &#xd;&#xa;	and p.cl_bank_acct_id &#x3c;&#x3e; 18&#xd;&#xa;	--and p.cl_bank_acct_id &#x3c;&#x3e; 12&#xd;&#xa;	group by d.r_loan_id&#x29;&#xd;&#xa;&#xd;&#xa;,pay as &#x28;select d.r_loan_id, min&#x28;payment_date&#x29;&#x3a;&#x3a;date as first_payment_date , max&#x28;payment_date&#x29;&#x3a;&#x3a;date as last_payment_date&#xd;&#xa;	from r_payment_distr d join r_payment p on  &#x28;d.r_payment_id &#x3d; p.r_payment_id&#x29; &#xd;&#xa;		where p.type &#x3d; &#x27;IN_Loan&#x27; and p.cl_bank_acct_id &#x3c;&#x3e; 12 and cl_bank_acct_id &#x3c;&#x3e;  18&#xd;&#xa;		group by d.r_loan_id&#x29;&#xd;&#xa;&#xd;&#xa;,vpay as &#x28;select d.r_loan_id, min&#x28;payment_date&#x29;&#x3a;&#x3a;date as first_virtual_payment_date , max&#x28;payment_date&#x29;&#x3a;&#x3a;date as last_payment_date&#xd;&#xa;	from r_payment_distr d join r_payment p on  &#x28;d.r_payment_id &#x3d; p.r_payment_id&#x29; &#xd;&#xa;		where p.type &#x3d; &#x27;IN_Loan&#x27; and &#x28;p.cl_bank_acct_id &#x3d; 12 or cl_bank_acct_id &#x3d;  18&#x29;&#xd;&#xa;		group by d.r_loan_id&#x29;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;,out as &#x28;select d.r_loan_id, min&#x28;payment_date&#x29;&#x3a;&#x3a;date as out_loan_date &#xd;&#xa;	from r_payment_distr d join r_payment p on  &#x28;d.r_payment_id &#x3d; p.r_payment_id&#x29; &#xd;&#xa;		where p.type &#x3d; &#x27;OUT_Loan&#x27; and p.cl_bank_acct_id &#x3c;&#x3e; 12 and cl_bank_acct_id &#x3c;&#x3e;  18&#xd;&#xa;		group by d.r_loan_id&#x29;&#xd;&#xa;&#xd;&#xa;,freex as &#x28;select d.r_loan_id, wp.amount_prolongation , wp.days_prolongation, min&#x28;p.payment_date&#x29;&#x3a;&#x3a;date as free_extension_date&#xd;&#xa;	from r_payment_distr d join r_payment p on  &#x28;d.r_payment_id &#x3d; p.r_payment_id&#x29; &#xd;&#xa;		join r_withdrawal_prolongation_log wp on &#x28;d.r_payment_distr_id &#x3d; wp.r_payment_distr_id&#x29;&#xd;&#xa;		where p.type &#x3d; &#x27;IN_Loan&#x27; and p.cl_bank_acct_id &#x3d; 18&#xd;&#xa;		group by d.r_loan_id, wp.amount_prolongation , wp.days_prolongation&#x29;&#xd;&#xa;&#xd;&#xa;--&#x23;&#x23;&#x23;&#x23;&#x23; add payments by 30 day intervals&#xd;&#xa;,pint as &#x28;select r_loan_id,&#xd;&#xa;max&#x28;case when interval30 &#x3d; 30 then sum else 0 end&#x29; as amount30,&#xd;&#xa;max&#x28;case when interval30 &#x3d; 60 then sum else 0 end&#x29; as amount60,&#xd;&#xa;max&#x28;case when interval30 &#x3d; 90 then sum else 0 end&#x29; as amount90,&#xd;&#xa;max&#x28;case when interval30 &#x3d; 120 then sum else 0 end&#x29; as amount120,&#xd;&#xa;max&#x28;case when interval30 &#x3d; 150 then sum else 0 end&#x29; as amount150,&#xd;&#xa;max&#x28;case when interval30 &#x3d; 180 then sum else 0 end&#x29; as amount180,&#xd;&#xa;max&#x28;case when interval30 &#x3d; 210 then sum else 0 end&#x29; as amount210,&#xd;&#xa;max&#x28;case when interval30 &#x3d; 240 then sum else 0 end&#x29; as amount240,&#xd;&#xa;max&#x28;case when interval30 &#x3d; 270 then sum else 0 end&#x29; as amount270,&#xd;&#xa;max&#x28;case when interval30 &#x3d; 300 then sum else 0 end&#x29; as amount300,&#xd;&#xa;max&#x28;case when interval30 &#x3d; 330 then sum else 0 end&#x29; as amount330,&#xd;&#xa;max&#x28;case when interval30 &#x3d; 360 then sum else 0 end&#x29; as amount360&#xd;&#xa;&#xd;&#xa;from &#x28;select r_loan_id, interval30, sum&#x28;amount&#x29; from &#x28;select d.r_loan_id, &#x28;d.amount-d.amount_surplus&#x29; as amount,&#xd;&#xa;case when &#x28;payment_date&#x3a;&#x3a;date -l.loan_date&#x3a;&#x3a;date&#x29; &#x3c;&#x3d; 30 then 30&#xd;&#xa;	when &#x28;payment_date&#x3a;&#x3a;date -l.loan_date&#x3a;&#x3a;date&#x29; &#x3c;&#x3d; 60 then 60&#xd;&#xa;	when &#x28;payment_date&#x3a;&#x3a;date -l.loan_date&#x3a;&#x3a;date&#x29; &#x3c;&#x3d; 90 then 90&#xd;&#xa;	when &#x28;payment_date&#x3a;&#x3a;date -l.loan_date&#x3a;&#x3a;date&#x29; &#x3c;&#x3d; 120 then 120&#xd;&#xa;	when &#x28;payment_date&#x3a;&#x3a;date -l.loan_date&#x3a;&#x3a;date&#x29; &#x3c;&#x3d; 150 then 150&#xd;&#xa;	when &#x28;payment_date&#x3a;&#x3a;date -l.loan_date&#x3a;&#x3a;date&#x29; &#x3c;&#x3d; 180 then 180&#xd;&#xa;	when &#x28;payment_date&#x3a;&#x3a;date -l.loan_date&#x3a;&#x3a;date&#x29; &#x3c;&#x3d; 210 then 210&#xd;&#xa;	when &#x28;payment_date&#x3a;&#x3a;date -l.loan_date&#x3a;&#x3a;date&#x29; &#x3c;&#x3d; 240 then 240&#xd;&#xa;	when &#x28;payment_date&#x3a;&#x3a;date -l.loan_date&#x3a;&#x3a;date&#x29; &#x3c;&#x3d; 270 then 270&#xd;&#xa;	when &#x28;payment_date&#x3a;&#x3a;date -l.loan_date&#x3a;&#x3a;date&#x29; &#x3c;&#x3d; 300 then 300&#xd;&#xa;	when &#x28;payment_date&#x3a;&#x3a;date -l.loan_date&#x3a;&#x3a;date&#x29; &#x3c;&#x3d; 330 then 330&#xd;&#xa;	when &#x28;payment_date&#x3a;&#x3a;date -l.loan_date&#x3a;&#x3a;date&#x29; &#x3c;&#x3d; 360 then 360 end as interval30&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;from r_payment_distr d join r_payment p on  &#x28;d.r_payment_id &#x3d; p.r_payment_id&#x29; join r_loan l on &#x28;l.r_loan_id &#x3d; d.r_loan_id&#x29;&#xd;&#xa;	where p.type &#x3d; &#x27;IN_Loan&#x27; and p.cl_bank_acct_id &#x3c;&#x3e; 18&#xd;&#xa;&#x29;dd		&#xd;&#xa;group by r_loan_id, interval30&#xd;&#xa;order by r_loan_id, interval30&#x29;int30&#xd;&#xa;&#xd;&#xa;group by r_loan_id&#x29;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;-- &#x23;&#x23;&#x23;&#x23;&#x23; add also subquery to attach info on virtual transactions&#xd;&#xa;-- &#x23;&#x23; in case when surplus &#x3c; loan_amount, seems like loan defaulted, otherwise seems like a good loan&#xd;&#xa;&#xd;&#xa;---&#x23;&#x23;&#x23;&#x23;&#x23;&#xd;&#xa;	 &#xd;&#xa;&#xd;&#xa;,clci as &#x28;&#xd;&#xa;	 select &#x2a; from &#x28;select distinct r_client_id, id1, id2,&#xd;&#xa; decision, &#xd;&#xa; app_date,&#xd;&#xa; r_credit_info_id,&#xd;&#xa; rank&#x28;&#x29; over &#x28;partition by r_client_id, id1 order by r_client_id, id1, decision DESC, r_credit_info_id DESC &#x29; as rrow from&#xd;&#xa;&#x28;select  rra.r_client_id&#xd;&#xa;	,rra.r_application_id as id1&#xd;&#xa;	, raa.r_application_id as id2&#xd;&#xa;	,r_credit_info_id&#xd;&#xa;	--,decision&#xd;&#xa;	,case when decision &#x3d; &#x27;ERROR&#x27; then &#x27;ERROR&#x27;&#xd;&#xa;		 when decision is null then &#x27;no info&#x27;&#xd;&#xa;		else &#x27;ok&#x27; end as decision&#xd;&#xa;			,rra.app_date&#x3a;&#x3a;date&#xd;&#xa;	--,raa.app_date&#x3a;&#x3a;date&#xd;&#xa;&#xd;&#xa;	from&#xd;&#xa;	r_application rra join r_application raa on &#x28;rra.r_client_id &#x3d; raa.r_client_id&#x29;-- and raa.app_date &#x3c; rra.app_date&#x29;&#xd;&#xa;	left outer join r_credit_info ci on &#x28;raa.r_application_id &#x3d; ci.r_application_id&#x29;&#xd;&#xa;&#xd;&#xa;	where 1&#x3d;1 &#xd;&#xa;	--and rra.r_client_id &#x3d; 45254&#xd;&#xa;	--in &#x28;&#xd;&#xa;&#xd;&#xa;	and raa.r_application_id &#x3c;&#x3d; rra.r_application_id&#xd;&#xa;	and decision  &#x3c;&#x3e; &#x27;no info&#x27;&#xd;&#xa;	order by rra.r_client_id, rra.r_application_id, raa.r_application_id DESC&#x29;aa&#xd;&#xa;&#xd;&#xa;	order by id1, decision DESC&#x29;cc&#xd;&#xa;	where rrow &#x3d;1&#x29;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;, neg as 	&#xd;&#xa;	&#x28;with&#xd;&#xa;xx as &#x28;select r_credit_info_id, status, count&#x28;status&#x29; as count_isnegative from r_credit_info_negative where status &#x3d; &#x27;IsNegative&#x27; group by 1,2&#x29;,&#xd;&#xa;&#xd;&#xa;yy as &#x28;select r_credit_info_id, status, count&#x28;status&#x29; as count_wasnegative from r_credit_info_negative where status &#x3d; &#x27;WasNegative&#x27; group by 1,2&#x29;&#xd;&#xa;&#xd;&#xa;SELECT r.r_credit_info_id, r.debtor_existing_operations, &#xd;&#xa;       r.debtor_terminated_operations ,&#xd;&#xa;        xx.count_isnegative, yy.count_wasnegative&#xd;&#xa;FROM r_credit_info r&#xd;&#xa;    left outer  join xx xx&#xd;&#xa;  on &#x28;r.r_credit_info_id &#x3d; xx.r_credit_info_id&#x29; &#xd;&#xa;  left outer join yy yy on &#x28;r.r_credit_info_id &#x3d; yy.r_credit_info_id&#x29;&#x29;&#xd;&#xa;-- need to add count of credit calls for errors&#xd;&#xa;---&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;3	&#xd;&#xa;&#xd;&#xa;SELECT &#xd;&#xa;&#xd;&#xa;distinct l.r_loan_id,&#xd;&#xa; l.loan_Date&#x3a;&#x3a;date as prev_loan_date,&#xd;&#xa; extract&#x28;week from l.loan_Date&#x29; as iso_week, --&#x23;&#x23;&#x23; extract week number&#xd;&#xa; l.status, &#xd;&#xa;substring&#x28;l.loan_no from &#x27;..&#x24;&#x27;&#x29;&#x3a;&#x3a;numeric loan_seq,&#xd;&#xa;case when substring&#x28;l.loan_no from &#x27;..&#x24;&#x27;&#x29;&#x3a;&#x3a;numeric &#x3d;1 then &#x27;first&#x27; else &#x27;repeated&#x27; end as loan_type,&#xd;&#xa; l.loan_amount as prev_loan_amount&#xd;&#xa;, l.loan_commission as prev_loan_commission,&#xd;&#xa;&#xd;&#xa;dd.amount as amount_distr,&#xd;&#xa; dd.amount_principal as amount_principal_distr, &#xd;&#xa;       dd.amount_commission as amount_commission_distr, &#xd;&#xa;       dd.amount_penalty as amount_penalty_distr,&#xd;&#xa;        dd.amount_surplus as amount_surplus_distr, &#xd;&#xa;        dd.amount_prolongation as amount_prolongation_distr, &#xd;&#xa;        dd.amount_fees as amount_fees_distr&#xd;&#xa; 