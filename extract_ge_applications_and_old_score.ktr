    <sql>SELECT&#xd;&#xa;a.r_application_id&#xd;&#xa;,a.app_date&#x3a;&#x3a;date&#xd;&#xa;,a.status&#xd;&#xa;,a.reject_reason&#xd;&#xa;,a.score&#xd;&#xa;--,c.accept_marketing&#xd;&#xa;--,extract&#x28;dow from a.app_date&#x29; as APPLICATION_DAY_OF_WEEK&#xd;&#xa;--,extract&#x28;day from a.app_date&#x29; as APPLICATION_DAY_OF_MONTH&#xd;&#xa;--,extract&#x28;years from &#x28;-age&#x28;a.app_date&#x3a;&#x3a;date&#x29; &#x2b; age&#x28;c.dob&#x29;&#x29;&#x29; as age&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;,&#x28;case when extract&#x28;dow from a.app_date&#x29; &#x3d; 1 then 0&#xd;&#xa;	 when extract&#x28;dow from a.app_date&#x29; &#x3d; 2 then 0&#xd;&#xa;	  when extract&#x28;dow from a.app_date&#x29; &#x3d; 3 then 0&#xd;&#xa;	   when extract&#x28;dow from a.app_date&#x29; &#x3d; 4 then 0&#xd;&#xa;	    when extract&#x28;dow from a.app_date&#x29; &#x3d; 5 then 0&#xd;&#xa;	     when extract&#x28;dow from a.app_date&#x29; &#x3d; 6 then 0&#xd;&#xa;	   else 0 end &#xd;&#xa;&#xd;&#xa;&#x2b;&#xd;&#xa;&#xd;&#xa;case when extract&#x28;day from a.app_date&#x29; &#x3c; 8 then 0&#xd;&#xa;	 when extract&#x28;day from a.app_date&#x29; &#x3c; 14 then 58.145&#xd;&#xa;	  when extract&#x28;day from a.app_date&#x29; &#x3c; 18 then 8.098&#xd;&#xa;	   when extract&#x28;day from a.app_date&#x29; &#x3c; 24 then 28.856&#xd;&#xa;	   else 65.222 end &#xd;&#xa;&#xd;&#xa;&#x2b;&#xd;&#xa;	   &#xd;&#xa;case when extract&#x28;hour from a.app_date&#x29; &#x3c; 10 then 8.086&#xd;&#xa;	 when extract&#x28;hour from a.app_date&#x29; &#x3c; 12 then 49.624&#xd;&#xa;	  when extract&#x28;hour from a.app_date&#x29; &#x3c; 16 then 0&#xd;&#xa;	   when extract&#x28;hour from a.app_date&#x29; &#x3c; 18 then 31.259&#xd;&#xa;	   else 19.193 end &#xd;&#xa;	   &#xd;&#xa;&#x2b;&#xd;&#xa;&#xd;&#xa;case when extract&#x28;years from &#x28;-age&#x28;a.app_date&#x3a;&#x3a;date&#x29; &#x2b; age&#x28;c.dob&#x29;&#x29;&#x29; &#x3c; 28 then 0&#xd;&#xa;	when extract&#x28;years from &#x28;-age&#x28;a.app_date&#x3a;&#x3a;date&#x29; &#x