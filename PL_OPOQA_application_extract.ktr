ft outer join lending.loan l on &#x28;l.loan_application_id &#x3d; la.id&#x29;	&#xd;&#xa;&#xd;&#xa;left outer join &#x28;select &#x2a; from &#x28;select distinct loan_application_id, scoring, e.id as score_id, split_run_selected_result, rank&#x28;&#x29; over &#x28;partition by loan_application_id  order by e.id DESC &#x29; as seq_score&#xd;&#xa;from scoring.scorecard_evaluation e &#xd;&#xa;	join scoring.scorecard s on &#x28;e.scorecard_id &#x3d;  s.id and s.name in &#x28;&#x27;CREDIT_BUREAU_SCORECARD_0&#x27;,&#x27;CREDIT_BUREAU_SCORECARD_1&#x27;,&#x27;CREDIT_BUREAU_SCORECARD_2&#x27;,&#x27;CREDIT_BUREAU_SCORECARD_3&#x2b;&#x27;&#x29;&#x29;&#xd;&#xa;&#x29;aa where seq_score&#x3d;1&#x29;ss &#xd;&#xa;	on ss.loan_application_id&#x3d;la.id&#xd;&#xa;&#xd;&#xa;left outer join &#x28;select &#x2a; from &#x28;select distinct loan_application_id, scoring, e.id as score_id, split_run_selected_result, rank&#x28;&#x29; over &#x28;partition by loan_application_id  order by e.id DESC &#x29; as seq_score&#xd;&#xa;from scoring.scorecard_evaluation e &#xd;&#xa;	join scoring.scorecard s on &#x28;e.scorecard_id &#x3d;  s.id and s.name in &#x28;&#x27;CREDIT_BUREAU_SCORECARD_0a&#x27;,&#x27;CREDIT_BUREAU_SCORECARD_1a&#x27;,&#x27;CREDIT_BUREAU_SCORECARD_2a&#x27;,&#x27;CREDIT_BUREAU_SCORECARD_3&#x2b;a&#x27;&#x29;&#x29;&#x29;aa where seq_score&#x3d;1&#x29;sss &#xd;&#xa;	on sss.loan_application_id&#x3d;la.id&#xd;&#xa;&#xd;&#xa;left outer join &#x28;select &#x2a; from &#x28;select distinct loan_application_id, scoring, e.id as score_id, split_run_selected_result, e.evaluation_time, rank&#x28;&#x29; over &#x28;partition by loan_application_id  order by e.id ASC &#x29; as seq_score&#xd;&#xa;from scoring.scorecard_evaluation e&#xd;&#xa;       join scoring.scorecard s on &#x28;e.scorecard_id &#x3d;  s.id and s.name in &#x28;&#x27;CREDIT_BUREAU_SCORECARD_0a&#x27;,&#x27;CREDIT_BUREAU_SCORECARD_1a&#x27;,&#x27;CREDIT_BUREAU_SCORECARD_2a&#x27;,&#x27;CREDIT_BUREAU_SCORECARD_3&#x2b;a&#x27;&#x29;&#x29;&#x29;aa where seq_score&#x3d;1&#x29;ssss&#xd;&#xa;       on ssss.loan_application_id&#x3d;la.id&#xd;&#xa;&#xd;&#xa;left outer join &#x28;select &#x2a; from &#x28;select distinct loan_application_id, scoring, e.id as score_id, split_run_selected_result, rank&#x28;&#x29; over &#x28;partition by loan_application_id  order by e.id DESC &#x29; as seq_score&#xd;&#xa;from scoring.scorecard_evaluation e &#xd;&#xa;	join scoring.scorecard s on &#x28;e.scorecard_id &#x3d;  s.id and s.name in &#x28;&#x27;LENDING_RULES_SCORECARD_a&#x27;&#x29;&#x29;&#xd;&#xa;&#x29;aa where seq_score&#x3d;1&#x29;sslr&#xd;&#xa;	on sslr.loan_application_id&#x3d;la.id&#xd;&#xa;&#xd;&#xa;left outer join &#x28;select  client_id, max&#x28;fee_paid&#x29; as amount_paid, max&#x28;created_at&#x29; as created_at,  max&#x28;post_date&#x29; as post_date  &#xd;&#xa;from  &#xd;&#xa;transaction.transaction t &#xd;&#xa;	where 1&#x3d;1 and transaction_type &#x3d;  &#x27;FEE_PAYMENT&#x27; &#xd;&#xa;	and t.transaction_sub_type &#x3d; &#x27;REGISTRATION_FEE&#x27;&#xd;&#xa;	group by client_id &#x29;fp&#xd;&#xa;	on &#x28;fp.client_id &#x3d; la.client_id&#x29;&#xd;&#xa;&#xd;&#xa;left outer join &#x28;select k.&#x2a;,rank&#x28;&#x29; over &#x28;partition by loan_application_id  order by created_at DESC&#x29; as rank&#xd;&#xa; from poland_krd.response k&#x29;krd on &#x28;krd.loan_application_id&#x3d;la.id and krd.rank &#x3d; 1&#x29; -- need to select the latest&#x21;&#x21;&#xd;&#xa;&#xd;&#xa;left outer join &#x28;select e.&#x2a;,rank&#x28;&#x29; over &#x28;partition by loan_application_id  order by created_at DESC&#x29; as rank&#xd;&#xa; from poland_eucb.response e&#x29;eucb on &#x28;eucb.loan_application_id&#x3d;la.id and eucb.rank &#x3d; 1&#x29;&#xd;&#xa;&#xd;&#xa;left outer join &#x28;select e.&#x2a;,rank&#x28;&#x29; over &#x28;partition by loan_application_id  order by created_at DESC&#x29; as rank&#xd;&#xa; from poland_erif.response e&#x29;erif on &#x28;erif.loan_application_id&#x3d;la.id and erif.rank &#x3d; 1&#x29;&#xd;&#xa;&#xd;&#xa;left outer join &#x28;select &#x2a; from &#x28;SELECT distinct client_id,  json_value&#x3a;&#x3a;json -&#x3e;&#x3e;&#x27;monthlyIncome&#x27; as income, rank&#x28;&#x29; over &#x28;partition by client_id  order by created_at DESC&#x29; as rrow&#xd;&#xa;  FROM crm.client_attribute where key &#x3d; &#x27;employmentData&#x27;&#x29;aa where rrow&#x3d;1&#x29;inc on &#x28;la.client_id &#x3d; inc.client_id&#x29;&#xd;&#xa;&#xd;&#xa;left outer join scoring.scorecard_variable_score vs on &#x28;vs.attribute_score_id&#x3d;ss.score_id and vs.name &#x3d; &#x27;KrdMaxDelay&#x27;&#x29;&#xd;&#xa;left outer join scoring.scorecard_variable_score vso on &#x28;vso.attribute_score_id&#x3d;ss.score_id and vso.name &#x3d; &#x27;OrigAmtVsPrevLoanAmt&#x27;&#x29;&#xd;&#xa;left outer join scoring.scorecard_variable_score vsd on &#x28;vsd.attribute_score_id&#x3d;ss.score_id and vsd.name &#x3d; &#x27;DaysSinceLastPaidDebtKRD&#x27;&#x29;&#xd;&#xa;left outer join scoring.scorecard_variable_score vsap on &#x28;vsap.attribute_score_id&#x3d;ss.score_id and vsap.name &#x3d; &#x27;LnAveragePaymentTimeInDaysKRD&#x27;&#x29;&#xd;&#xa;left outer join scoring.scorecard_variable_score vsc on &#x28;vsc.attribute_score_id&#x3d;ss.score_id and vsc.name &#x3d; &#x27;Constant&#x27;&#x29;&#xd;&#xa;left outer join scoring.scorecard_variable_score vsp on &#x28;vsp.attribute_score_id&#x3d;ss.score_id and vsp.name &#x3d; &#x27;PrevLoanDPD&#x27;&#x29;&#xd;&#xa;left outer join scoring.scorecard_variable_score vsk on &#x28;vsk.attribute_score_id&#x3d;ss.score_id and vsk.name &#x3d; &#x27;KrdCountPaidDebt&#x27;&#x29;&#xd;&#xa;left outer join scoring.scorecard_variable_score vse on &#x28;vse.attribute_score_id&#x3d;ss.score_id and vse.name &#x3d; &#x27;CasesEUCB&#x27;&#x29;&#xd;&#xa;left outer join scoring.scorecard_variable_score vsx on &#x28;vsx.attribute_score_id&#x3d;ss.score_id and vsx.name &#x3d; &#x27;EUCBExpiredCount&#x27;&#x29;&#xd;&#xa;&#xd;&#xa;left outer join &#x28;select d.&#x2a;, rank&#x28;&#x29; over &#x28;partition by d.loan_application_id  order by d.id desc&#x29; as seq from poland_deltavista.response d&#x29;dv on &#x28;la.id &#x3d; dv.loan_application_id and dv.seq &#x3d;1&#x29;&#xd;&#xa;&#xd;&#xa;left outer join crm.client cc on &#x28;cc.id &#x3d; la.client_id&#x29;&#xd;&#xa;&#xd;&#xa;left outer join lending.payment_plan pp on &#x28;pp.loan_id &#x3d; la.loan_id and pp.latest &#x3d; TRUE&#x29;&#xd;&#xa;&#xd;&#xa;left outer join &#x28;select min&#x28;booking_date&#x29;&#x3a;&#x3a;date as first_payment_date, max&#x28;booking_date&#x29;&#x3a;&#x3a;date as last_payment_date, loan_id&#xd;&#xa;&#xd;&#xa;from transaction.transaction where transaction_type in &#x28;&#x27;REPAYMENT&#x27;, &#x27;FEE_PAYMENT&#x27;&#x29; and voided &#x3d; FALSE group by loan_id&#x29;pay on &#x28;la.loan_id &#x3d; pay.loan_id&#x29;&#xd;&#xa;--left outer join migration_source.r_application ra on &#x28;la.id&#x3d;ra.r_application_id&#x29;&#xd;&#xa;&#xd;&#xa;left outer join &#x28;select loan_application_id, max&#x28;case_count&#x29; as cases_count_new, count&#x28;copy_of&#x29; as eucb_copy  from poland_eucb.response group by loan_application_id&#x29;ecc on &#x28;ecc.loan_application_id &#x3d; la.id&#x29;&#xd;&#xa;&#xd;&#xa;left outer join &#x28;select loan_application_id,  count&#x28;copy_of&#x29; as krd_copy  from poland_krd.response group by loan_application_id&#x29;krdc on &#x28;krdc.loan_application_id &#x3d; la.id&#x29;&#xd;&#xa;&#xd;&#xa;left outer join lending.loan_application_attribute attr on &#x28;la.id&#x3d;attr.application_id and attr.key &#x3d; &#x27;loanPurpose&#x27;&#x29;&#xd;&#xa;&#xd;&#xa;left outer join &#x28;select e.client_id,  lp.name as first_landing_page, rank&#x28;&#x29; over &#x28;partition by e.client_id  order by e.created_at ASC&#x29; as seq&#xd;&#xa;from marketing_landing.event e &#xd;&#xa;join marketing_landing.landing_page lp on &#x28;e.landing_page_id&#x3d;lp.id&#x29;&#xd;&#xa; where event_type &#x3d; &#x27;LEAD&#x27;&#x29;lpf on &#x28;la.client_id &#x3d; lpf.client_id and lpf.seq&#x3d;1&#x29;&#xd;&#xa;&#xd;&#xa;left outer join &#x28;SELECT &#xd;&#xa;	&#x2a;&#xd;&#xa;  FROM &#xd;&#xa;	&#x28;SELECT v.client_id, e.utm_source, e.utm_campaign, e.referrer_domain, e.created_at&#x3a;&#x3a;Date as event_date&#xd;&#xa;	&#xd;&#xa;	, e.geo_location, e.os, e.device, e.screen_size, la.id&#xd;&#xa;	,rank&#x28;&#x29; over &#x28;partition by la.client_id order by e.created_at&#x29; as rrow &#xd;&#xa;	from marketing_tracking.event e&#xd;&#xa;  join marketing_tracking.visitor v on &#x28;v.id &#x3d; e.visitor_id&#x29; join &#xd;&#xa;  	lending.loan_application la on &#x28;la.client_id &#x3d; v.client_id&#x29;&#x29;aa&#xd;&#xa;  where rrow&#x3d;1&#x29;mt2 on &#x28;mt2.id &#x3d; la.id&#x29;&#xd;&#xa;&#xd;&#xa;left outer join &#x28;select client_id, response, rank&#x28;&#x29; over &#x28;partition by client_id  order by created_at DESC&#x29; as rank from poland.cross_check&#x29;cch on &#x28;la.client_id&#x3d;cch.client_id and cch.rank&#x3d;1&#x29;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#x2f;&#x2a;left outer join &#x28;SELECT &#xd;&#xa;	&#x2a;&#xd;&#xa;  FROM &#xd;&#xa;	&#x28;SELECT v.client_id, e.utm_source, e.utm_campaign, e.referrer_domain, e.created_at&#x3a;&#x3a;Date as event_date, e.referrer, e.utm_medium,&#xd;&#xa;	&#xd;&#xa;	case when e.url_params_json&#x3a;&#x3a;json -&#x3e;&#x3e;&#x27;utm_source&#x27; is null then&#xd;&#xa;	e.url_params_json&#x3a;&#x3a;json -&#x3e;&#x3e;&#x27;&#x25;3Futm_source&#x27; else e.url_params_json&#x3a;&#x3a;json -&#x3e;&#x3e;&#x27;utm_source&#x27; end as utm_source_json,&#xd;&#xa;	e.browser, e.geo_location, e.os, e.device, e.screen_size, la.id&#xd;&#xa;	,rank&#x28;&#x29; over &#x28;partition by la.client_id order by e.created_at&#x29; as rrow &#xd;&#xa;	from marketing_tracking.event e&#xd;&#xa;  join marketing_tracking.visitor v on &#x28;v.id &#x3d; e.visitor_id&#x29; join &#xd;&#xa;  	lending.loan_application la on &#x28;la.client_id &#x3d; v.client_id&#x29;&#x29;aa&#xd;&#xa;  where rrow&#x3d;1&#x29;mt2 on &#x28;mt2.id &#x3d; la.id&#x29;&#x2a;&#x2f;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;--limit 100</sql>
