  <step>
    <name>get application view</name>
    <type>TableInput</type>
    <description/>
    <distribute>N</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>Opoqa</connection>
    <sql>with app_ranks as &#x28;SELECT a.id, seq_application_total, seq_application_confirm as confirmed_rank, seq_application_reject as rejected_rank&#xd;&#xa;,case when seq_application_reject &#x3e; 0 then seq_application_reject else seq_application_total- seq_application_confirm end as seq_application_reject&#xd;&#xa;,case when seq_application_confirm &#x3e; 0 then seq_application_confirm else seq_application_total- seq_application_reject end as seq_application_confirm&#xd;&#xa;,seq_application_per_loan&#xd;&#xa;  FROM lending.loan_application a join &#x28;select id, rank&#x28;&#x29; over &#x28;partition by client_id &#xd;&#xa;  order by client_id ASC, id ASC &#x29; as seq_application_total  from lending.loan_application &#x29;aa &#xd;&#xa;  on a.id &#x3d; aa.id&#xd;&#xa;&#xd;&#xa;  left outer join &#x28;select id, rank&#x28;&#x29; over &#x28;partition by client_id &#xd;&#xa;  order by client_id ASC, id ASC &#x29; as seq_application_confirm from lending.loan_application&#xd;&#xa;  where status &#x3d; &#x27;APPROVED&#x27;&#x29;ac &#xd;&#xa;  on a.id &#x3d; ac.id&#xd;&#xa;&#xd;&#xa;  left outer join &#x28;select id, rank&#x28;&#x29; over &#x28;partition by client_id &#xd;&#xa;  order by client_id ASC, id ASC &#x29; as seq_application_reject from lending.loan_application &#xd;&#xa;  where status &#x3d; &#x27;REJECTED&#x27;&#x29;ar &#xd;&#xa;  on a.id &#x3d; ar.id&#xd;&#xa;&#xd;&#xa;  left outer join &#x28;select id, rank&#x28;&#x29; over &#x28;partition by client_id, loans_paid&#xd;&#xa;  order by client_id ASC, loans_paid ASC, id ASC &#x29; as seq_application_per_loan from lending.loan_application &#xd;&#xa;  &#x29;aspl &#xd;&#xa;  on &#x28;a.id &#x3d; aspl.id&#x29;&#xd;&#xa;  &#xd;&#xa;  order by id, a.id&#x29; &#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;SELECT &#xd;&#xa;distinct &#xd;&#xa;la.id as application_id&#xd;&#xa;, la.application_date&#x3a;&#x3a;date as app_date&#xd;&#xa;,extract&#x28;hour from la.created_at&#x29;&#x7c;&#x7c;&#x27;&#x3a;&#x27;&#x7c;&#x7c;ext