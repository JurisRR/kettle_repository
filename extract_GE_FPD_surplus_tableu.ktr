    </partitioning>
    <connection>Georgia</connection>
    <sql>-- &#x23;&#x23; Extract data on loan level including credit info, client, application data&#xd;&#xa;&#xd;&#xa;--&#x23;&#x23; aggregation of payments_distr table on loan level&#xd;&#xa;with dd as &#x28;select d.r_loan_id,&#xd;&#xa;sum&#x28;d.amount&#x29; as amount, sum&#x28;d.amount_principal&#x29; as amount_principal, &#xd;&#xa;       sum&#x28;d.amount_commission&#x29; as amount_commission, sum&#x28;d.amount_penalty&#x29; as amount_penalty, sum&#x28;d.amount_surplus&#x29; as amount_surplus,&#xd;&#xa;        sum&#x28;d.amount_prolongation&#x29; as amount_prolongation, sum&#x28;d.amount_fees&#x29; as amount_fees&#xd;&#xa;	&#xd;&#xa;from &#xd;&#xa;r_payment_distr d join r_payment p on  &#x28;d.r_payment_id &#x3d; p.r_payment_id&#x29; &#xd;&#xa;where 1&#x3d;1 &#xd;&#xa;	and p.cl_bank_acct_id &#x3c;&#x3e; 18&#xd;&#xa;	--and p.cl_bank_acct_id &#x3c;&#x3e; 12&#xd;&#xa;	group by d.r_loan_id&#x29;&#xd;&#xa;&#xd;&#xa;,pay as &#x28;select d.r_loan_id, min&#x28;payment_date&#x29;&#x3a;&#x3a;date as first_payment_date , max&#x28;payment_date&#x29;&#x3a;&#x3a;date as last_payment_date&#xd;&#xa;	from r_payment_distr d join r_payment p on  &#x28;d.r_payment_id &#x3d; p.r_payment_id&#x29; &#xd;&#xa;		where p.type &#x3d; &#x27;IN_Loan&#x27; and p.cl_bank_acct_id &#x3c;&#x3e; 12 and cl_bank_acct_id &#x3c;&#x3e;  18&#xd;&#xa;		group by d.r_loan_id&#x29;&#xd;&#xa;&#xd;&#xa;,vpay as &#x28;select d.r_loan_id, min&#x28;payment_date&#x29;&#x3a;&#x3a;date as first_virtual_payment_date , max&#x28;payment_date&#x29;&#x3a;&#x3a;date as last_payment_date&#xd;&#xa;	from r_payment_distr d join r_payment p on  &#x28;d.r_payment_id &#x3d; p.r_payment_id&#x29; &#xd;&#xa;		where p.type &#x3d; &#x27;IN_Loan&#x27; and &#x28;p.cl_bank_acct_id &#x3d; 12 or cl_bank_acct_id &#x3d;  18&#x29;&#xd;&#xa;		group by d.r_loan_id&#x29;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;,out as &#x28;select d.r_loan_id, min&#x28;payment_date&#x29;&#x3a;&#x3a;date as out_loan_date &#xd;&#xa;	from r_payment_distr d join r_payment p on  &#x28;d.r_payment_id &#x3d; p.r_payment_id&#x29; &#xd;&#xa;		where p.type &#x3d; &#x27;OUT_Loan&#x27; and p.cl_bank_acct_id &#x3c;&#x3e; 12 and cl_bank_acct_id &#x3c;&#x3e;  18&#xd;&#xa;		group by d.r_loan_id&#x29;&#xd;&#xa;&#xd;&#xa;,freex as &#x28;select d.r_loan_id, wp.amount_prolongation , wp.days_prolongation, min&#x28;p.payment_date&#x29;&#x3a;&#x3a;date as free_extension_date&#xd;&#xa;	from r_payment_distr d join r_payment p on  &#x28;d.r_payment_id &#x3d; p.r_payment_id&#x29; &#xd;&#xa;		join r_withdrawal_prolongation_log wp on &#x28;d.r_payment_distr_id &#x3d; wp.r_payment_distr_id&#x29;&#xd;&#xa;		where p.type &#x3d; &#x27;IN_Loan&#x27; and p.cl_bank_acct_id &#x3d; 18&#xd;&#xa;		group by d.r_loan_id, wp.amount_prolongation , wp.days_prolongation&#x29;&#xd;&#xa;&#xd;&#xa;--&#x23;&#x23;&#x23;&#x23;&#x23; add payments by 30 day intervals&#xd;&#xa;,pint as &#x28;select r_loan_id,&#xd;&#xa;max&#x28;case when interval30 &#x3d; 30 then sum else 0 end&#x29; as amount30,&#xd;&#xa;max&#x28;case when interval30 &#x3d; 60 then sum else 0 end&#x29; as amount60,&#xd;&#xa;max&#x28;case when interval30 &#x3d; 90 then sum else 0 end&#x29; as amount90,&#xd;&#xa;max&#x28;case when interval30 &#x3d; 120 then sum else 0 end&#x29; as amount120,&#xd;&#xa;max&#x28;case when interval30 &#x3d; 150 then sum else 0 end&#x29; as amount150,&#xd;&#xa;max&#x28;case when interval30 &#x3d; 180 then sum else 0 end&#x29; as amount180,&#xd;&#xa;max&#x28;case when interval30 &#x3d; 210 then sum else 0 end&#x29; as amount210,&#xd;&#xa;max&#x28;case when interval30 &#x3d; 240 then sum else 0 end&#x29; as amount240,&#xd;&#xa;max&#x28;case when interval30 &#x3d; 270 then sum else 0 end&#x29; as amount270,&#xd;&#xa;max&#x28;case when interval30 &#x3d; 300 then sum else 0 end&#x29; as amount300,&#xd;&#xa;max&#x28;case when interval30 &#x3d; 330 then sum else 0 end&#x29; as amount330,&#xd;&#xa;max&#x28;case when interval30 &#x3d; 360 then sum else 0 end&#x29; as amount360,&#xd;&#xa;max&#x28;case when interval30 &#x3d; 361 then sum else 0 end&#x29; as amount361&#xd;&#xa;&#xd;&#xa;from &#x28;select r_loan_id, interval30, sum&#x28;amount&#x29; from &#x28;select d.r_loan_id, &#x28;d.amount-d.amount_surplus&#x29; as amount,&#xd;&#xa;case when &#x28;payment_date&#x3a;&#x3a;date -l.loan_date&#x3a;&#x3a;date&#x29; &#x3c;&#x3d; 30 then 30&#xd;&#xa;	when &#x28;payment_date&#x3a;&#x3a;date -l.loan_date&#x3a;&#x3a;date&#x29; &#x3c;&#x3d; 60 then 60&#xd;&#xa;	when &#x28;payment_date&#x3a;&#x3a;date -l.loan_date&#x3a;&#x3a;date&#x29; &#x3c;&#x3d; 90 then 90&#xd;&#xa;	when &#x28;payment_date&#x3a;&#x3a;date -l.loan_date&#x3a;&#x3a;date&#x29; &#x3c;&#x3d; 120 then 120&#xd;&#xa;	when &#x28;payment_date&#x3a;&#x3a;date -l.loan_date&#x3a;&#x3a;date&#x29; &#x3c;&#x3d; 150 then 150&#xd;&#xa;	when &#x28;payment_date&#x3a;&#x3a;date -l.loan_date&#x3a;&#x3a;date&#x29; &#x3c;&#x3d; 180 then 180&#xd;&#xa;	when &#x28;payment_date&#x3a;&#x3a;date -l.loan_date&#x3a;&#x3a;date&#x29; &#x3c;&#x3d; 210 then 210&#xd;&#xa;	when &#x28;payment_date&#x3a;&#x3a;date -l.loan_date&#x3a;&#x3a;date&#x29; &#x3c;&#x3d; 240 then 240&#xd;&#xa;	when &#x28;payment_date&#x3a;&#x3a;date -l.loan_date&#x3a;&#x3a;date&#x29; &#x3c;&#x3d; 270 then 270&#xd;&#xa;	when &#x28;payment_date&#x3a;&#x3a;date -l.loan_date&#x3a;&#x3a;date&#x29; &#x3c;&#x3d; 300 then 300&#xd;&#xa;	when &#x28;payment_date&#x3a;&#x3a;date -l.loan_date&#x3a;&#x3a;date&#x29; &#x3c;&#x3d; 330 then 330&#xd;&#xa;	when &#x28;payment_date&#x3a;&#x3a;date -l.loan_date&#x3a;&#x3a;date&#x29; &#x3c;&#x3d; 360 then 360 &#xd;&#xa;	else 361 end as interval30&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;from r_payment_distr d join r_payment p on  &#x28;d.r_payment_id &#x3d; p.r_payment_id&#x29; join r_loan l on &#x28;l.r_loan_id &#x3d; d.r_loan_id&#x29;&#xd;&#xa;	where p.type &#x3d; &#x27;IN_Loan&#x27; and p.cl_bank_acct_id &#x3c;&#x3e; 18&#xd;&#xa;&#x29;dd		&#xd;&#xa;group by r_loan_id, interval30&#xd;&#xa;order by r_loan_id, interval30&#x29;int30&#xd;&#xa;&#xd;&#xa;group by r_loan_id&#x29;&#xd;&#xa;&#xd;&#xa;--&#x23;&#x23;&#x23;&#x23; add recovery amounts by intervals&#x3b; 24.01.2017&#xd;&#xa;,rec as &#x28;select r_loan_id,&#xd;&#xa;max&#x28;case when interval30 &#x3d; 0 then sum else 0 end&#x29; as recovery0,&#xd;&#xa;max&#x28;case when interval30 &#x3d; 5 then sum else 0 end&#x29; as recovery5,&#xd;&#xa;max&#x28;case when interval30 &#x3d; 30 then sum else 0 end&#x29; as recovery30,&#xd;&#xa;max&#x28;case when interval30 &#x3d; 60 then sum else 0 end&#x29; as recovery60,&#xd;&#xa;max&#x28;case when interval30 &#x3d; 90 then sum else 0 end&#x29; as recovery90,&#xd;&#xa;max&#x28;case when interval30 &#x3d; 91 then sum else 0 end&#x29; as recovery91&#xd;&#xa;&#xd;&#xa;from &#x28;select r_loan_id, interval30, sum&#x28;amount&#x29; from &#x28;select d.r_loan_id, &#x28;d.amount-d.amount_surplus&#x29; as amount,&#xd;&#xa;case when &#x28;payment_date&#x3a;&#x3a;date -l.term_date&#x3a;&#x3a;date&#x29; &#x3c;&#x3d; 0 then 0&#xd;&#xa;	when &#x28;payment_date&#x3a;&#x3a;date -l.term_date&#x3a;&#x3a;date&#x29; &#x3c;&#x3d; 5 then 5&#xd;&#xa;	when &#x28;payment_date&#x3a;&#x3a;date -l.term_date&#x3a;&#x3a;date&#x29; &#x3c;&#x3d; 30 then 30&#xd;&#xa;	when &#x28;payment_date&#x3a;&#x3a;date -l.term_date&#x3a;&#x3a;date&#x29; &#x3c;&#x3d; 60 then 60&#xd;&#xa;	when &#x28;payment_date&#x3a;&#x3a;date -l.term_date&#x3a;&#x3a;date&#x29; &#x3c;&#x3d; 90 then 90&#xd;&#xa;	else 91 end as interval30&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;from r_payment_distr d join r_payment p on  &#x28;d.r_payment_id &#x3d; p.r_payment_id&#x29; join r_loan l on &#x28;l.r_loan_id &#x3d; d.r_loan_id&#x29;&#xd;&#xa;	where p.type &#x3d; &#x27;IN_Loan&#x27; and p.cl_bank_acct_id &#x3c;&#x3e; 18&#xd;&#xa;&#x29;dd		&#xd;&#xa;group by r_loan_id, interval30&#xd;&#xa;order by r_loan_id, interval30&#x29;int30&#xd;&#xa;&#xd;&#xa;group by r_loan_id&#x29;&#xd;&#xa;--&#x23;&#x23;&#x23;&#x23; end of recovery amounts&#x23;&#x23;&#x23;&#x23;&#xd;&#xa;&#xd;&#xa;--&#x23;&#x23;&#x23;&#x23; add principal repayments by months&#xd;&#xa;,princ as &#x28;select r_loan_id,&#xd;&#xa;max&#x28;case when interval30 &#x3d; 0 then sum else 0 end&#x29; as amount_0m,&#xd;&#xa;max&#x28;case when interval30 &#x3d; 1 then sum else 0 end&#x29; as amount_1m,&#xd;&#xa;max&#x28;case when interval30 &#x3d; 2 then sum else 0 end&#x29; as amount_2m,&#xd;&#xa;max&#x28;case when interval30 &#x3d; 3 then sum else 0 end&#x29; as amount_3m,&#xd;&#xa;max&#x28;case when interval30 &#x3d; 4 then sum else 0 end&#x29; as amount_4m,&#xd;&#xa;max&#x28;case when interval30 &#x3d; 5 then sum else 0 end&#x29; as amount_5m,&#xd;&#xa;max&#x28;case when interval30 &#x3d; 6 then sum else 0 end&#x29; as amount_6m,&#xd;&#xa;max&#x28;case when interval30 &#x3d; 7 then sum else 0 end&#x29; as amount_7m,&#xd;&#xa;max&#x28;case when interval30 &#x3d; 8 then sum else 0 end&#x29; as amount_8m,&#xd;&#xa;max&#x28;case when interval30 &#x3d; 9 then sum else 0 end&#x29; as amount_9m,&#xd;&#xa;max&#x28;case when interval30 &#x3d; 10 then sum else 0 end&#x29; as amount_10m,&#xd;&#xa;max&#x28;case when interval30 &#x3d; 11 then sum else 0 end&#x29; as amount_11m,&#xd;&#xa;max&#x28;case when interval30 &#x3d; 12 then sum else 0 end&#x29; as amount_12m,&#xd;&#xa;max&#x28;case when interval30 &#x3e; 12 then sum else 0 end&#x29; as amount_13m&#xd;&#xa;&#xd;&#xa;from &#x28;select r_loan_id, interval30, sum&#x28;amount&#x29; from &#x28;select d.r_loan_id, d.amount_principal as amount,&#xd;&#xa;case when payment_date &#x3e;&#x3d; date_trunc&#x28;&#x27;month&#x27;,&#x28;l.loan_date &#x2b; &#x27;0 month&#x27;&#x29;&#x29; and payment_date &#x3c; date_trunc&#x28;&#x27;month&#x27;,&#x28;l.loan_date &#x2b; &#x27;1 month&#x27;&#x29;&#x29; then 0&#xd;&#xa;	when payment_date &#x3e;&#x3d; date_trunc&#x28;&#x27;month&#x27;,&#x28;l.loan_date &#x2b; &#x27;1 month&#x27;&#x29;&#x29; and payment_date &#x3c; date_trunc&#x28;&#x27;month&#x27;,&#x28;l.loan_date &#x2b; &#x27;2 month&#x27;&#x29;&#x29; then 1&#xd;&#xa;	when payment_date &#x3e;&#x3d; date_trunc&#x28;&#x27;month&#x27;,&#x28;l.loan_date &#x2b; &#x27;2 month&#x27;&#x29;&#x29; and payment_date &#x3c; date_trunc&#x28;&#x27;month&#x27;,&#x28;l.loan_date &#x2b; &#x27;3 month&#x27;&#x29;&#x29; then 2&#xd;&#xa;	when payment_date &#x3e;&#x3d; date_trunc&#x28;&#x27;month&#x27;,&#x28;l.loan_date &#x2b; &#x27;3 month&#x27;&#x29;&#x29; and payment_date &#x3c; date_trunc&#x28;&#x27;month&#x27;,&#x28;l.loan_date &#x2b; &#x27;4 month&#x27;&#x29;&#x29; then 3&#xd;&#xa;	when payment_date &#x3e;&#x3d; date_trunc&#x28;&#x27;month&#x27;,&#x28;l.loan_date &#x2b; &#x27;4 month&#x27;&#x29;&#x29; and payment_date &#x3c; date_trunc&#x28;&#x27;month&#x27;,&#x28;l.loan_date &#x2b; &#x27;5 month&#x27;&#x29;&#x29; then 4&#xd;&#xa;	when payment_date &#x3e;&#x3d; date_trunc&#x28;&#x27;month&#x27;,&#x28;l.loan_date &#x2b; &#x27;5 month&#x27;&#x29;&#x29; and payment_date &#x3c; date_trunc&#x28;&#x27;month&#x27;,&#x28;l.loan_date &#x2b; &#x27;6 month&#x27;&#x29;&#x29; then 5&#xd;&#xa;	when payment_date &#x3e;&#x3d; date_trunc&#x28;&#x27;month&#x27;,&#x28;l.loan_date &#x2b; &#x27;6 month&#x27;&#x29;&#x29; and payment_date &#x3c; date_trunc&#x28;&#x27;month&#x27;,&#x28;l.loan_date &#x2b; &#x27;7 month&#x27;&#x29;&#x29; then 6&#xd;&#xa;	when payment_date &#x3e;&#x3d; date_trunc&#x28;&#x27;month&#x27;,&#x28;l.loan_date &#x2b; &#x27;7 month&#x27;&#x29;&#x29; and payment_date &#x3c; date_trunc&#x28;&#x27;month&#x27;,&#x28;l.loan_date &#x2b; &#x27;8 month&#x27;&#x29;&#x29; then 7&#xd;&#xa;	when payment_date &#x3e;&#x3d; date_trunc&#x28;&#x27;month&#x27;,&#x28;l.loan_date &#x2b; &#x27;8 month&#x27;&#x29;&#x29; and payment_date &#x3c; date_trunc&#x28;&#x27;month&#x27;,&#x28;l.loan_date &#x2b; &#x27;9 month&#x27;&#x29;&#x29; then 8&#xd;&#xa;	when payment_date &#x3e;&#x3d; date_trunc&#x28;&#x27;month&#x27;,&#x28;l.loan_date &#x2b; &#x27;9 month&#x27;&#x29;&#x29; and payment_date &#x3c; date_trunc&#x28;&#x27;month&#x27;,&#x28;l.loan_date &#x2b; &#x27;10 month&#x27;&#x29;&#x29; then 9&#xd;&#xa;	when payment_date &#x3e;&#x3d; date_trunc&#x28;&#x27;month&#x27;,&#x28;l.loan_date &#x2b; &#x27;10 month&#x27;&#x29;&#x29; and payment_date &#x3c; date_trunc&#x28;&#x27;month&#x27;,&#x28;l.loan_date &#x2b; &#x27;11 month&#x27;&#x29;&#x29; then 10&#xd;&#xa;	when payment_date &#x3e;&#x3d; date_trunc&#x28;&#x27;month&#x27;,&#x28;l.loan_date &#x2b; &#x27;11 month&#x27;&#x29;&#x29; and payment_date &#x3c; date_trunc&#x28;&#x27;month&#x27;,&#x28;l.loan_date &#x2b; &#x27;12 month&#x27;&#x29;&#x29; then 11&#xd;&#xa;	when payment_date &#x3e;&#x3d; date_trunc&#x28;&#x27;month&#x27;,&#x28;l.loan_date &#x2b; &#x27;12 month&#x27;&#x29;&#x29; and payment_date &#x3c; date_trunc&#x28;&#x27;month&#x27;,&#x28;l.loan_date &#x2b; &#x27;13 month&#x27;&#x29;&#x29; then 12&#xd;&#xa;	else 13 end as interval30&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;from r_payment_distr d join r_payment p on  &#x28;d.r_payment_id &#x3d; p.r_payment_id&#x29; join r_loan l on &#x28;l.r_loan_id &#x3d; d.r_loan_id&#x29;&#xd;&#xa;	where p.type &#x3d; &#x27;IN_Loan&#x27; and p.cl_bank_acct_id &#x3c;&#x3e; 18&#xd;&#xa;&#x29;dd		&#xd;&#xa;group by r_loan_id, interval30&#xd;&#xa;order by r_loan_id, interval30&#x29;int30&#xd;&#xa;&#xd;&#xa;group by r_loan_id&#x29;&#xd;&#xa;&#xd;&#xa;--&#x23;&#x23;&#x23;&#x23; add commission repayments by months&#xd;&#xa;,comm as &#x28;select r_loan_id,&#xd;&#xa;max&#x28;case when interval30 &#x3d; 0 then sum else 0 end&#x29; as amount_0mc,&#xd;&#xa;max&#x28;case when interval30 &#x3d; 1 then sum else 0 end&#x29; as amount_1mc,&#xd;&#xa;max&#x28;case when interval30 &#x3d; 2 then sum else 0 end&#x29; as amount_2mc,&#xd;&#xa;max&#x28;case when interval30 &#x3d; 3 then sum else 0 end&#x29; as amount_3mc,&#xd;&#xa;max&#x28;case when interval30 &#x3d; 4 then sum else 0 end&#x29; as amount_4mc,&#xd;&#xa;max&#x28;case when interval30 &#x3d; 5 then sum else 0 end&#x29; as amount_5mc,&#xd;&#xa;max&#x28;case when interval30 &#x3d; 6 then sum else 0 end&#x29; as amount_6mc,&#xd;&#xa;max&#x28;case