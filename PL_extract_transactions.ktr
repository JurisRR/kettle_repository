    <connection>PL_Slave_Finotech</connection>
    <sql>SELECT &#xd;&#xa;t.id&#xd;&#xa;&#xd;&#xa;, case when t.created_at&#x3a;&#x3a;date &#x3e; &#x27;2017-06-09&#x27; then t.created_at&#x3a;&#x3a;Date else t.post_date&#x3a;&#x3a;Date end as transaction_date&#xd;&#xa;, t.booking_date&#x3a;&#x3a;date&#xd;&#xa;, t.cash_in&#xd;&#xa;, t.cash_out&#xd;&#xa;, t.client_id&#xd;&#xa;, t.fee_applied&#xd;&#xa;, t.fee_paid&#xd;&#xa;, t.interest_applied&#xd;&#xa;, t.interest_paid &#xd;&#xa;       , t.loan_id&#xd;&#xa;       , t.overpayment_received&#xd;&#xa;       , t.overpayment_refunded &#xd;&#xa;       , t.overpayment_used&#xd;&#xa;       , t.penalty_applied&#xd;&#xa;       , t.penalty_paid&#xd;&#xa;       , t.principal_paid &#xd;&#xa;       , t.transaction_type&#xd;&#xa;       , t.transaction_sub_type&#xd;&#xa;&#xd;&#xa;,t.principal_disbursed&#xd;&#xa;,t.interest_written_off&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;,l.issue_date&#x3a;&#x3a;Date&#xd;&#xa;,l.principal_disbursed as issued_amount&#xd;&#xa;,l.loans_paid&#xd;&#xa;,t.fee_paid &#x2b; t.interest_paid &#x2b; t.penalty_paid &#x2b; t.principal_paid as paid_total&#xd;&#xa;,prt.paid_running_total&#xd;&#xa;--,&#x28;select sum&#x28;cash_in&#x29; as paid_running_total from transaction.transaction tt where tt.loan_id &#x3d; t.loan_id and &#xd;&#xa;,pp.maturity_date&#x3a;&#x3a;date&#xd;&#xa;,lsn.total_due as total_due_dpd1&#xd;&#xa;&#xd;&#xa;  FROM transaction.transaction t left outer join lending.loan l on &#x28;l.id &#x3d; t.loan_id&#x29; &#xd;&#xa;&#xd;&#xa;left outer join &#x28;select id, fp&#x2b;ip&#x2b;pp&#x2b;prp as paid_running_total from &#x28;select tt.id,  sum&#x28;ttt.fee_paid&#x29; fp, sum&#x28;ttt.interest_paid&#x29; ip, sum&#x28;ttt.penalty_paid&#x29; pp , sum&#x28;ttt.principal_paid&#x29; prp &#xd;&#xa;	from transaction.transaction tt join transaction.transaction ttt on &#x28;tt.loan_id &#x3d; ttt.loan_id and ttt.id &#x3c;&#x3d; tt.id&#x29; group by tt.id&#x29;p&#xd;&#xa;&#x29;prt on &#x28;prt.id &#x3d; t.id&#x29;&#xd;&#xa;&#xd;&#xa;left outer join lending.payment_plan pp on &#x28;l.id &#x3d; pp.loan_id and pp.latest &#x3d; &#x27;TRUE&#x27;&#x29;&#xd;&#xa;left outer join lending.loan_daily_snapshot lsn on &#x28;l.id &#x3d; lsn.loan_id and lsn.created_at&#x3a;&#x3a;Date &#x3d; pp.maturity_date&#x3a;&#x3a;date &#x2b; 1&#x29;   -- status on dpd1&#xd;&#xa; &#xd;&#xa;&#xd;&#xa;where &#xd;&#xa;&#x28;transaction_type ilike &#x27;&#x25;PAYMENT&#x27; or transaction_type ilike &#x27;WRITE_OFF&#x27; or transaction_type ilike &#x27;DISBURSEMENT&#x27;&#x29;&#xd;&#xa;--and loan_id is not null&#xd;&#xa;and voided &#x3d; FALSE</sql>
