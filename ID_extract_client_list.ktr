  <step>
    <name>get clients</name>
    <type>TableInput</type>
    <description/>
    <distribute>N</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>Indonesia</connection>
    <sql>select&#xd;&#xa;distinct&#xd;&#xa;c.id as client_id&#xd;&#xa;--,c.client_number&#xd;&#xa;,ec.email&#xd;&#xa;,phcon.local_number as mobile&#xd;&#xa;,pi.first_name&#xd;&#xa;,pi.last_name &#xd;&#xa;,coalesce&#x28;cl.loan_cnt,0&#x29; as clients_total_loan_cnt&#xd;&#xa;,coalesce&#x28;cal.act_loan_cnt,0&#x29; as clients_active_loan_cnt&#xd;&#xa;--,case when t.id is null then &#x27;Unidentified&#x27; else &#x27;Identified&#x27; end as cl_status&#xd;&#xa;--,cra.rej_app_cnt&#xd;&#xa;--,cra30.rej_last_month&#xd;&#xa;--,last_loan_date&#xd;&#xa;--,last_loan_close_date&#xd;&#xa;--,aa.app_cnt&#xd;&#xa;--,aa.last_app_date&#xd;&#xa;--,pi.receive_offers&#xd;&#xa;,&#x27;https&#x3a;&#x2f;&#x2f;kreditcepat.id&#x2f;&#x3f;promo&#x3d;&#x27;&#x7c;&#x7c;c.client_number as promo_code&#xd;&#xa;,coalesce&#x28;app_count,0&#x29; as app_count&#xd;&#xa;,coalesce&#x28;loan_count,0&#x29; as loan_count&#xd;&#xa;,coalesce&#x28;paid_loan_count,0&#x29; as paid_loan_count&#xd;&#xa;,case when valid_cnt &#x3e;&#x3d; 1 then &#x27;completed&#x27; else &#x27;incomplete&#x27; end as registration_status&#xd;&#xa;,case when dsnht.max_overdue_days&#x3e;39 then 1 else 0 end as max_overdue_days_40&#xd;&#xa;,&#x27;main&#x27; as list&#xd;&#xa;&#xd;&#xa;,case when crr.refused_to_reg&#x3e;0 then 1 else 0 end as refused_to_reg&#xd;&#xa;,case when cli.income_too_low&#x3e;0 then 1 else 0 end as income_too_low&#xd;&#xa;,case when coalesce&#x28;ce.salary, 0&#x29; between 2500000 and 3000000 then 1 else 0 end as salary_under3m_yn&#xd;&#xa;,coalesce&#x28;ce.salary, 0&#x29; as salary&#xd;&#xa;,province&#xd;&#xa;,coalesce&#x28;cip.invalid_province,0&#x29; as invalid_province&#xd;&#xa;,ma.download_date&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;from crm.client c&#xd;&#xa;&#xd;&#xa;--left outer join transaction.transaction t on &#x28;c.id &#x3d; t.client_id and transaction_sub_type &#x3d; &#x27;REGISTRATION_FEE&#x27;&#x29;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;left outer join &#x28;select count&#x28;id&#x29; as loan_cnt, max&#x28;issue_date&#x3a;&#x3a;date&#x29; as last_loan_date, max&#x28;close_date&#x3a;&#x3a;date&#x29; as last_loan_close_date, client_id from lending.loan group by client_id&#x29;cl on &#x28;cl.client_id &#x3d; c.id&#x29;&#xd;&#xa;left outer join &#x28;select count&#x28;id&#x29; as act_loan_cnt, client_id from lending.loan where status &#x3d; &#x27;ACTIVE&#x27; group by client_id&#x29;cal  on &#x28;cal.client_id &#x3d; c.id&#x29;&#xd;&#xa;left outer join &#x28;select count&#x28;id&#x29; as act_loan_cnt, client_id from lending.loan where status &#x3d; &#x27;PAID&#x27; group by client_id&#x29;cal2  on &#x28;cal2.client_id &#x3d; c.id&#x29;&#xd;&#xa;&#xd;&#xa;left outer join &#x28;select count&#x28;id&#x29; as rej_app_cnt, client_id from lending.loan_application where &#x28;close_reason is not null and close_reason not ilike &#x27;&#x25;BAD_SCORING&#x25;&#x27;&#x29; group by client_id&#x29;cra  on &#x28;cra.client_id &#x3d; c.id&#x29;&#xd;&#xa;left outer join &#x28;select count&#x28;id&#x29; as rej_last_month, client_id from lending.loan_application where close_reason is not null and application_date&#x3a;&#x3a;date &#x3e; &#x28;current_date-31&#x29; group by client_id&#x29;cra30  on &#x28;cra30.client_id &#x3d; c.id&#x29;&#xd;&#xa;left outer join &#x28;select count&#x28;id&#x29; as app_cnt, max&#x28;application_date&#x3a;&#x3a;date&#x29; as last_app_date, client_id from lending.loan_application group by client_id&#x29;aa  on &#x28;aa.client_id &#x3d; c.id&#x29;&#xd;&#xa;&#xd;&#xa;left outer join &#x28;select count&#x28;id&#x29; as refused_to_reg, client_id from lending.loan_application where &#x28;close_reason is not null and close_reason ilike &#x27;&#x25;clientrefusedtoregister&#x25;&#x27;&#x29; group by client_id&#x29;crr  on &#x28;crr.client_id &#x3d; c.id&#x29;&#xd;&#xa;&#xd;&#xa;left outer join &#x28;select count&#x28;la.id&#x29; as invalid_province, la.client_id from lending.loan_application la where &#x28;la.close_reason is not null and close_reason ilike &#x27;&#x25;invalidprovince&#x25;&#x27;&#x29; group by la.client_id&#x29;cip  on &#x28;cip.client_id &#x3d; c.id&#x29;&#xd;&#xa;&#xd;&#xa;left outer join &#xd;&#xa;  &#x28;select client_id, min&#x28;created_at&#x3a;&#x3a;Date&#x29; as download_date, count&#x28;id&#x29; as devices from mobile.device group by 1&#x29;ma on &#x28;c.id&#x3d;ma.client_id&#x29;&#xd;&#xa;&#xd;&#xa;left outer join &#x28;select count&#x28;id&#x29; as income_too_low, client_id from lending.loan_application where &#x28;close_reason is not null and close_reason ilike &#x27;&#x25;incometoolow&#x25;&#x27;&#x29; group by client_id&#x29;cli  on &#x28;cli.client_id &#x3d; c.id&#x29;&#xd;&#xa;&#xd;&#xa;join crm.personal_info pi on &#x28;pi.client_id &#x3d; c.id&#x29;&#xd;&#xa;left outer join crm.phone_contact phcon on &#x28;phcon.client_id&#x3d;c.id and phcon.is_primary &#x3d; TRUE and phcon.phone_type &#x3d; 0&#x29;&#xd;&#xa;left outer join crm.email_contact ec on &#x28;ec.client_id&#x3d;c.id and ec.is_primary &#x3d; TRUE&#x29;&#xd;&#xa;&#xd;&#xa;left outer join &#x28;select promo_code, count&#x28;la.id&#x29; as app_count, count&#x28;l.id&#x29; as loan_count, count&#x28;ll.id&#x29; as paid_loan_count  from lending.loan_application la left outer join lending.loan l on &#x28;l.loan_application_id &#x3d; la.id&#x29; &#xd;&#xa;		left outer join lending.loan ll on &#x28;ll.loan_application_id &#x3d; la.id and ll.status&#x3d; &#x27;PAID&#x27;&#x29;  group by promo_code&#x29;promo on &#x28;promo.promo_code&#x3d;c.client_number&#x29;&#xd;&#xa;&#xd;&#xa;left outer join &#x28;select client_id, count&#x28;id&#x29; as valid_cnt from crm.client_attachment where attachment_type &#x3d; &#x27;salarySlip&#x27; and status &#x3d; &#x27;valid&#x27; group by 1&#x29;ss on &#x28;ss.client_id&#x3d; c.id&#x29;&#xd;&#xa;left outer join &#x28;select client_id, max&#x28;overdue_days&#x29; as max_overdue_days from lending.loan_daily_snapshot group by client_id&#x29;dsnht on &#x28;dsnht.client_id &#x3d; c.id&#x29; &#xd;&#xa;&#xd;&#xa;left outer join &#x28;&#xd;&#xa;&#xd;&#xa;SELECT client_id&#xd;&#xa; , json_value&#x3a;&#x3a;json-&#x3e;&#x3e;&#x27;employmentStatus&#x27; as employmentStatus&#xd;&#xa; , &#x28;json_value&#x3a;&#x3a;json-&#x3e;&#x3e;&#x27;contractExpiryDate&#x27;&#x29;&#x3a;&#x3a;date as contractExpiryDate&#xd;&#xa; , &#x28;json_value&#x3a;&#x3a;json-&#x3e;&#x3e;&#x27;employmentDurationInMonths&#x27;&#x29;&#x3a;&#x3a;smallint as employmentDurationInMonths&#xd;&#xa; , json_value&#x3a;&#x3a;json-&#x3e;&#x3e;&#x27;jobPosition&#x27; as jobPosition&#xd;&#xa; , &#x28;json_value&#x3a;&#x3a;json-&#x3e;&#x3e;&#x27;salary&#x27;&#x29;&#x3a;&#x3a;bigint as salary&#xd;&#xa; , &#x28;json_value&#x3a;&#x3a;json-&#x3e;&#x3e;&#x27;otherLiabilities&#x27;&#x29;&#x3a;&#x3a;bigint as otherLiabilities&#xd;&#xa;    &#xd;&#xa;      FROM crm.client_attribute &#xd;&#xa;    where &#xd;&#xa;	key &#x3d; &#x27;employmentData&#x27; &#xd;&#xa;&#x29;ce  on &#x28;ce.client_id &#x3d; c.id&#x29;&#xd;&#xa;&#xd;&#xa;left outer join &#x28;&#xd;&#xa;&#xd;&#xa;SELECT client_id&#xd;&#xa; , json_value&#x3a;&#x3a;json-&#x3e;&#x3e;&#x27;province&#x27; as province&#xd;&#xa;    &#xd;&#xa;      FROM crm.client_attribute &#xd;&#xa;    where &#xd;&#xa;	key &#x3d; &#x27;addressData&#x27; &#xd;&#xa;&#x29;cad  on &#x28;cad.client_id &#x3d; c.id&#x29;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;where 1&#x3d;1&#xd;&#xa;--and pi.receive_offers &#x3d; TRUE &#xd;&#xa;--and cal2.act_loan_cnt is null &#xd;&#xa;and c.id not in &#x28;25, 28, 3110, 3485, 3485, 46967,406088&#x29; --test clients and Gatis, invalid addresses&#xd;&#xa;order by c.id&#xd;&#xa;&#xd;&#xa;--limit 100</sql>
    <limit>0</limit>
