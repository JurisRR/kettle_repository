  <step>
    <name>get new loans</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>Georgia</connection>
    <sql>-- &#x23;&#x23; Extract data on loan level including credit info, client, application data&#xd;&#xa;&#xd;&#xa;--&#x23;&#x23; aggregation of payments_distr table on loan level&#xd;&#xa;with dd as &#x28;select d.r_loan_id,&#xd;&#xa;sum&#x28;d.amount&#x29; as amount, sum&#x28;d.amount_principal&#x29; as amount_principal, &#xd;&#xa;       sum&#x28;d.amount_commission&#x29; as amount_commission, sum&#x28;d.amount_penalty&#x29; as amount_penalty, sum&#x28;d.amount_surplus&#x29; as amount_surplus,&#xd;&#xa;        sum&#x28;d.amount_prolongation&#x29; as amount_prolongation, sum&#x28;d.amount_fees&#x29; as amount_fees&#xd;&#xa;	&#xd;&#xa;from &#xd;&#xa;r_payment_distr d join r_payment p on  &#x28;d.r_payment_id &#x3d; p.r_payment_id&#x29; &#xd;&#xa;where 1&#x3d;1 &#xd;&#xa;	and p.cl_bank_acct_id &#x3c;&#x3e; 18&#xd;&#xa;	--and p.cl_bank_acct_id &#x3c;&#x3e; 12&#xd;&#xa;	group by d.r_loan_id&#x29;&#xd;&#xa;&#xd;&#xa;,pay as &#x28;select d.r_loan_id, min&#x28;payment_date&#x29;&#x3a;&#x3a;date as first_payment_date , max&#x28;payment_date&#x29;&#x3a;&#x3a;date as last_payment_date&#xd;&#xa;	from r_payment_distr d join r_payment p on  &#x28;d.r_payment_id &#x3d; p.r_payment_id&#x29; &#xd;&#xa;		where p.type &#x3d; &#x27;IN_Loan&#x27; and p.cl_bank_acct_id &#x3c;&#x3e; 12 and cl_bank_acct_id &#x3c;&#x3e;  18&#xd;&#xa;		group by d.r_loan_id&#x29;&#xd;&#xa;&#xd;&#xa;,vpay as &#x28;select d.r_loan_id, min&#x28;payment_date&#x29;&#x3a;&#x3a;date as first_virtual_payment_date , max&#x28;payment_date&#x29;&#x3a;&#x3a;date as last_payment_date&#xd;&#xa;	from r_payment_distr d join r_payment p on  &#x28;d.r_payment_id &#x3d; p.r_payment_id&#x29; &#xd;&#xa;		where p.type &#x3d; &#x27;IN_Loan&#x27; and &#x28;p.cl_bank_acct_id &#x3d; 12 or cl_bank_acct_id &#x3d;  18&#x29;&#xd;&#xa;		group by d.r_loan_id&#x29;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;,out as &#x28;select d.r_loan_id, min&#x28;payment_date&#x29;&#x3a;&#x3a;date as out_loan_date &#xd;&#xa;	from r_payment_distr d join r_payment p on  &#x28;d.r_payment_id &#x3d; p.r_payment_id&#x29; &#xd;&#xa;		where p.type &#x3d; &#x27;OUT_Loan&#x27; and p.cl_bank_acct_id &#x3c;&#x3e; 12 and cl_bank_acct_id &#x3c;&#x3e;  18&#xd;&#xa;		group by d.r_loan_id&#x29;&#xd;&#xa;&#xd;&#xa;,freex as &#x28;select d.r_loan_id, wp.amount_prolongation , wp.days_prolongation, min&#x28;p.payment_date&#x29;&#x3a;&#x3a;date as free_extension_date&#xd;&#xa;	from r_payment_distr d join r_payment p on  &#x28;d.r_payment_id &#x3d; p.r_payment_id&#x29; &#xd;&#xa;		join r_withdrawal_prolongation_log wp on &#x28;d.r_payment_distr_id &#x3d; wp.r_payment_distr_id&#x29;&#xd;&#xa;		where p.type &#x3d; &#x27;IN_Loan&#x27; and p.cl_bank_acct_id &#x3d; 18&#xd;&#xa;		group by d.r_loan_id, wp.amount_prolongation , wp.days_prolongation&#x29;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;---&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;	&#xd;&#xa;&#xd;&#xa;SELECT &#xd;&#xa;&#xd;&#xa;l.r_loan_id&#xd;&#xa;,&#x27;New Cases&#x27; as report_type&#xd;&#xa;, current_date as report_date   &#xd;&#xa;, current_date as dc_start -- this shouldn&#x27;t be updated&#xd;&#xa;, null&#x3a;&#x3a;date as dc_end&#xd;&#xa;, &#x27;START DC&#x27; as status&#xd;&#xa;, l.loan_no&#xd;&#xa;, l.loan_date&#x3a;&#x3a;date&#xd;&#xa;, l.term_date&#xd;&#xa;, l.loan_amount&#xd;&#xa;, l.loan_commission&#xd;&#xa;, l.term_days&#xd;&#xa;, current_date - l.term_date as current_delay&#xd;&#xa;, l.amount_open_principal as open_principal&#xd;&#xa;, l.amount_open_commission as open_commission&#xd;&#xa;, l.amount_open_penalty as open_penalty&#xd;&#xa;, l.amount_open_principal &#x2b; l.amount_open_commission &#x2b; l.amount_open_penalty &#x2b; l.amount_open_fees as open_total&#xd;&#xa;--paid_after_external_dc_started -&#x3e; in the next step&#xd;&#xa;,c.first_name as client_name&#xd;&#xa;,c.last_name as client_surname&#xd;&#xa;,c.personal_no as id_no&#xd;&#xa;,c.mobile as client_phone_no&#xd;&#xa;,c.email as client_email&#xd;&#xa;,c.home_phone as client_homephone_no&#xd;&#xa;,c.notes&#xd;&#xa;,ad.in_one_string as client_actual&#xd;&#xa;,c.alternative_contact_first_name as contact_person_fname&#xd;&#xa;,c.alternative_contact_last_name as contact_person_lname&#xd;&#xa;,c.alternative_contact_mobile_phone as contact_person_phone&#xd;&#xa;,case when extract&#x28;week from current_date&#x29;&#x3a;&#x3a;integer &#x25; 2 &#x3d; 1 then &#x27;ProConsulting&#x27;&#xd;&#xa;	else &#x27;OperationalServices&#x27; end as external_partner&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;--&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;  FROM r_loan l   &#xd;&#xa;		join r_client c on &#x28;l.r_client_id &#x3d; c.r_client_id&#x29; &#xd;&#xa;		left outer join r_client_address ad on &#x28;c.act_client_address_id &#x3d; ad.r_client_address_id&#x29; &#xd;&#xa;		left outer join r_client_address ad1 on &#x28;c.decl_client_address_id &#x3d; ad.r_client_address_id&#x29; &#xd;&#xa;	left outer join dd dd on &#x28;dd.r_loan_id &#x3d; l.r_loan_id&#x29; left outer join pay pay on &#x28;pay.r_loan_id &#x3d; l.r_loan_id&#x29;&#xd;&#xa;&#xd;&#xa;	&#xd;&#xa;	join out out on &#x28;l.r_loan_id &#x3d; out.r_loan_id&#x29; &#xd;&#xa;	left outer join vpay v on &#x28;l.r_loan_id &#x3d; v.r_loan_id&#x29;&#xd;&#xa;	left outer join freex x on &#x28;l.r_loan_id &#x3d; x.r_loan_id&#x29;&#xd;&#xa;	&#xd;&#xa;	&#xd;&#xa;where 1&#x3d;1&#xd;&#xa;--and l.r_loan_id &#x3c;&#x3e; 12042 --&#x23;&#x23;&#x23; test loan&#xd;&#xa;--and l.r_client_id not in &#x28;39607, 39648&#x29;  --&#x23;&#x23;&#x23; test clients&#x3a; BI and Nino&#xd;&#xa;and l.status &#x3d; &#x27;Active &#x28;delay&#x29;&#x27; -- for new loans&#xd;&#xa;and  &#x28;l.term_date &#x2b; 60&#x29;  between current_date-7 and current_date-1&#xd;&#xa;&#xd;&#xa; &#xd;&#xa;  &#xd;&#xa;-- between 6 and 19 december reached 60 days&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;</sql>
    <limit>0</limit>
    <lookup/>
    <execute_each_row>N</execute_each_row>
    <variables_active>N</variables_active>
    <lazy_conversion_active>N</lazy_conversion_active>
