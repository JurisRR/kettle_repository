  <step>
    <name>add payments</name>
    <type>DBJoin</type>
    <description/>
    <distribute>N</distribute>
    <custom_distribution/>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>Georgia</connection>
    <rowlimit>0</rowlimit>
    <sql>select d.r_loan_id,&#xd;&#xa;sum&#x28;d.amount&#x29; as paid_after_external_dc_started&#xd;&#xa;	&#xd;&#xa;from &#xd;&#xa;r_payment_distr d join r_payment p on  &#x28;d.r_payment_id &#x3d; p.r_payment_id&#x29; &#xd;&#xa;where 1&#x3d;1 &#xd;&#xa;	--and p.cl_bank_acct_id &#x3c;&#x3e; 18&#xd;&#xa;	and p.type &#x3d; &#x27;IN_Loan&#x27;&#xd;&#xa;	--and p.cl_bank_acct_id &#x3c;&#x3e; 12&#xd;&#xa;	&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;and d.r_loan_id &#x3d;&#x3f;&#xd;&#xa;and p.payment_date&#x3a;&#x3a;date &#x3e; &#x3f;&#xd;&#xa;--and p.payment_date&#x3a;&#x3a;date &#x3c; &#x3f;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;group by d.r_loan_id</sql>
    <outer_join>Y</outer_join>
    <replace_vars>N</replace_vars>
    <parameter>
      <field>
        <name>r_loan_id</name>
        <type>None</type>
      </field>
      <field>
        <name>report_date</name>
        <type>Date</type>
      </field>
    </parameter>
