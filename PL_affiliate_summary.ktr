    <connection>PL_Slave_Finotech</connection>
    <sql>select affiliate, extract&#x28;year from first_issued_loan_date&#x29; as year, extract&#x28;month from first_issued_loan_date&#x29; as month, count&#x28;r_client_id&#x29; from &#x28;SELECT&#xd;&#xa;&#xd;&#xa;distinct c.id as r_client_id&#xd;&#xa;,e.client_number&#xd;&#xa;,1 as status&#xd;&#xa;,c.created_at&#x3a;&#x3a;date as reg_date&#xd;&#xa;,&#x27;&#x27; as web_tracking_code&#xd;&#xa;,e.params_json&#x3a;&#x3a;json-&#x3e;&#x3e;&#x27;aff_sub&#x27; as web_tracking_code_aff44&#xd;&#xa;,e.params_json&#x3a;&#x3a;json-&#x3e;&#x3e;&#x27;transaction_id&#x27; as web_tracking_code_credy&#xd;&#xa;,doaff.doaff_visitor&#xd;&#xa;,ilm.issued_last_month&#xd;&#xa;,l.first_issued_loan_date&#xd;&#xa;,lp.name as Affiliate&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;from lending.loan_application a &#xd;&#xa;&#xd;&#xa;join &#x28;select min&#x28;issue_date&#x3a;&#x3a;Date&#x29; as first_issued_loan_date, client_id from&#xd;&#xa; lending.loan group by client_id&#x29;l on &#x28;a.client_id &#x3d; l.client_id&#x29;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;join &#x28;select id, rank&#x28;&#x29; over &#x28;partition by client_id &#xd;&#xa; order by client_id ASC, id ASC &#x29; as seq_app_total  from lending.loan_application&#x29;aa &#xd;&#xa;  on &#x28;a.id &#x3d; aa.id&#x29;&#xd;&#xa;&#xd;&#xa;left outer join &#x28;select client_id, loan_application_id, rank&#x28;&#x29; over &#x28;partition by client_id &#xd;&#xa; order by client_id ASC, id ASC &#x29; as seq_loan_total  from lending.loan&#x29;ll &#xd;&#xa;  on &#x28;a.id &#x3d; ll.loan_application_id&#x29;&#xd;&#xa;&#xd;&#xa; left outer join &#x28;select client_id, count&#x28;id&#x29; as issued_last_month from lending.loan&#xd;&#xa;where issue_date&#x3a;&#x3a;date  &#x3e;&#x3d; date_trunc&#x28;&#x27;month&#x27;, current_date - interval &#x27;1&#x27; month&#x29;&#xd;&#xa;  and issue_date&#x3a;&#x3a;date &#x3c; date_trunc&#x28;&#x27;month&#x27;, current_date&#x29;&#xd;&#xa;  group by client_id&#x29;ilm on &#x28;ilm.client_id &#x3d; a.client_id&#x29;&#xd;&#xa;&#xd;&#xa;join marketing_landing.event e on &#x28;e.client_id &#x3d; a.client_id&#x29; join marketing_landing.landing_page lp on &#x28;e.landing_page_id &#x3d; lp.id&#x29;&#xd;&#xa;join crm.client c on &#x28;a.client_id &#x3d; c.id&#x29;&#xd;&#xa;left outer join &#x28;select distinct ve.cookies_json&#x3a;&#x3a;json-&#x3e;&#x3e;&#x27;doaff_visitor&#x27; as doaff_visitor, v.client_id &#xd;&#xa;	from  marketing_tracking.visitor v join marketing_tracking.event ve on &#x28;v.id &#x3d; ve.visitor_id and ve.name &#x3d; &#x27;signup&#x27;&#x29;&#x29;doaff on &#x28;doaff.client_id &#x3d; c.id&#x29; &#xd;&#xa;&#xd;&#xa;where 1&#x3d;1&#xd;&#xa;and seq_app_total &#x3d; 1&#xd;&#xa;and l.first_issued_loan_date&#x3a;&#x3a;date  &#x3e;&#x3d; date_trunc&#x28;&#x27;month&#x27;, current_date - interval &#x27;1&#x27; month&#x29;&#xd;&#xa;  and l.first_issued_loan_date&#x3a;&#x3a;date &#x3c; date_trunc&#x28;&#x27;month&#x27;, current_date&#x29;&#xd;&#xa;&#xd;&#xa;and &#x28;l.first_issued_loan_date&#x3a;&#x3a;date-c.created_at&#x3a;&#x3a;date &#x29; &#x3c; 31&#x29;aa&#xd;&#xa;&#xd;&#xa;group by 1,2,3&#xd;&#xa;order by 2 desc,3 desc,1</sql>
