    <sql>--SELECT application_id FROM application_main WHERE application_id IN&#xa; -- &#x28;SELECT application_id FROM application_main GROUP BY application_id HAVING COUNT&#x28;&#x2a;&#x29; &#x3e; 1&#x29;&#xa;&#xa;  --select table_name from information_schema.columns where column_name &#x3d; &#x27;cms_landing_page_id&#x27;&#xa;  &#xa;&#xa;with app_ranks as &#x28;SELECT a.id, seq_application_total, seq_application_confirm as confirmed_rank, seq_application_reject as rejected_rank&#xa;,case when seq_application_reject &#x3e; 0 then seq_application_reject else seq_application_total- seq_application_confirm end as seq_application_reject&#xa;,case when seq_application_confirm &#x3e; 0 then seq_application_confirm else seq_application_total- seq_application_reject end as seq_application_confirm&#xa;,seq_application_per_loan&#xa;  FROM lending.loan_application a join &#x28;select id, rank&#x28;&#x29; over &#x28;partition by client_id &#xa;  order by client_id ASC, id ASC &#x29; as seq_application_total  from lending.loan_application &#x29;aa &#xa;  on a.id &#x3d; aa.id&#xa;&#xa;  left outer join &#x28;select id, rank&#x28;&#x29; over &#x28;partition by client_id &#xa;  order by client_id ASC, id ASC &#x29; as seq_application_confirm from lending.loan_application&#xa;  where status &#x3d; &#x27;APPROVED&#x27;&#x29;ac &#xa;  on a.id &#x3d; ac.id&#xa;&#xa;  left outer join &#x28;select id, rank&#x28;&#x29; over &#x28;partition by client_id &#xa;  order by client_id ASC, id ASC &#x29; as seq_application_reject from lending.loan_application &#xa;  where status &#x3d; &#x27;REJECTED&#x27;&#x29;ar &#xa;  on a.id &#x3d; ar.id&#xa;&#xa;  left outer join &#x28;select id, rank&#x28;&#x29; over &#x28;partition by client_id, loans_paid&#xa;  order by client_id ASC, loans_paid ASC, id ASC &#x29; as seq_application_per_loan from lending.loan_application &#xa;  &#x29;aspl &#xa;  on &#x28;a.id &#x3d; aspl.id&#x29;&#xa;  &#xa;  order by id, a.id&#x29; &#xa;&#xa;&#xa;&#xa;SELECT &#xa;distinct &#xa;la.id as application_id&#xa;,case when la.application_date&#x3a;&#x3a;date &#x3c; &#x27;2017-06-10&#x27; then la.application_date else la.created_at end as application_datetime &#xa;,extract&#x28;week from la.application_date&#x29; as app_iso_week&#xa;, la.client_id&#xa;, la.requested_principal as application_amount&#xa;, la.offered_principal as offered_amount&#xa;, la.status&#xa;, la.close_reason&#xa;, ss.scoring as score&#xa;, sss.scoring as score_alternative&#xa;, case &#xa;when ssss.split_run_selected_result &#x3d; TRUE then &#x27;alternative&#x27; &#xa;when ssss.split_run_selected_result&#x3d; false then &#x27;main_score&#x27;&#xd;&#xa;when ssss.evaluation_time is null then &#x27;main_score&#x27; end as score_type -- NEW SCORECARDS&#xa;, inc.income as income1&#xa;, income_app_atr&#xa;, la.loans_paid as finished_loan_cnt&#xa;, la.loan_id&#xa;, l.status as loan_status&#xa;, la.offered_period_count&#xa;,la.requested_period_count&#xa;, l.principal_disbursed&#xa;, l.interest_applied&#xa;, case when l.extensions&#x3e; 0 then 1 else 0 end as prolongated&#xa;--LANDING PAGES&#xa;, case when cms_landing_page_id is not null then cms_landing_page_id else first_landing_new.LP_ID end as first_landing_page&#xa;,lp.id as landing_page&#xa;, case when pi.gender &#x3d; &#x27;MALE&#x27; then 1 else 2 end as gender&#xa;, extract&#x28; years from age&#x28;la.application_date, pi.date_of_birth&#x29;&#x29; as age&#xa;, case when la.loans_paid &#x3d; 0 then 0 else 1 end as app_type&#xa;&#xa;, seq_application_total&#xa;, seq_application_confirm&#xa;, seq_application_reject&#xa;, rejected_rank&#xa;, confirmed_rank&#xa;, seq_application_per_loan&#xa;&#xa;, l.issue_date&#x3a;&#x3a;date as loan_date&#xa;&#xa;, current_timestamp as extraction_datetime&#xa;&#xa;&#xa;, mt.browser&#xa;, substring&#x28;split_part&#x28;mt.geo_location, &#x27;,&#x27;, 1&#x29;,8,100&#x29; as application_latitude&#xa;, trim&#x28;both &#x27;&#x7d;&#x27; from substring&#x28;split_part&#x28;mt.geo_location, &#x27;,&#x27;, 2&#x29;,8,100&#x29;&#x29; as application_longtitude&#xa;&#xa;&#xa;, mt.os&#xa;, mt.device&#xa;&#xa;, &#x28;SELECT prev_dpd from &#x28;SELECT  COALESCE&#x28;&#x28;SELECT &#x28;close_date&#x3a;&#x3a;date - maturity_date&#x3a;&#x3a;Date&#x29;&#xa;       FROM lending.loan ll&#xa;       WHERE ll.client_id&#x3d;la.client_id and ll.issue_date&#x3a;&#x3a;Date&#x3c;la.application_date&#x3a;&#x3a;date&#xa;       ORDER BY created_at DESC&#xa;       LIMIT 1&#x29;, 0&#x29;as prev_dpd&#x29;pdpd&#x29; as prev_dpd&#xa;&#xa;, &#x28;SELECT prev_loan from &#x28;SELECT  COALESCE&#x28;&#x28;SELECT principal_disbursed&#xa;       FROM lending.loan ll&#xa;       WHERE ll.client_id&#x3d;la.client_id and ll.issue_date&#x3a;&#x3a;Date&#x3c;la.application_date&#x3a;&#x3a;date&#xa;       ORDER BY created_at DESC&#xa;       LIMIT 1&#x29;, 0&#x29;as prev_loan&#x29;pdpd&#x29; as prev_loan&#xa;&#xa;&#xa;, case when fp.amount_paid &#x3e;&#x3d; 0.01 then 1 else 0 end as id_payment&#xa;&#xa;,case when fp.created_at&#x3a;&#x3a;date &#x3e; &#x27;2017-06-09&#x27; then fp.created_at else fp.post_date end as identification_datetime&#xa;&#xa;&#xa;&#xa;&#xa;&#xa;--&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23; end of selected data &#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#xa; from lending.loan_application la &#xa;join lending.loan_application lla on &#x28;la.id &#x3d; lla.id&#x29;&#xa;&#xa;&#xa;left outer join &#xa;&#xa;&#x28;select &#xa;&#xa;	workflow_id, number_value as app_id from workflow.workflow_param &#xa;	where name &#x3d; &#x27;loanApplicationId&#x27;&#x29;app on &#x28;app.app_id &#x3d; la.id&#x29;&#xa;	&#xa;left outer join &#xa;&#xa;&#x28;select &#xa;&#xa;	workflow_id, text_value as resolution from workflow.workflow_param &#xa;	where name &#x3d; &#x27;Resolution&#x27;&#x29;app2 on &#x28;app.workflow_id &#x3d; app2.workflow_id&#x29;&#xa;&#xa;left outer join crm.personal_info pi on &#x28;la.client_id &#x3d; pi.client_id&#x29;&#xa;left outer join crm.email_login el on &#x28;la.client_id &#x3d; el.client_id&#x29;&#xa;&#xa;left outer join &#x28;SELECT &#xa;&#xa;	&#x2a;&#xa;&#xa;  FROM &#xa;	&#x28;SELECT v.client_id, e.utm_source, e.utm_campaign, e.referrer_domain, e.created_at&#x3a;&#x3a;Date as event_date,&#xa;	e.browser, e.geo_location, e.os, e.device, e.screen_size, la.id&#xa;	,rank&#x28;&#x29; over &#x28;partition by la.id order by e.created_at&#x29; as rrow &#xa;	from marketing_tracking.event e&#xa;&#xa;  join marketing_tracking.visitor v on &#x28;v.id &#x3d; e.visitor_id&#x29; join &#xa;  --&#x28;select min&#x28;e.id&#x29; as first_event_id, v.client_id from marketing_tracking.event e join marketing_tracking.visitor v on &#x28;v.id &#x3d; e.visitor_id&#x29;  group by v.client_id&#x29; fe on &#x28;v.client_id &#x3d; fe.client_id&#x29;&#xa;&#xa;	lending.loan_application la on &#x28;la.client_id &#x3d; v.client_id and e.created_at&#x3a;&#x3a;Date &#x3d; la.application_date&#x3a;&#x3a;Date&#x29;&#x29;aa&#xa;&#xa;  where rrow&#x3d;1&#x29;mt on &#x28;mt.id &#x3d; la.id&#x29;&#xa;&#xa;&#xa;left outer join &#x28;select distinct application_id, name as traffic_source, lp.id from marketing_landing.event e join marketing_landing.landing_page lp on &#x28;e.landing_page_id&#x3d;lp.id&#x29;&#x29;lp on la.id &#x3d; lp.application_id&#xa;&#xa;join app_ranks apr on &#x28;apr.id &#x3d; la.id&#x29;&#xa;&#xa;left outer join lending.loan l on &#x28;l.loan_application_id &#x3d; la.id&#x29;	&#xa;&#xa;--new scoring begins&#xa;&#xa;left outer join &#x28;select &#x2a; from &#x28;select distinct loan_application_id, scoring, e.id as score_id, split_run_selected_result, rank&#x28;&#x29; over &#x28;partition by loan_application_id  order by e.id DESC &#x29; as seq_score&#xa;from scoring.scorecard_evaluation e&#xa;       join scoring.scorecard s on &#x28;e.scorecard_id &#x3d;  s.id and s.name in &#x28;&#x27;CREDIT_BUREAU_SCORECARD_0&#x27;,&#x27;CREDIT_BUREAU_SCORECARD_1&#x27;,&#x27;CREDIT_BUREAU_SCORECARD_2&#x27;,&#x27;CREDIT_BUREAU_SCORECARD_3&#x2b;&#x27;&#x29;&#x29;&#xa;&#x29;aa where seq_score&#x3d;1&#x29;ss&#xa;       on ss.loan_application_id&#x3d;la.id&#xd;&#xa; &#xa;left outer join &#x28;select &#x2a; from &#x28;select distinct loan_application_id, scoring, e.id as score_id, split_run_selected_result, rank&#x28;&#x29; over &#x28;partition by loan_application_id  order by e.id DESC &#x29; as seq_score&#xa;from scoring.scorecard_evaluation e&#xa;       join scoring.scorecard s on &#x28;e.scorecard_id &#x3d;  s.id and s.name in &#x28;&#x27;CREDIT_BUREAU_SCORECARD_0a&#x27;,&#x27;CREDIT_BUREAU_SCORECARD_1a&#x27;,&#x27;CREDIT_BUREAU_SCORECARD_2a&#x27;,&#x27;CREDIT_BUREAU_SCORECARD_3&#x2b;a&#x27;&#x29;&#x29;&#x29;aa where seq_score&#x3d;1&#x29;sss&#xa;       on sss.loan_application_id&#x3d;la.id&#xd;&#xa;&#xd;&#xa;left outer join &#x28;select &#x2a; from &#x28;select distinct loan_application_id, scoring, e.id as score_id, split_run_selected_result, e.evaluation_time, rank&#x28;&#x29; over &#x28;partition by loan_application_id  order by e.id ASC &#x29; as seq_score&#xd;&#xa;from scoring.scorecard_evaluation e&#xd;&#xa;       join scoring.scorecard s on &#x28;e.scorecard_id &#x3d;  s.id and s.name in &#x28;&#x27;CREDIT_BUREAU_SCORECARD_0a&#x27;,&#x27;CREDIT_BUREAU_SCORECARD_1a&#x27;,&#x27;CREDIT_BUREAU_SCORECARD_2a&#x27;,&#x27;CREDIT_BUREAU_SCORECARD_3&#x2b;a&#x27;&#x29;&#x29;&#x29;aa where seq_score&#x3d;1&#x29;ssss&#xd;&#xa;       on ssss.loan_application_id&#x3d;la.id&#xa;&#xa;--new scoring end&#xa;&#xa;--INCOME JOINS&#xa;left join &#x28;select application_id as id, value as income_app_atr from lending.loan_application_attribute where key&#x3d;&#x27;monthlyIncome&#x27;&#x29;lapinc&#xa;on &#x28;lapinc.id&#x3d;la.id&#x29;&#xa;&#xa;left outer join &#x28;SELECT distinct client_id,  max&#x28;json_value&#x3a;&#x3a;json -&#x3e;&#x3e;&#x27;monthlyIncome&#x27; &#x29;as income&#xa;  FROM crm.client_attribute group by 1 &#x29;inc on &#x28;la.client_id &#x3d; inc.client_id&#x29;&#xa;&#xa;&#xa;--INCOME END&#xa;left outer join &#x28;select  client_id, max&#x28;fee_paid&#x29; as amount_paid, max&#x28;created_at&#x29; as created_at,  max&#x28;post_date&#x29; as post_date  &#xa;from  &#xa;transaction.transaction t &#xa;	where 1&#x3d;1 and transaction_type &#x3d;  &#x27;FEE_PAYMENT&#x27; &#xa;	and t.transaction_sub_type &#x3d; &#x27;REGISTRATION_FEE&#x27;&#xa;	group by client_id &#x29;fp&#xa;	on &#x28;fp.client_id &#x3d; la.client_id&#x29;&#xa;&#xa;&#xa; left join migration_source.r_client migration_client on la.client_id&#x3d;migration_client.r_client_id&#xa;&#xa; --needs further check, created duplicates&#xa;left join &#x28;select fln.id as id, fln.min_id,fln1.lp_id as lp_id from &#xa;&#xa;&#x28;&#x28;select la.client_id as id, min&#x28;ma_ev.id&#x29;as min_id from lending.loan_application la&#xa;&#xa;left join marketing_landing.event ma_ev on la.id&#x3d;ma_ev.application_id group by la.client_id&#x29;fln &#xa;&#xa;left join &#x28;select  la.id as id, ma_ev.id as all_id, ma_ev.landing_page_id as lp_id from lending.loan_application la&#xa;left join marketing_landing.event ma_ev on la.id&#x3d;ma_ev.application_id&#x29;fln1&#xa; on fln.min_id&#x3d;fln1.all_id&#x29;&#xa; &#x29; first_landing_new on first_landing_new.id&#x3d;la.client_id&#xa;&#xa;</sql>
