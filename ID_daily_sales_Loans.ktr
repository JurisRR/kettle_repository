    <sql>select &#xd;&#xa;l.issue_date&#x3a;&#x3a;Date,&#xd;&#xa;tr.agent,&#xd;&#xa;l.id as loan&#xd;&#xa;,l.loan_number&#xd;&#xa;,case when now&#x28;&#x29; - l.issue_date &#x3e; interval &#x27;24 hours&#x27; then &#x27;before&#x27; else &#x27;last 24 hours&#x27; end as last_24hours&#xd;&#xa;,case when now&#x28;&#x29; - l.issue_date &#x3e; interval &#x27;7 days&#x27; then &#x27;before&#x27; else &#x27;last 7 days&#x27; end as last_7days&#xd;&#xa;,case when now&#x28;&#x29; - l.issue_date &#x3e; interval &#x27;30 days&#x27; then &#x27;before&#x27; else &#x27;last 30 days&#x27; end as last_30days&#xd;&#xa;,case when l.loans_paid &#x3d; 0 then &#x27;first&#x27; else &#x27;repeated&#x27; end as loan_type&#xd;&#xa;&#xd;&#xa;from lending.loan l&#xd;&#xa;&#xd;&#xa;left outer join &#x28;select &#x2a; from &#x28;select application_id, agent, rank&#x28;&#x29; over &#x28;partition by application_id &#xd;&#xa;  order by created_at DESC&#x29; as task_rank  from task.task where dc_debt_case_id is null and task_type &#x3c;&#x3e; &#x27;ExtensionCall&#x27;--task_group ilike &#x27;&#x25;Underwriting&#x25;&#x27;&#xd;&#xa;&#x29;tt where task_rank&#x3d;1&#x29;tr&#xd;&#xa;  on &#x28;tr.application_id &#x3d; l.loan_application_id&#x29; &#xd;&#xa;  --where l.issue_date &#x3d; &#x7b;&#x7b;l.issue_date&#x7d;&#x7d;&#xd;&#xa; --group by l.issue_date, tr.agent&#xd;&#xa;&#xd;&#xa;where l.status &#x21;&#x3d; &#x27;VOIDED&#x27;&#xd;&#xa; &#xd;&#xa; order by l.issue_date desc&#xd;&#xa;</sql>
