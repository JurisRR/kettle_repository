    <connection>Poland</connection>
    <rowlimit>0</rowlimit>
    <sql>-- &#x23;&#x23; Extract data on loan level including credit info, client, application data&#xd;&#xa;&#xd;&#xa;--&#x23;&#x23; aggregation of payments_distr table on loan level&#xd;&#xa;with dd as &#x28;select d.r_loan_id,&#xd;&#xa;sum&#x28;d.amount&#x29; as amount, sum&#x28;d.amount_principal&#x29; as amount_principal, &#xd;&#xa;       sum&#x28;d.amount_commission&#x29; as amount_commission, sum&#x28;d.amount_penalty&#x29; as amount_penalty, sum&#x28;d.amount_surplus&#x29; as amount_surplus,&#xd;&#xa;        sum&#x28;d.amount_prolongation&#x29; as amount_prolongation, sum&#x28;d.amount_fees&#x29; as amount_fees&#xd;&#xa;	&#xd;&#xa;from &#xd;&#xa;r_payment_distr d join r_payment p on  &#x28;d.r_payment_id &#x3d; p.r_payment_id&#x29; &#xd;&#xa;where 1&#x3d;1 &#xd;&#xa;	and p.cl_bank_acct_id &#x3c;&#x3e; 11&#xd;&#xa;group by d.r_loan_id&#x29;&#xd;&#xa;&#xd;&#xa;,pay as &#x28;select d.r_loan_id, min&#x28;payment_date&#x29;&#x3a;&#x3a;date as first_payment_date , max&#x28;payment_date&#x29;&#x3a;&#x3a;date as last_payment_date&#xd;&#xa;	,sum&#x28;d.amount&#x29; as paid_so_far&#xd;&#xa;	from r_payment_distr d join r_payment p on  &#x28;d.r_payment_id &#x3d; p.r_payment_id&#x29; &#xd;&#xa;		where p.type &#x3d; &#x27;IN_Loan&#x27; and p.cl_bank_acct_id &#x3c;&#x3e; 11 &#xd;&#xa;		group by d.r_loan_id&#x29;&#xd;&#xa;&#xd;&#xa;,pay_dc as &#x28;select d.r_loan_id, min&#x28;payment_date&#x29;&#x3a;&#x3a;date as first_payment_date , max&#x28;payment_date&#x29;&#x3a;&#x3a;date as last_payment_date&#xd;&#xa;	,sum&#x28;d.amount&#x29; as paid_so_far&#xd;&#xa;	from r_payment_distr d join r_payment p on  &#x28;d.r_payment_id &#x3d; p.r_payment_id&#x29; &#xd;&#xa;		where p.type &#x3d; &#x27;IN_Loan&#x27; and p.cl_bank_acct_id &#x3c;&#x3e; 11 and p.payment_date &#x3e; current_date&#xd;&#xa;		group by d.r_loan_id&#x29;&#xd;&#xa;&#xd;&#xa;,vpay as &#x28;select d.r_loan_id, min&#x28;payment_date&#x29;&#x3a;&#x3a;date as first_virtual_payment_date , max&#x28;payment_date&#x29;&#x3a;&#x3a;date as last_payment_date&#xd;&#xa;	from r_payment_distr d join r_payment p on  &#x28;d.r_payment_id &#x3d; p.r_payment_id&#x29; &#xd;&#xa;		where p.type &#x3d; &#x27;IN_Loan&#x27; and &#x28;p.cl_bank_acct_id &#x3d; 11 or cl_bank_acct_id &#x3d;  9&#x29;&#xd;&#xa;		group by d.r_loan_id&#x29;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;,out as &#x28;select d.r_loan_id, min&#x28;payment_date&#x29;&#x3a;&#x3a;date as out_loan_date &#xd;&#xa;	from r_payment_distr d join r_payment p on  &#x28;d.r_payment_id &#x3d; p.r_payment_id&#x29; &#xd;&#xa;		where p.type &#x3d; &#x27;OUT_Loan&#x27; and p.cl_bank_acct_id &#x3c;&#x3e; 11 and cl_bank_acct_id &#x3c;&#x3e;  9&#xd;&#xa;		group by d.r_loan_id&#x29;&#xd;&#xa;&#xd;&#xa;,freex as &#x28;select d.r_loan_id, wp.amount_prolongation , wp.days_prolongation, min&#x28;p.payment_date&#x29;&#x3a;&#x3a;date as free_extension_date&#xd;&#xa;	from r_payment_distr d join r_payment p on  &#x28;d.r_payment_id &#x3d; p.r_payment_id&#x29; &#xd;&#xa;		join r_withdrawal_prolongation_log wp on &#x28;d.r_payment_distr_id &#x3d; wp.r_payment_distr_id&#x29;&#xd;&#xa;		where p.type &#x3d; &#x27;IN_Loan&#x27; and p.cl_bank_acct_id &#x3d; 11&#xd;&#xa;		group by d.r_loan_id, wp.amount_prolongation , wp.days_prolongation&#x29;&#xd;&#xa;&#xd;&#xa;,address as &#x28;select &#x2a; from &#xd;&#xa;		&#x28;select r_client_id, type, region, city, in_one_string,&#xd;&#xa;	rank&#x28;&#x29; over &#x28;partition by r_client_id, type order by r_client_id asc, type asc, r_client_address_id desc&#x29;as rrow&#xd;&#xa;	from r_client_address &#x29;kk&#xd;&#xa;	where rrow &#x3d; 1&#x29;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;---&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;	&#xd;&#xa;&#xd;&#xa;SELECT &#xd;&#xa;&#xd;&#xa;l.r_loan_id&#xd;&#xa;--,&#x27;New Cases&#x27; as report_type&#xd;&#xa;, current_date as report_date   &#xd;&#xa;, current_date as dc_start -- this shouldn&#x27;t be updated&#xd;&#xa;, null&#x3a;&#x3a;date as dc_end&#xd;&#xa;&#xd;&#xa;-- need to add status correctly&#xd;&#xa;, case when  &#x28;l.amount_open_principal&#x2b;l.amount_open_commission&#x2b;l.amount_open_fees&#x2b;l.amount_open_penalty&#x29; &#x3c; 10  -- don&#x27;t give small amounts to Lindorff&#xd;&#xa;	--or  &#x28;current_date - &#x3f; &#x29;  &#x3e; 100 &#xd;&#xa;	then &#x27;STOP DC&#x27; &#xd;&#xa;	else  &#x27;START DC&#x27;  end as status_new&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;, l.loan_no&#xd;&#xa;, l.loan_date&#x3a;&#x3a;date&#xd;&#xa;--, l.term_date&#xd;&#xa;, l.loan_amount&#xd;&#xa;, l.loan_commission&#xd;&#xa;, l.term_days&#xd;&#xa;, current_date - l.term_date as current_delay&#xd;&#xa;, l.amount_open_principal as open_principal&#xd;&#xa;, l.amount_open_commission as open_commission&#xd;&#xa;, l.amount_open_fees as open_fees&#xd;&#xa;, l.amount_open_penalty as open_penalty&#xd;&#xa;&#xd;&#xa;, l.amount_total_fees as standart_costs  -- &#x28;CASE WHEN substring&#x28;lo.loan_no from &#x27;..&#x24;&#x27;&#x29;&#x3a;&#x3a;numeric&#x3d;1 THEN coalesce&#x28;&#x28;select sum&#x28;lf.amount&#x29; &#xd;&#xa;							--from r_loan_fee lf where lf.r_loan_id&#x3d;lo.r_loan_id and lf.type&#x3d;&#x27;Standard commission&#x27;&#x29;&#x3a;&#x3a;decimal,0&#x29; ELSE 0 END&#x29; Standart_Costs&#xd;&#xa;&#xd;&#xa;--compare the fees&#x3f;&#xd;&#xa;&#xd;&#xa;,l.prolongated as taken_extensions&#xd;&#xa;,pay.paid_so_far as paid_so_far_new&#xd;&#xa;&#xd;&#xa;, l.amount_open_principal &#x2b; l.amount_open_commission &#x2b; l.amount_open_penalty &#x2b; l.amount_open_fees as open_total&#xd;&#xa;&#xd;&#xa;,pay_dc.paid_so_far as  paid_after_external_dc_started&#xd;&#xa;,tt.last_task_comment&#xd;&#xa;,tt.last_task_status&#xd;&#xa;&#xd;&#xa;,c.first_name as client_name&#xd;&#xa;,c.last_name as client_surname&#xd;&#xa;,c.personal_no as pesel&#xd;&#xa;,c.document_no as id_doc_no&#xd;&#xa;,c.mobile as client_phone_no&#xd;&#xa;,c.email as client_email&#xd;&#xa;--,c.home_phone as client_homephone_no&#xd;&#xa;--,c.notes&#xd;&#xa;&#xd;&#xa;,ad1.in_one_string as client_declared&#xd;&#xa;,ad.in_one_string as client_actual&#xd;&#xa;,c.workplace_title as client_workplace&#xd;&#xa;,c.workplace_address as client_work_address&#xd;&#xa;,c.workplace_phone as client_work_phone&#xd;&#xa;,c.contact_person as contact_person_name&#xd;&#xa;,c.contact_person_phone as contact_person_phone&#xd;&#xa;,c.bank_account as person_bankaccount_no&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;--&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;  FROM r_loan l   &#xd;&#xa;		join r_client c on &#x28;l.r_client_id &#x3d; c.r_client_id&#x29; &#xd;&#xa;		--left outer join r_client_address ad on &#x28;c.act_client_address_id &#x3d; ad.r_client_address_id&#x29; &#xd;&#xa;		--left outer join r_client_address ad1 on &#x28;c.decl_client_address_id &#x3d; ad.r_client_address_id&#x29; &#xd;&#xa;	left outer join dd dd on &#x28;dd.r_loan_id &#x3d; l.r_loan_id&#x29; &#xd;&#xa;	left outer join pay pay on &#x28;pay.r_loan_id &#x3d; l.r_loan_id&#x29;&#xd;&#xa;	left outer join pay_dc pay_dc on &#x28;pay_dc.r_loan_id &#x3d; l.r_loan_id&#x29;&#xd;&#xa;	&#xd;&#xa;	join out out on &#x28;l.r_loan_id &#x3d; out.r_loan_id&#x29; &#xd;&#xa;	left outer join vpay v on &#x28;l.r_loan_id &#x3d; v.r_loan_id&#x29;&#xd;&#xa;	left outer join freex x on &#x28;l.r_loan_id &#x3d; x.r_loan_id&#x29;&#xd;&#xa;	left outer join &#x28;select ct.r_loan_id, ct.comments as last_task_comment, ts.name as last_task_status  from &#x28;SELECT r_loan_id, max&#x28;crm_task_id&#x29; as max_id&#xd;&#xa; 					 FROM crm_task&#xd;&#xa;						group by r_loan_id&#x29;aa join crm_task ct on &#x28; ct.crm_task_id&#x3d;aa.max_id&#x29; &#xd;&#xa;						join crm_task_status ts on &#x28;ct.crm_task_status_id&#x3d;ts.crm_task_status_id&#x29;&#x29;tt on &#x28;l.r_loan_id&#x3d;tt.r_loan_id&#x29;&#xd;&#xa;	left outer join address ad on &#x28;c.r_client_id &#x3d; ad.r_client_id and ad.type &#x3d; &#x27;Actual&#x27;&#x29; &#xd;&#xa;	left outer join address ad1 on &#x28;c.r_client_id &#x3d; ad1.r_client_id and ad1.type &#x3d; &#x27;Declared&#x27;&#x29;&#xd;&#xa;	&#xd;&#xa;&#xd;&#xa;	&#xd;&#xa;	&#xd;&#xa;where 1&#x3d;1&#xd;&#xa;--and l.r_loan_id &#x3c;&#x3e; 12042 --&#x23;&#x23;&#x23; test loan&#xd;&#xa;--and l.r_client_id not in &#x28;39607, 39648&#x29;  --&#x23;&#x23;&#x23; test clients&#x3a; BI and Nino&#xd;&#xa;and l.status &#x3d; &#x27;Active &#x28;delay&#x29;&#x27; -- for new loans&#xd;&#xa;and  &#x28;current_date - l.term_date &#x29;  &#x3e;&#x3d; 31&#xd;&#xa;&#xd;&#xa;and c.client_no not in &#x28;&#x27;100500&#x27;, &#x27;101124&#x27;, &#x27;101495&#x27;, &#x27;102152&#x27;, &#x27;103907&#x27;, &#x27;105181&#x27;, &#x27;108319&#x27;, &#x27;109014&#x27;, &#x27;110260&#x27;, &#x27;120566&#x27;, &#x27;120815&#x27;, &#x27;131442&#x27;, &#x27;140412&#x27;&#x29; --&#x3f;&#x3f; from Aleksejs query&#xd;&#xa;&#xd;&#xa;and l.r_loan_id &#x3d; &#x3f;&#xd;&#xa; &#xd;&#xa; ORDER BY l.r_loan_id &#xd;&#xa;-- between 6 and 19 december reached 60 days&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;</sql>
    <outer_join>Y</outer_join>
