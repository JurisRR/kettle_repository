    <sql>select&#xd;&#xa;la.id as application_id&#xd;&#xa;,la.client_id&#xd;&#xa;,la.application_date&#x3a;&#x3a;date&#xd;&#xa;,extract&#x28;hour from la.created_at&#x29; as app_h&#xd;&#xa;,extract&#x28;day from la.created_at&#x29; as app_day&#xd;&#xa;,requested_principal&#xd;&#xa;,offered_principal&#xd;&#xa;&#xd;&#xa;,at.value as loan_purpose&#xd;&#xa;,la.loans_paid as finished_loan_cnt&#xd;&#xa;,case when l.extensions&#x3e; 0 then 1 else 0 end as prolongated&#xd;&#xa;, lp.id as landing_page&#xd;&#xa;, lp.traffic_source as landing_page_name&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;--&#x23;&#x23;&#x23;&#x23; client attributes&#xd;&#xa;, case when pi.gender &#x3d; &#x27;MALE&#x27; then 1 else 2 end as gender&#xd;&#xa;, gender as genderMF&#xd;&#xa;, extract&#x28; years from age&#x28;la.application_date, pi.date_of_birth&#x29;&#x29; as age&#xd;&#xa;, &#x28;case when inc.income&#x3a;&#x3a;numeric&#x3e;0 then la.requested_principal&#x2f;&#x28;inc.income&#x3a;&#x3a;numeric&#x29; else null end&#x29;&#x3a;&#x3a;numeric&#x28;13,3&#x29; as req_income&#xd;&#xa;&#xd;&#xa;--&#x23;&#x23;&#x23;&#x23; delta vista parameters&#xd;&#xa;,dv.max_delay as dv_max_delay&#xd;&#xa;,dv.last_delay as dv_last_delay&#xd;&#xa;,dv.overdue_loans_number as dv_overdue_loans_number&#xd;&#xa;,dv.canceled_loans_number as dv_canceled_loans_number&#xd;&#xa;,dv.providers_count as dv_providers_count&#xd;&#xa;,dv.last_paid_debt_date as dv_last_paid_debt_date&#xd;&#xa;,la.application_date&#x3a;&#x3a;date-dv.last_paid_debt_date&#x3a;&#x3a;date as days_since_dv&#xd;&#xa;,dv.deltavista_score_generic&#xd;&#xa;,dv.loans_number_sum-dv.active_loans_number-canceled_loans_number as dv_paid_obligations_count&#xd;&#xa;, case when &#x28;coalesce&#x28;dv.loans_number_sum,0&#x29;-coalesce&#x28;dv.active_loans_number,0&#x29;-coalesce&#x28;dv.canceled_loans_number,0&#x29;&#x29;&#x3e;20 then 21 &#xd;&#xa;else  &#x28;coalesce&#x28;dv.loans_number_sum,0&#x29;-coalesce&#x28;dv.active_loans_number,0&#x29;-coalesce&#x28;dv.canceled_loans_number,0&#x29;&#x29;&#x2b;1 end as dv_paid_obligations_count_fixed&#xd;&#xa;&#xd;&#xa;,dv.active_loans_number as dv_active_loans_number&#xd;&#xa;,dv.active_loans_open_amount as dv_active_loans_open_amount&#xd;&#xa;,dv.loans_number_0_1&#xd;&#xa;,dv.loans_number_2_3&#xd;&#xa;,dv.loans_number_4_7&#xd;&#xa;,dv.loans_number_8_30&#xd;&#xa;,dv.loans_number_31_90&#xd;&#xa;,dv.loans_number_91_210&#xd;&#xa;&#xd;&#xa;,dv.overdue_loans_number_0_1&#x2b;dv.overdue_loans_number_2_3 as overdue_0_3&#xd;&#xa;,dv.overdue_loans_number_4_7&#x2b;dv.overdue_loans_number_8_30&#x2b;dv.overdue_loans_number_31_90 as overdue_4_90&#xd;&#xa;,dv.overdue_loans_number_91_210&#x2b;dv.overdue_loans_number_211_360&#x2b;dv.overdue_loans_number_361plus as overdue_91&#xd;&#xa;,dv.payment_experience&#xd;&#xa;,dv.same_phone_different_pesel_sum&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;--&#x23;&#x23;&#x23;&#x23;&#x23; income before loan&#xd;&#xa;&#xd;&#xa;, &#x28;select income from &#x28;select bb.&#x2a;,rank&#x28;&#x29; over &#x28;partition by client_id order by updated_at asc&#x29; from &#x28;select id, client_id, created_at, updated_at, &#x28;json_value&#x3a;&#x3a;json-&#x3e;&#x3e;&#x27;monthlyIncome&#x27;&#x29;&#x3a;&#x3a;int as income, &#x28;json_value&#x3a;&#x3a;json-&#x3e;&#x3e;&#x27;monthlyLiabilities&#x27;&#x29;&#x3a;&#x3a;int as liabilities from crm.client_attribute &#xd;&#xa;where key &#x3d; &#x27;employmentData&#x27;&#xd;&#xa;union&#xd;&#xa;select id, client_id, created_at,updated_at, &#x28;json_value&#x3a;&#x3a;json-&#x3e;&#x3e;&#x27;monthlyIncome&#x27;&#x29;&#x3a;&#x3a;int as income, &#x28;json_value&#x3a;&#x3a;json-&#x3e;&#x3e;&#x27;monthlyLiabilities&#x27;&#x29;&#x3a;&#x3a;int as liabilities from crm.client_attribute_audit&#xd;&#xa;where key &#x3d; &#x27;employmentData&#x27;&#x29;bb where bb.client_id &#x3d; la.client_id and bb.updated_at &#x3c;&#x3d; la.updated_at limit 1&#x29;inc&#x29;&#xd;&#xa;, &#x28;select liabilities from &#x28;select bb.&#x2a;,rank&#x28;&#x29; over &#x28;partition by client_id order by updated_at asc&#x29; from &#x28;select id, client_id, created_at, updated_at, &#x28;json_value&#x3a;&#x3a;json-&#x3e;&#x3e;&#x27;monthlyIncome&#x27;&#x29;&#x3a;&#x3a;int as income, &#x28;json_value&#x3a;&#x3a;json-&#x3e;&#x3e;&#x27;monthlyLiabilities&#x27;&#x29;&#x3a;&#x3a;int as liabilities from crm.client_attribute &#xd;&#xa;where key &#x3d; &#x27;employmentData&#x27;&#xd;&#xa;union&#xd;&#xa;select id, client_id, created_at,updated_at, &#x28;json_value&#x3a;&#x3a;json-&#x3e;&#x3e;&#x27;monthlyIncome&#x27;&#x29;&#x3a;&#x3a;int as income, &#x28;json_value&#x3a;&#x3a;json-&#x3e;&#x3e;&#x27;monthlyLiabilities&#x27;&#x29;&#x3a;&#x3a;int as liabilities from crm.client_attribute_audit&#xd;&#xa;where key &#x3d; &#x27;employmentData&#x27;&#x29;bb where bb.client_id &#x3d; la.client_id and bb.updated_at &#x3c;&#x3d; la.updated_at limit 1&#x29;inc&#x29;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;--&#x23;&#x23;&#x23;&#x23; eucb parameters&#xd;&#xa;,eucb.rejected_loan_applications&#xd;&#xa;,eucb.total_loans_taken&#xd;&#xa;,eucb.expired_loans_count&#xd;&#xa;,eucb.current_active_loans&#xd;&#xa;,eucb.current_debt&#xd;&#xa;,eucb.current_overdue_loans&#xd;&#xa;,eucb.current_overdue_credit_amount&#xd;&#xa;,eucb.case_count&#xd;&#xa;,eucb.current_delay&#xd;&#xa;,eucb.max_delay&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;--&#x23;&#x23;&#x23;&#x23; other &#xd;&#xa;&#xd;&#xa;, &#x28;SELECT prev_dpd from &#x28;SELECT  COALESCE&#x28;&#x28;SELECT &#x28;close_date&#x3a;&#x3a;date - maturity_date&#x3a;&#x3a;Date&#x29;&#xd;&#xa;       FROM lending.loan ll&#xd;&#xa;       WHERE ll.client_id&#x3d;la.client_id and ll.issue_date&#x3a;&#x3a;Date&#x3c;la.application_date&#x3a;&#x3a;date&#xd;&#xa;       ORDER BY created_at DESC&#xd;&#xa;       LIMIT 1&#x29;, 0&#x29;as prev_dpd&#x29;pdpd&#x29; as prev_dpd&#xd;&#xa;&#xd;&#xa;, &#x28;SELECT prev_close_date from &#x28;SELECT  COALESCE&#x28;&#x28;SELECT &#x28;close_date&#x3a;&#x3a;date&#x29;&#xd;&#xa;       FROM lending.loan ll&#xd;&#xa;       WHERE ll.client_id&#x3d;la.client_id and ll.loan_application_id&#x3c;la.id&#xd;&#xa;       ORDER BY created_at DESC&#xd;&#xa;       LIMIT 1&#x29;, &#x27;2000-01-01&#x27;&#x29;as prev_close_date&#x29;pcd&#x29; as prev_close_date&#xd;&#xa;&#xd;&#xa;, &#x28;SELECT prev_loan from &#x28;SELECT  COALESCE&#x28;&#x28;SELECT principal_disbursed&#xd;&#xa;       FROM lending.loan ll&#xd;&#xa;       WHERE ll.client_id&#x3d;la.client_id and ll.issue_date&#x3a;&#x3a;Date&#x3c;la.application_date&#x3a;&#x3a;date&#xd;&#xa;       ORDER BY created_at DESC&#xd;&#xa;       LIMIT 1&#x29;, 0&#x29;as prev_loan&#x29;pdpd&#x29; as prev_loan&#xd;&#xa;&#xd;&#xa;, &#x28;SELECT prev_app_id from &#x28;SELECT  COALESCE&#x28;&#x28;SELECT &#x28;id&#x29;&#xd;&#xa;       FROM lending.loan_application ll&#xd;&#xa;       WHERE ll.client_id&#x3d;l.client_id and ll.id&#x3c;l.loan_application_id&#xd;&#xa;       ORDER BY created_at DESC&#xd;&#xa;       LIMIT 1&#x29;, 0&#x29;as prev_app_id&#x29;paid&#x29; as prev_app_id&#xd;&#xa;-- &#x23;&#x23;&#x23;&#x23; loan performance&#xd;&#xa;&#xd;&#xa;, case when l.status is null then null&#xd;&#xa;	when current_date-pp.maturity_date&#x3a;&#x3a;date&#x3c;30 then &#xd;&#xa;&#x28;case when l.status is null then null&#xd;&#xa;	when l.status &#x3d; &#x27;PAID&#x27; then 0 else null end&#x29;&#xd;&#xa;	when l.status &#x3d;&#x27;PAID&#x27; and pay.last_payment_date-pp.maturity_date&#x3a;&#x3a;date &#x3c;&#x3d; 30 then 0&#xd;&#xa;else 1 end as dpd30&#xd;&#xa;&#xd;&#xa;, case when l.status is null then null&#xd;&#xa;	when current_date-pp.maturity_date&#x3a;&#x3a;date&#x3c;5 then &#xd;&#xa;&#x28;case when l.status is null then null&#xd;&#xa;	when l.status &#x3d; &#x27;PAID&#x27; then 0 else null end&#x29;&#xd;&#xa;	when l.status &#x3d;&#x27;PAID&#x27; and pay.last_payment_date-pp.maturity_date&#x3a;&#x3a;date &#x3c;&#x3d; 5 then 0&#xd;&#xa;else 1 end as dpd5&#xd;&#xa;&#xd;&#xa;, case when l.status is null then null&#xd;&#xa;	when current_date-pp.maturity_date&#x3a;&#x3a;date&#x3c;10 then &#xd;&#xa;&#x28;case when l.status is null then null&#xd;&#xa;	when l.status &#x3d; &#x27;PAID&#x27; then 0 else null end&#x29;&#xd;&#xa;	when l.status &#x3d;&#x27;PAID&#x27; and pay.last_payment_date-pp.maturity_date&#x3a;&#x3a;date &#x3c;&#x3d; 10 then 0&#xd;&#xa;else 1 end as dpd10&#xd;&#xa;&#xd;&#xa;from&#xd;&#xa;&#xd;&#xa;--&#x23;&#x23;&#x23; lending&#xd;&#xa;lending.loan_application la &#xd;&#xa;left outer join lending.loan_application_attribute at on &#x28;at.application_id&#x3d;la.id and at.key &#x3d; &#x27;loanPurpose&#x27;&#x29;&#xd;&#xa;left outer join lending.loan l on &#x28;l.loan_application_id &#x3d; la.id&#x29;&#xd;&#xa;left outer join lending.payment_plan pp on &#x28;pp.loan_id &#x3d; la.loan_id and pp.latest &#x3d; TRUE&#x29;&#xd;&#xa;&#xd;&#xa;--&#x23;&#x23; client data	&#xd;&#xa;left outer join crm.personal_info pi on &#x28;la.client_id &#x3d; pi.client_id&#x29;&#xd;&#xa;left outer join crm.email_contact emc on &#x28;la.client_id &#x3d; emc.client_id and is_primary&#x3d;true&#x29;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;--&#x23;&#x23;credit bureaus&#xd;&#xa;left outer join &#x28;select e.&#x2a;,rank&#x28;&#x29; over &#x28;partition by loan_application_id  order by created_at DESC&#x29; as rank&#xd;&#xa; from poland_eucb.response e&#x29;eucb on &#x28;eucb.loan_application_id&#x3d;la.id and eucb.rank &#x3d; 1&#x29;&#xd;&#xa;&#xd;&#xa;left outer join poland_deltavista.response dv on &#x28;la.id &#x3d; dv.loan_application_id&#x29;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;--&#x23;&#x23; others&#xd;&#xa;left outer join &#x28;select distinct application_id, name as traffic_source, lp.id, params_json&#x3a;&#x3a;json-&#x3e;&#x3e;&#x27;d_aid&#x27; as doaff_publisher from marketing_landing.event e join marketing_landing.landing_page lp &#xd;&#xa;on &#x28;e.landing_page_id&#x3d;lp.id&#x29;&#x29;lp on la.id &#x3d; lp.application_id&#xd;&#xa; left outer join &#x28;select e.client_id,  lp.name as first_landing_page, rank&#x28;&#x29; over &#x28;partition by e.client_id  order by e.created_at ASC&#x29; as seq&#xd;&#xa;from marketing_landing.event e &#xd;&#xa;join marketing_landing.landing_page lp on &#x28;e.landing_page_id&#x3d;lp.id&#x29;&#xd;&#xa; where event_type &#x3d; &#x27;LEAD&#x27;&#x29;lpf on &#x28;la.client_id &#x3d; lpf.client_id and lpf.seq&#x3d;1&#x29;&#xd;&#xa;&#xd;&#xa;left outer join &#x28;SELECT &#xd;&#xa;	&#x2a;&#xd;&#xa;  FROM &#xd;&#xa;	&#x28;SELECT v.client_id, e.utm_source, e.utm_campaign, e.referrer_domain, e.created_at&#x3a;&#x3a;Date as event_date&#xd;&#xa;	&#xd;&#xa;	, e.geo_location, e.os, e.device, e.screen_size, la.id&#xd;&#xa;	,rank&#x28;&#x29; over &#x28;partition by la.client_id order by e.created_at&#x29; as rrow &#xd;&#xa;	from marketing_tracking.event e&#xd;&#xa;  join marketing_tracking.visitor v on &#x28;v.id &#x3d; e.visitor_id&#x29; join &#xd;&#xa;  	lending.loan_application la on &#x28;la.client_id &#x3d; v.client_id&#x29;&#x29;aa&#xd;&#xa;  where rrow&#x3d;1&#x29;mt2 on &#x28;mt2.id &#x3d; la.id&#x29;&#xd;&#xa;&#xd;&#xa;  &#xd;&#xa;left outer join &#x28;select &#x2a; from &#x28;SELECT distinct client_id,  json_value&#x3a;&#x3a;json -&#x3e;&#x3e;&#x27;monthlyIncome&#x27; as income, json_value&#x3a;&#x3a;json -&#x3e;&#x3e;&#x27;monthlyLiabilities&#x27; as liabilities, rank&#x28;&#x29; over &#x28;partition by client_id  order by created_at DESC&#x29; as rrow&#xd;&#xa;  FROM crm.client_attribute where key &#x3d; &#x27;employmentData&#x27;&#x29;aa where rrow&#x3d;1&#x29;inc on &#x28;la.client_id &#x3d; inc.client_id&#x29;&#xd;&#xa;&#xd;&#xa;left outer join &#x28;select min&#x28;booking_date&#x29;&#x3a;&#x3a;date as first_payment_date, max&#x28;booking_date&#x29;&#x3a;&#x3a;date as last_payment_date, loan_id&#xd;&#xa;from transaction.transaction where transaction_type in &#x28;&#x27;REPAYMENT&#x27;, &#x27;FEE_PAYMENT&#x27;&#x29; and voided &#x3d; FALSE group by loan_id&#x29;pay on &#x28;la.loan_id &#x3d; pay.loan_id&#x29;&#xd;&#xa;&#xd;&#xa;where application_date&#x3a;&#x3a;date between&#xd;&#xa; &#x27;2018-01-01&#x27; and &#x27;2018-05-31&#x27;   ---- load month by month afterwards&#xd;&#xa;&#xd;&#xa;--between&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;  &#xd;&#xa;--limit 100&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa; </sql>
