 outer join &#x28;select min&#x28;booking_date&#x29;&#x3a;&#x3a;date as first_payment_date, max&#x28;booking_date&#x29;&#x3a;&#x3a;date as last_payment_date, loan_id&#xd;&#xa;&#xd;&#xa;from transaction.transaction where transaction_type in &#x28;&#x27;REPAYMENT&#x27;, &#x27;FEE_PAYMENT&#x27;&#x29; and voided &#x3d; FALSE group by loan_id&#x29;pay on &#x28;l.id &#x3d; pay.loan_id&#x29;&#xd;&#xa;&#xd;&#xa;left outer join am5 am5 on &#x28;am5.loan_id &#x3d; l.id&#x29;&#xd;&#xa;&#xd;&#xa;--&#x23;&#x23;&#x23; add info on employer verification &#xd;&#xa;left outer join &#x28;select a.attempts, a.resolution, a.status, w.application_id, t.times_postponed, t.times_reopened &#xd;&#xa;		from workflow.activity a join workflow.workflow w on &#x28;a.workflow_id &#x3d; w.id&#x29; left outer join task.task t on &#x28;a.id &#x3d; t.activity_id&#x29;&#xd;&#xa;		where a.name &#x3d; &#x27;EmployerCall&#x27;&#x29;ec on &#x28;la.id &#x3d; ec.application_id&#x29; &#xd;&#xa;&#xd;&#xa;left outer join workflow.workflow ww on &#x28;ww.id &#x3d; la.workflow_id&#x29;&#xd;&#xa;&#xd;&#xa;left outer join &#x28;SELECT client_id, json_value&#x3a;&#x3a;json-&#x3e;&#x3e;&#x27;ektp&#x27; as ektp    &#xd;&#xa;      FROM crm.client_attribute &#xd;&#xa;    where key &#x3d; &#x27;personalData&#x27;&#x29;cc  on &#x28;cc.client_id &#x3d; l.client_id&#x29;&#xd;&#xa;&#xd;&#xa;left outer join crm.personal_info pi on &#x28;pi.client_id &#x3d; l.client_id&#x29;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;left outer join &#x28;select min&#x28;booking_date&#x29;&#x3a;&#x3a;date as first_extension_date, loan_id&#xd;&#xa;&#xd;&#xa;from transaction.transaction where transaction_type in &#x28;&#x27;FEE_PAYMENT&#x27;&#x29; &#xd;&#xa;and transaction_sub_type &#x3d; &#x27;EXTENSION&#x27; and voided &#x3d; FALSE group by loan_id&#x29;ext on &#x28;l.id &#x3d; ext.loan_id&#x29;&#xd;&#xa;&#xd;&#xa;left outer join &#x28;select &#x2a; from &#x28;select distinct loan_application_id, scoring, e.id as score_id, split_run_selected_result, rank&#x28;&#x29; over &#x28;partition by loan_application_id  order by e.id DESC &#x29; as seq_score&#xd;&#xa;from scoring.scorecard_evaluation e &#xd;&#xa;	join scoring.scorecard s on &#x28;e.scorecard_id &#x3d;  s.id and s.name in &#x28;&#x27;CREDIT_BUREAU_SCORECARD_0&#x27;,&#x27;CREDIT_BUREAU_SCORECARD_1&#x27;,&#x27;CREDIT_BUREAU_SCORECARD_2&#x27;,&#x27;CREDIT_BUREAU_SCORECARD_3&#x2b;&#x27;&#x29;&#x29;&#xd;&#xa;&#x29;aa where seq_score&#x3d;1&#x29;ss &#xd;&#xa;	on ss.loan_application_id&#x3d;la.id&#xd;&#xa;&#xd;&#xa;left outer join &#x28;select &#x2a; from &#x28;select distinct loan_application_id, scoring, e.id as score_id, split_run_selected_result, rank&#x28;&#x29; over &#x28;partition by loan_application_id  order by e.id DESC &#x29; as seq_score&#xd;&#xa;from scoring.scorecard_evaluation e &#xd;&#xa;	join scoring.scorecard s on &#x28;e.scorecard_id &#x3d;  s.id and s.name in &#x28;&#x27;CLIENT_PROFILE_SCORECARD_0&#x27;,&#x27;CLIENT_PROFILE_SCORECARD_1&#x27;,&#x27;CLIENT_PROFILE_SCORECARD_2&#x27;,&#x27;CLIENT_PROFILE_SCORECARD_3&#x2b;&#x27;&#x29;&#x29;&#xd;&#xa;&#x29;aa where seq_score&#x3d;1&#x29;sss &#xd;&#xa;	on sss.loan_application_id&#x3d;la.id&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;where --trans.principal &#x3e; 0 --for two loans it is 0 because voided&#xd;&#xa;l.status &#x21;&#x3d; &#x27;VOIDED&#x27;&#xd;&#xa;&#xd;&#xa;and l.id &#x3c;&#x3e; 19413&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;--limit 100</sql>
