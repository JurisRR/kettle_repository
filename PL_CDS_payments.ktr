    <sql>select &#xd;&#xa;p.r_payment_id&#xd;&#xa;,p.amount as amount_payment&#xd;&#xa;,p.r_client_id&#xd;&#xa;,p.cl_bank_acct_id&#xd;&#xa;,p.payment_date&#x3a;&#x3a;date as payment_date_payment&#xd;&#xa;,extract&#x28;month from p.payment_date&#x29; as month_payment&#xd;&#xa;,extract&#x28;year from p.payment_date&#x29; as year_payment&#xd;&#xa;--, rank &#xd;&#xa;&#xd;&#xa;&#xd;&#xa;,&#x28;select sum&#x28;p.amount&#x29; as sum_payment_month from r_payment_distr d join r_payment p on  &#x28;d.r_payment_id &#x3d; p.r_payment_id&#x29; join r_loan l on &#xd;&#xa;	&#x28;d.r_loan_id &#x3d; l.r_loan_id&#x29; where l.loan_no &#x3d;&#x3f; and p.payment_date&#x3a;&#x3a;date between &#x27;2017-04-01&#x27; and &#x27;2017-04-30&#x27; &#x29;&#xd;&#xa;&#xd;&#xa;,p.payment_date&#x3a;&#x3a;date - &#x3f; as days_after_cds&#xd;&#xa;&#xd;&#xa;from &#xd;&#xa;r_payment_distr d join r_payment p on  &#x28;d.r_payment_id &#x3d; p.r_payment_id&#x29; join r_loan l on &#x28;d.r_loan_id &#x3d; l.r_loan_id&#x29; &#xd;&#xa;where 1&#x3d;1 &#xd;&#xa;	and p.cl_bank_acct_id &#x3c;&#x3e; 11&#xd;&#xa;	and p.type &#x3d; &#x27;IN_Loan&#x27;&#xd;&#xa;	&#xd;&#xa;	&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;and l.loan_no &#x3d;&#x3f;&#xd;&#xa;and p.payment_date&#x3a;&#x3a;date between &#x27;2017-04-01&#x27; and &#x27;2017-04-30&#x27; &#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;</sql>
