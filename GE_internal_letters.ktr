    <sql>-- &#x23;&#x23; Extract data on loan level including credit info, client, application data&#xd;&#xa;&#xd;&#xa;--&#x23;&#x23; aggregation of payments_distr table on loan level&#xd;&#xa;with dd as &#x28;select d.r_loan_id,&#xd;&#xa;sum&#x28;d.amount&#x29; as amount, sum&#x28;d.amount_principal&#x29; as amount_principal, &#xd;&#xa;       sum&#x28;d.amount_commission&#x29; as amount_commission, sum&#x28;d.amount_penalty&#x29; as amount_penalty, sum&#x28;d.amount_surplus&#x29; as amount_surplus,&#xd;&#xa;        sum&#x28;d.amount_prolongation&#x29; as amount_prolongation, sum&#x28;d.amount_fees&#x29; as amount_fees&#xd;&#xa;	&#xd;&#xa;from &#xd;&#xa;r_payment_distr d join r_payment p on  &#x28;d.r_payment_id &#x3d; p.r_payment_id&#x29; &#xd;&#xa;where 1&#x3d;1 &#xd;&#xa;	and p.cl_bank_acct_id &#x3c;&#x3e; 18&#xd;&#xa;	--and p.cl_bank_acct_id &#x3c;&#x3e; 12&#xd;&#xa;	group by d.r_loan_id&#x29;&#xd;&#xa;&#xd;&#xa;,pay as &#x28;select d.r_loan_id, min&#x28;payment_date&#x29;&#x3a;&#x3a;date as first_payment_date , max&#x28;payment_date&#x29;&#x3a;&#x3a;date as last_payment_date&#xd;&#xa;	from r_payment_distr d join r_payment p on  &#x28;d.r_payment_id &#x3d; p.r_payment_id&#x29; &#xd;&#xa;		where p.type &#x3d; &#x27;IN_Loan&#x27; and p.cl_bank_acct_id &#x3c;&#x3e; 12 and cl_bank_acct_id &#x3c;&#x3e;  18&#xd;&#xa;		group by d.r_loan_id&#x29;&#xd;&#xa;&#xd;&#xa;,vpay as &#x28;select d.r_loan_id, min&#x28;payment_date&#x29;&#x3a;&#x3a;date as first_virtual_payment_date , max&#x28;payment_date&#x29;&#x3a;&#x3a;date as last_payment_date&#xd;&#xa;	from r_payment_distr d join r_payment p on  &#x28;d.r_payment_id &#x3d; p.r_payment_id&#x29; &#xd;&#xa;		where p.type &#x3d; &#x27;IN_Loan&#x27; and &#x28;p.cl_bank_acct_id &#x3d; 12 or cl_bank_acct_id &#x3d;  18&#x29;&#xd;&#xa;		group by d.r_loan_id&#x29;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;,out as &#x28;select d.r_loan_id, min&#x28;payment_date&#x29;&#x3a;&#x3a;date as out_loan_date &#xd;&#xa;	from r_payment_distr d join r_payment p on  &#x28;d.r_payment_id &#x3d; p.r_payment_id&#x29; &#xd;&#xa;		where p.type &#x3d; &#x27;OUT_Loan&#x27; and p.cl_bank_acct_id &#x3c;&#x3e; 12 and cl_bank_acct_id &#x3c;&#x3e;  18&#xd;&#xa;		group by d.r_loan_id&#x29;&#xd;&#xa;&#xd;&#xa;,freex as &#x28;select d.r_loan_id, wp.amount_prolongation , wp.days_prolongation, min&#x28;p.payment_date&#x29;&#x3a;&#x3a;date as free_extension_date&#xd;&#xa;	from r_payment_distr d join r_payment p on  &#x28;d.r_payment_id &#x3d; p.r_payment_id&#x29; &#xd;&#xa;		join r_withdrawal_prolongation_log wp on &#x28;d.r_payment_distr_id &#x3d; wp.r_payment_distr_id&#x29;&#xd;&#xa;		where p.type &#x3d; &#x27;IN_Loan&#x27; and p.cl_bank_acct_id &#x3d; 18&#xd;&#xa;		group by d.r_loan_id, wp.amount_prolongation , wp.days_prolongation&#x29;&#xd;&#xa;&#xd;&#xa;--&#x23;&#x23;&#x23;&#x23;&#x23; add payments by 30 day intervals&#xd;&#xa;,pint as &#x28;select r_loan_id,&#xd;&#xa;max&#x28;case when interval5 &#x3d; 0 then sum else 0 end&#x29; as amount0,&#xd;&#xa;max&#x28;case when interval5 &#x3d; 5 then sum else 0 end&#x29; as amount5,&#xd;&#xa;max&#x28;case when interval5 &#x3d; 10 then sum else 0 end&#x29; as amount10,&#xd;&#xa;max&#x28;case when interval5 &#x3d; 15 then sum else 0 end&#x29; as amount15,&#xd;&#xa;max&#x28;case when interval5 &#x3d; 20 then sum else 0 end&#x29; as amount20,&#xd;&#xa;max&#x28;case when interval5 &#x3d; 25 then sum else 0 end&#x29; as amount25,&#xd;&#xa;max&#x28;case when interval5 &#x3d; 30 then sum else 0 end&#x29; as amount30,&#xd;&#xa;max&#x28;case when interval5 &#x3d; 35 then sum else 0 end&#x29; as amount35,&#xd;&#xa;max&#x28;case when interval5 &#x3d; 40 then sum else 0 end&#x29; as amount40,&#xd;&#xa;max&#x28;case when interval5 &#x3d; 45 then sum else 0 end&#x29; as amount45,&#xd;&#xa;max&#x28;case when interval5 &#x3d; 50 then sum else 0 end&#x29; as amount50,&#xd;&#xa;max&#x28;case when interval5 &#x3d; 55 then sum else 0 end&#x29; as amount55,&#xd;&#xa;max&#x28;case when interval5 &#x3d; 60 then sum else 0 end&#x29; as amount60,&#xd;&#xa;max&#x28;case when interval5 &#x3d; 61 then sum else 0 end&#x29; as amount61&#xd;&#xa;&#xd;&#xa;from &#x28;select r_loan_id, interval5, sum&#x28;amount&#x29; from &#x28;select d.r_loan_id, &#x28;d.amount-d.amount_surplus&#x29; as amount,&#xd;&#xa;case &#xd;&#xa;	when &#x28;payment_date&#x3a;&#x3a;date -&#x3f;&#x29; &#x3c;&#x3d; 0 then 0&#xd;&#xa;	when &#x28;payment_date&#x3a;&#x3a;date -&#x3f;&#x29; &#x3c;&#x3d; 5 then 5        	&#xd;&#xa;	when &#x28;payment_date&#x3a;&#x3a;date -&#x3f;&#x29; &#x3c;&#x3d; 10 then 10&#xd;&#xa;	when &#x28;payment_date&#x3a;&#x3a;date -&#x3f;&#x29; &#x3c;&#x3d; 15 then 15&#xd;&#xa;	when &#x28;payment_date&#x3a;&#x3a;date -&#x3f;&#x29; &#x3c;&#x3d; 20 then 20&#xd;&#xa;	when &#x28;payment_date&#x3a;&#x3a;date -&#x3f;&#x29; &#x3c;&#x3d; 25 then 25&#xd;&#xa;	when &#x28;payment_date&#x3a;&#x3a;date -&#x3f;&#x29; &#x3c;&#x3d; 30 then 30&#xd;&#xa;	when &#x28;payment_date&#x3a;&#x3a;date -&#x3f;&#x29; &#x3c;&#x3d; 35 then 35&#xd;&#xa;	when &#x28;payment_date&#x3a;&#x3a;date -&#x3f;&#x29; &#x3c;&#x3d; 40 then 40&#xd;&#xa;	when &#x28;payment_date&#x3a;&#x3a;date -&#x3f;&#x29; &#x3c;&#x3d; 45 then 45&#xd;&#xa;	when &#x28;payment_date&#x3a;&#x3a;date -&#x3f;&#x29; &#x3c;&#x3d; 50 then 50&#xd;&#xa;	when &#x28;payment_date&#x3a;&#x3a;date -&#x3f;&#x29; &#x3c;&#x3d; 55 then 55&#xd;&#xa;	when &#x28;payment_date&#x3a;&#x3a;date -&#x3f;&#x29; &#x3c;&#x3d; 60 then 60 &#xd;&#xa;	else 61 end as interval5&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;from r_payment_distr d join r_payment p on  &#x28;d.r_payment_id &#x3d; p.r_payment_id&#x29; join r_loan l on &#x28;l.r_loan_id &#x3d; d.r_loan_id&#x29;&#xd;&#xa;	where p.type &#x3d; &#x27;IN_Loan&#x27; and p.cl_bank_acct_id &#x3c;&#x3e; 18&#xd;&#xa;&#x29;dd		&#xd;&#xa;group by r_loan_id, interval5&#xd;&#xa;order by r_loan_id, interval5&#x29;int5&#xd;&#xa;&#xd;&#xa;group by r_loan_id&#x29;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;-- &#x23;&#x23;&#x23;&#x23;&#x23; add also subquery to attach info on virtual transactions&#xd;&#xa;-- &#x23;&#x23; in case when surplus &#x3c; loan_amount, seems like loan defaulted, otherwise seems like a good loan&#xd;&#xa;&#xd;&#xa;---&#x23;&#x23;&#x23;&#x23;&#x23;&#xd;&#xa;	 &#xd;&#xa;&#xd;&#xa;,clci as &#x28;&#xd;&#xa;	 select &#x2a; from &#x28;select distinct r_client_id, id1, id2,&#xd;&#xa; decision, &#xd;&#xa; app_date,&#xd;&#xa; r_credit_info_id,&#xd;&#xa; rank&#x28;&#x29; over &#x28;partition by r_client_id, id1 order by r_client_id, id1, decision DESC, r_credit_info_id DESC &#x29; as rrow from&#xd;&#xa;&#x28;select  rra.r_client_id&#xd;&#xa;	,rra.r_application_id as id1&#xd;&#xa;	, raa.r_application_id as id2&#xd;&#xa;	,r_credit_info_id&#xd;&#xa;	--,decision&#xd;&#xa;	,case when decision &#x3d; &#x27;ERROR&#x27; then &#x27;ERROR&#x27;&#xd;&#xa;		 when decision is null then &#x27;no info&#x27;&#xd;&#xa;		else &#x27;ok&#x27; end as decision&#xd;&#xa;			,rra.app_date&#x3a;&#x3a;date&#xd;&#xa;	--,raa.app_date&#x3a;&#x3a;date&#xd;&#xa;&#xd;&#xa;	from&#xd;&#xa;	r_application rra join r_application raa on &#x28;rra.r_client_id &#x3d; raa.r_client_id&#x29;-- and raa.app_date &#x3c; rra.app_date&#x29;&#xd;&#xa;	left outer join r_credit_info ci on &#x28;raa.r_application_id &#x3d; ci.r_application_id&#x29;&#xd;&#xa;&#xd;&#xa;	where 1&#x3d;1 &#xd;&#xa;	--and rra.r_client_id &#x3d; 45254&#xd;&#xa;	--in &#x28;&#xd;&#xa;&#xd;&#xa;	and raa.r_application_id &#x3c;&#x3d; rra.r_application_id&#xd;&#xa;	and decision  &#x3c;&#x3e; &#x27;no info&#x27;&#xd;&#xa;	order by rra.r_client_id, rra.r_application_id, raa.r_application_id DESC&#x29;aa&#xd;&#xa;&#xd;&#xa;	order by id1, decision DESC&#x29;cc&#xd;&#xa;	where rrow &#x3d;1&#x29;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;, neg as 	&#xd;&#xa;	&#x28;with&#xd;&#xa;xx as &#x28;select r_credit_info_id, status, count&#x28;status&#x29; as count_isnegative from r_credit_info_negative where status &#x3d; &#x27;IsNegative&#x27; group by 1,2&#x29;,&#xd;&#xa;&#xd;&#xa;yy as &#x28;select r_credit_info_id, status, count&#x28;status&#x29; as count_wasnegative from r_credit_info_negative where status &#x3d; &#x27;WasNegative&#x27; group by 1,2&#x29;&#xd;&#xa;&#xd;&#xa;SELECT r.r_credit_info_id, r.debtor_existing_operations, &#xd;&#xa;       r.debtor_terminated_operations ,&#xd;&#xa;        xx.count_isnegative, yy.count_wasnegative&#xd;&#xa;FROM r_credit_info r&#xd;&#xa;    left outer  join xx xx&#xd;&#xa;  on &#x28;r.r_credit_info_id &#x3d; xx.r_credit_info_id&#x29; &#xd;&#xa;  left outer join yy yy on &#x28;r.r_credit_info_id &#x3d; yy.r_credit_info_id&#x29;&#x29;&#xd;&#xa;-- need to add count of credit calls for errors&#xd;&#xa;---&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;3	&#xd;&#xa;&#xd;&#xa;SELECT &#xd;&#xa;&#xd;&#xa;distinct l.r_loan_id,&#xd;&#xa; l.loan_Date&#x3a;&#x3a;date,&#xd;&#xa; extract&#x28;week from l.loan_Date&#x29; as iso_week, --&#x23;&#x23;&#x23; extract week number&#xd;&#xa; l.status, &#xd;&#xa;substring&#x28;l.loan_no from &#x27;..&#x24;&#x27;&#x29;&#x3a;&#x3a;numeric loan_seq,&#xd;&#xa;case when substring&#x28;l.loan_no from &#x27;..&#x24;&#x27;&#x29;&#x3a;&#x3a;numeric &#x3d;1 then &#x27;first&#x27; else &#x27;repeated&#x27; end as loan_type,&#xd;&#xa; l.loan_amount, l.loan_commission,&#xd;&#xa;&#xd;&#xa;dd.amount as amount_distr,&#xd;&#xa; dd.amount_principal as amount_principal_distr, &#xd;&#xa;       dd.amount_commission as amount_commission_distr, &#xd;&#xa;       dd.amount_penalty as amount_penalty_distr,&#xd;&#xa;        dd.amount_surplus as amount_surplus_distr, &#xd;&#xa;        dd.amount_prolongation as amount_prolongation_distr, &#xd;&#xa;        dd.amount_fees as amount_fees_distr&#xd;&#xa;      -- d.is_early_repayment, d.ad_client_id, d.isactive, p.type&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;       ,&#xd;&#xa;     --  a.amount as application_amount,&#xd;&#xa;       a.original_amount as original_application_amount,&#xd;&#xa;       a.term,&#xd;&#xa;     --  a.original_term,&#xd;&#xa;       a.ip_address,&#xd;&#xa;     --  a.score,&#xd;&#xa;     --  &#x28;a.answered_time&#x3a;&#x3a;date-a.app_date&#x3a;&#x3a;date&#x29; as days_to_answer,&#xd;&#xa;       &#xd;&#xa;&#xd;&#xa;&#xd;&#xa;c.r_client_id&#xd;&#xa;,c.gender&#xd;&#xa;,extract&#x28;years from age&#x28;c.dob&#x29;&#x29; as age  --&#x23;&#x23;&#x23; &#xd;&#xa;,div&#x28;extract&#x28;years from age&#x28;c.dob&#x29;&#x29;&#x3a;&#x3a;int-1,5&#x29;&#x2a;5&#x2b;1 &#x7c;&#x7c;&#x27;-&#x27;&#x7c;&#x7c; div&#x28;extract&#x28;years from age&#x28;c.dob&#x29;&#x29;&#x3a;&#x3a;int-1,5&#x29;&#x2a;5&#x2b;5 as age_group&#xd;&#xa;,c.income_amount&#xd;&#xa;,c.liabilities_amount&#xd;&#xa;--,c.employment_type&#xd;&#xa;--,c.mobile_operator&#xd;&#xa;--,c.actual_address_city&#xd;&#xa;,ad.city&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;,a.r_application_id as loan_application_id&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;,       l.paid_date&#x3a;&#x3a;date,&#xd;&#xa;&#xd;&#xa;       l.prolongated as prolongation_count,&#xd;&#xa;		case when l.prolongated &#x3d; 0 then 0&#xd;&#xa;				when l.prolongated &#x3e; 0 then 1 else null end as prolongated,&#xd;&#xa;&#xd;&#xa; &#xd;&#xa;       l.loan_commission_non_free&#xd;&#xa;, pay.first_payment_date&#xd;&#xa;, pay.last_payment_date     &#xd;&#xa;&#xd;&#xa;-- surplus calculations&#xd;&#xa;, &#x28;l.loan_amount&#x2b;l.loan_commission&#x29;&#x2f;l.loan_amount&#x2a;100&#x3a;&#x3a;double precision as surplus_contract&#xd;&#xa;, &#x28;l.loan_amount&#x2b;dd.amount&#x29;&#x2f;l.loan_amount&#x2a;100&#x3a;&#x3a;double precision as surplus_fact&#xd;&#xa;&#xd;&#xa;, case when &#x28;l.status &#x3d; &#x27;Completed&#x27; or &#x28;current_date-l.term_date&#x3a;&#x3a;date&#x29;&#x3e;0&#x29; then &#x28;l.loan_amount&#x2b;dd.amount&#x29;&#x2f;l.loan_amount&#x2a;100&#x3a;&#x3a;double precision &#xd;&#xa;else &#x28;dd.amount&#x2b;l.loan_amount &#x2b; l.amount_open_principal&#x2b; &#xd;&#xa;       l.amount_open_commission &#x2b; l.amount_open_fees &#x2b; l.amount_open_penalty&#x29;&#x2f;l.loan_amount&#x2a;100&#x3a;&#x3a;double precision end as surplus_potential_0days-- &#xd;&#xa;&#xd;&#xa;,case when &#x28;l.status &#x3d; &#x27;Completed&#x27; or &#x28;current_date-l.term_date&#x3a;&#x3a;date&#x29;&#x3e;3&#x29; then &#x28;l.loan_amount&#x2b;dd.amount&#x29;&#x2f;l.loan_amount&#x2a;100&#x3a;&#x3a;double precision &#xd;&#xa;else &#x28;dd.amount&#x2b;l.loan_amount &#x2b; l.amount_open_principal&#x2b; &#xd;&#xa;       l.amount_open_commission &#x2b; l.amount_open_fees &#x2b; l.amount_open_penalty&#x29;&#x2f;l.loan_amount&#x2a;100&#x3a;&#x3a;double precision end as surplus_potential_3days	-- 5 days grace period								-- 5 days grace period	&#xd;&#xa;&#xd;&#xa;,case when &#x28;l.status &#x3d; &#x27;Completed&#x27; or &#x28;current_date-l.term_date&#x3a;&#x3a;date&#x29;&#x3e;5&#x29; then &#x28;l.loan_amount&#x2b;dd.amount&#x29;&#x2f;l.loan_amount&#x2a;100&#x3a;&#x3a;double precision &#xd;&#xa;else &#x28;dd.amount&#x2b;l.loan_amount &#x2b; l.amount_open_principal&#x2b; &#xd;&#xa;       l.amount_open_commission &#x2b; l.amount_open_fees &#x2b; l.amount_open_penalty&#x29;&#x2f;l.loan_amount&#x2a;100&#x3a;&#x3a;double precision end as surplus_potential_5days&#xd;&#xa;-- surpluses should be correct &#xd;&#xa;--&#x23;&#x23;&#x23;&#x23;&#x23; FPDs  &#xd;&#xa;-- &#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23; &#xd;&#xa;,case when l.status &#x3d; &#x27;Completed&#x27; then &#x28;l.loan_date&#x3a;&#x3a;date&#x2b;l.term_days&#x29;&#x3a;&#x3a;date - pay.last_payment_date &#xd;&#xa;	else null end &#xd;&#xa;	as finished_n_days_earlier&#xd;&#xa;&#xd;&#xa;, case when &#x28;&#x28;l.loan_date&#x3a;&#x3a;date&#x2b;l.term_days&#x29;&#x3a;&#x3a;date&#x2b;0&#x29; &#x3e; current_date and pay.first_payment_date is NULL then 6 --&#xd;&#xa;	when pay.first_payment_date is null then 1&#xd;&#xa;	WHEN  pay.first_payment_date&#x3a;&#x3a;date &#x3c;&#x3d; &#x28;&#x28;l.loan_date&#x3a;&#x3a;date&#x2b;l.term_days&#x29;&#x3a;&#x3a;date&#x2b;0&#x29; THEN 0&#xd;&#xa;     WHEN pay.first_payment_date&#x3a;&#x3a;date &#x3e;  &#x28;&#x28;l.loan_date&#x3a;&#x3a;date&#x2b;l.term_days&#x29;&#x3a;&#x3a;date&#x2b;0&#x29; THEN 1&#xd;&#xa;	else -1 end as FPD0&#xd;&#xa;&#xd;&#xa;, case when &#x28;&#x28;l.loan_date&#x3a;&#x3a;date&#x2b;l.term_days&#x29;&#x3a;&#x3a;date&#x2b;5&#x29; &#x3e; current_date and pay.first_payment_date is NULL then 6 --&#xd;&#xa;	when pay.first_payment_date is null then 1&#xd;&#xa;	WHEN  pay.first_payment_date&#x3a;&#x3a;date &#x3c;&#x3d; &#x28;&#x28;l.loan_date&#x3a;&#x3a;date&#x2b;l.term_days&#x29;&#x3a;&#x3a;date&#x2b;5&#x29; THEN 0&#xd;&#xa;     WHEN pay.first_payment_date&#x3a;&#x3a;date &#x3e;  &#x28;&#x28;l.loan_date&#x3a;&#x3a;date&#x2b;l.term_days&#x29;&#x3a;&#x3a;date&#x2b;5&#x29; THEN 1&#xd;&#xa;	else -1 end as FPD5&#xd;&#xa;&#xd;&#xa;, case when &#x28;&#x28;l.loan_date&#x3a;&#x3a;date&#x2b;l.term_days&#x29;&#x3a;&#x3a;date&#x2b;7&#x29; &#x3e; current_date and pay.first_payment_date is NULL then 6 --&#xd;&#xa;	when pay.first_payment_date is null then 1&#xd;&#xa;	WHEN  pay.first_payment_date&#x3a;&#x3a;date &#x3c;&#x3d; &#x28;&#x28;l.loan_date&#x3a;&#x3a;date&#x2b;l.term_days&#x29;&#x3a;&#x3a;date&#x2b;7&#x29; THEN 0&#xd;&#xa;     WHEN pay.first_payment_date&#x3a;&#x3a;date &#x3e;  &#x28;&#x28;l.loan_date&#x3a;&#x3a;date&#x2b;l.term_days&#x29;&#x3a;&#x3a;date&#x2b;7&#x29; THEN 1&#xd;&#xa;	else -1 end as FPD7		&#xd;&#xa;&#xd;&#xa;, case when &#x28;&#x28;l.loan_date&#x3a;&#x3a;date&#x2b;l.term_days&#x29;&#x3a;&#x3a;date&#x2b;10&#x29; &#x3e; current_date and pay.first_payment_date is NULL then 6 --&#xd;&#xa;	when pay.first_payment_date is null then 1&#xd;&#xa;	WHEN  pay.first_payment_date&#x3a;&#x3a;date &#x3c;&#x3d; &#x28;&#x28;l.loan_date&#x3a;&#x3a;date&#x2b;l.term_days&#x29;&#x3a;&#x3a;date&#x2b;10&#x29; THEN 0&#xd;&#xa;     WHEN pay.first_payment_date&#x3a;&#x3a;date &#x3e;  &#x28;&#x28;l.loan_date&#x3a;&#x3a;date&#x2b;l.term_days&#x29;&#x3a;&#x3a;date&#x2b;10&#x29; THEN 1&#xd;&#xa;	else -1 end as FPD10&#xd;&#xa;&#xd;&#xa;, case when &#x28;&#x28;l.loan_date&#x3a;&#x3a;date&#x2b;l.term_days&#x29;&#x3a;&#x3a;date&#x2b;15&#x29; &#x3e; current_date and pay.first_payment_date is NULL then 6 --&#xd;&#xa;	when pay.first_payment_date is null then 1&#xd;&#xa;	WHEN  pay.first_payment_date&#x3a;&#x3a;date &#x3c;&#x3d; &#x28;&#x28;l.loan_date&#x3a;&#x3a;date&#x2b;l.term_days&#x29;&#x3a;&#x3a;date&#x2b;15&#x29; THEN 0&#xd;&#xa;     WHEN pay.first_payment_date&#x3a;&#x3a;date &#x3e;  &#x28;&#x28;l.loan_date&#x3a;&#x3a;date&#x2b;l.term_days&#x29;&#x3a;&#x3a;date&#x2b;15&#x29; THEN 1&#xd;&#xa;	else -1 end as FPD15&#xd;&#xa;&#xd;&#xa;, case when &#x28;&#x28;l.loan_date&#x3a;&#x3a;date&#x2b;l.term_days&#x29;&#x3a;&#x3a;date&#x2b;30&#x29; &#x3e; current_date and pay.first_payment_date is NULL then 6 --&#xd;&#xa;	when pay.first_payment_date is null then 1&#xd;&#xa;	WHEN  pay.first_payment_date&#x3a;&#x3a;date &#x3c;&#x3d; &#x28;&#x28;l.loan_date&#x3a;&#x3a;date&#x2b;l.term_days&#x29;&#x3a;&#x3a;date&#x2b;30&#x29; THEN 0&#xd;&#xa;     WHEN pay.first_payment_date&#x3a;&#x3a;date &#x3e;  &#x28;&#x28;l.loan_date&#x3a;&#x3a;date&#x2b;l.term_days&#x29;&#x3a;&#x3a;date&#x2b;30&#x29; THEN 1&#xd;&#xa;	else -1 end as FPD30&#xd;&#xa;&#xd;&#xa;,current_date as extraction_date&#xd;&#xa;,extract&#x28;hour from current_timestamp&#x29;&#x7c;&#x7c;&#x27;&#x3a;&#x27;&#x7c;&#x7c;extract&#x28;minutes from current_timestamp&#x29; as extraction_time&#xd;&#xa;,v.first_virtual_payment_date&#xd;&#xa;&#xd;&#xa;,x.amount_prolongation as free_extension_amount&#xd;&#xa;,x.days_prolongation as free_extension_days&#xd;&#xa;,x.free_extension_date&#xd;&#xa;&#xd;&#xa;,0&#x2b;amount0 as amount0&#xd;&#xa;,0&#x2b;amount5 as amount5&#xd;&#xa;,0&#x2b;amount10 as amount10&#xd;&#xa;,0&#x2b;amount15 as amount15&#xd;&#xa;,0&#x2b;amount20 as amount20&#xd;&#xa;,0&#