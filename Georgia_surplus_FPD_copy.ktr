    <sql>-- with IN_Loan&#xd;&#xa;&#xd;&#xa;with inloan as &#x28;select r_loan_id, sum&#x28;amount &#x2b; amount_additional&#x29; CashIn from r_payment where type&#x3d;&#x27;IN_Loan&#x27; and cl_bank_acct_id&#x3c;&#x3e;18  group by r_loan_id&#x29;,&#xd;&#xa;	outloan as &#x28;select r_loan_id, sum&#x28;amount &#x2b; amount_additional&#x29; CashOut from r_payment where type&#x3d;&#x27;OUT_Loan&#x27; group by r_loan_id&#x29;,&#xd;&#xa;	firstd as &#x28;select l.r_loan_id, MIN&#x28;pay.payment_date&#x29;&#x3a;&#x3a;date CashIn_FirstD from r_payment pay join r_loan l on &#x28; pay.r_loan_id&#x3d;l.r_loan_id and pay.type&#x3d;&#x27;IN_Loan&#x27; and pay.cl_bank_acct_id&#x3c;&#x3e;18 &#x29; group by l.r_loan_id&#x29;,&#xd;&#xa;	firstdall as &#x28;select loa.r_loan_id, MIN&#x28;p1.payment_date&#x29;&#x3a;&#x3a;date CashIn_FirstD_All from r_payment p1 join r_loan loa on &#x28;p1.r_loan_id&#x3d;loa.r_loan_id and p1.type&#x3d;&#x27;IN_Loan&#x27; and p1.cl_bank_acct_id&#x3c;&#x3e;12&#x29; group by loa.r_loan_id&#x29;,&#xd;&#xa;	loans as &#x28;select i.r_loan_id, cashin, cashout,CashIn_FirstD,  CashIn_FirstD_All from inloan i join outloan o using &#x28;r_loan_id&#x29; join firstd f using &#x28;r_loan_id&#x29; join firstdall fa using &#x28;r_loan_id&#x29;&#x29;,&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;    &#xd;&#xa;        &#xd;&#xa;x02 as &#x28;	 SELECT lo.r_loan_id, lo.r_client_id, lo.r_application_id,substring&#x28;lo.loan_no from &#x27;..&#x24;&#x27;&#x29;&#x3a;&#x3a;numeric loan_seq,&#xd;&#xa;          lo.loan_date&#x3a;&#x3a;date, lo.term_days, lo.finished_Date&#x3a;&#x3a;date,&#xd;&#xa;          &#x28;lo.loan_date&#x3a;&#x3a;date&#x2b;lo.term_days&#x29;&#x3a;&#x3a;date due_Date,&#xd;&#xa;          lo.status, lo.loan_amount, lo.loan_commission, lo.loan_commission_non_free,lo.prolongated, &#xd;&#xa;          &#x28;case when lo.prolongated&#x3d;0 then 0 else 1 end&#x29; ext,&#xd;&#xa;          &#x28;case when lo.finished_date is null then 0 else 1 end&#x29; repaid,&#xd;&#xa;&#xd;&#xa;          &#x28;select count&#x28;p.r_payment_id&#x29; from r_payment p where p.r_loan_id&#x3d;lo.r_loan_id and p.cl_bank_acct_id&#x3d;18&#x29; ext_free,&#xd;&#xa;                       &#xd;&#xa;          EXTRACT&#x28;&#x27;YEAR&#x27; FROM AGE&#x28;lo.loan_date, &#x28;select cl.dob from r_client cl where cl.r_client_id&#x3d;lo.r_client_id&#x29; &#x29;&#x29;&#x3a;&#x3a;INTEGER Age,&#xd;&#xa;         &#x28;select score from r_application ap where ap.r_Application_id&#x3d;lo.r_application_id&#x29; Score,&#xd;&#xa;		 loa.CashIn,&#xd;&#xa;        loa.cashout,&#xd;&#xa;        loa.CashIn_FirstD,&#xd;&#xa;        loa.CashIn_FirstD_All&#xd;&#xa;&#xd;&#xa;          FROM r_loan Lo left outer join loans loa using&#x28;r_loan_id&#x29;&#xd;&#xa;         --where lo.r_loan_id in &#x28;164&#x29;&#xd;&#xa;         -- LIMIT 100&#xd;&#xa;          &#x29;&#xd;&#xa;    &#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;SELECT x02.&#x2a;  ,&#xd;&#xa;&#xd;&#xa;&#xd;&#xa; &#x28;CASE WHEN &#x28;x02.due_date&#x2b;0&#x29;&#x3e;current_Date then NULL&#xd;&#xa;		 WHEN x02.cashin_firstd_all IS NULL THEN 1&#xd;&#xa;		WHEN x02.finished_date&#x3c;&#x3d;&#x28;x02.due_date&#x2b;0&#x29; THEN 0&#xd;&#xa;        WHEN x02.finished_date&#x3e;&#x28;x02.due_date&#x2b;0&#x29; AND x02.cashin_firstd_all&#x3e;&#x28;x02.due_Date&#x2b;0&#x29; THEN 1&#xd;&#xa;        WHEN x02.finished_date&#x3e;&#x28;x02.due_date&#x2b;0&#x29; AND x02.cashin_firstd_all&#x3c;&#x3d;&#x28;x02.due_Date&#x2b;0&#x29; THEN 0&#xd;&#xa;        ELSE -1 END&#x29; FPD0,&#xd;&#xa;&#xd;&#xa;&#x28;CASE WHEN &#x28;x02.due_date&#x2b;5&#x29;&#x3e;current_Date then NULL&#xd;&#xa;		WHEN x02.cashin_firstd_all IS NULL THEN 1&#xd;&#xa;		WHEN x02.finished_date&#x3c;&#x3d;&#x28;x02.due_date&#x2b;5&#x29; THEN 0&#xd;&#xa;        WHEN x02.finished_date&#x3e;&#x28;x02.due_date&#x2b;5&#x29; AND x02.cashin_firstd_all&#x3e;&#x28;x02.due_Date&#x2b;5&#x29; THEN 1&#xd;&#xa;        WHEN x02.finished_date&#x3e;&#x28;x02.due_date&#x2b;5&#x29; AND x02.cashin_firstd_all&#x3c;&#x3d;&#x28;x02.due_Date&#x2b;5&#x29; THEN 0&#xd;&#xa;        ELSE -1 END&#x29; FPD5,&#xd;&#xa;&#xd;&#xa;&#x28;CASE WHEN &#x28;x02.due_date&#x2b;7&#x29;&#x3e;current_Date then NULL&#xd;&#xa;		WHEN x02.cashin_firstd_all IS NULL THEN 1&#xd;&#xa;		WHEN x02.finished_date is NULL AND x02.cashin_firstd_all&#x3c;&#x3d;&#x28;x02.due_Date&#x2b;7&#x29; THEN 0&#xd;&#xa;        WHEN x02.finished_date is NULL AND x02.cashin_firstd_all&#x3e;&#x28;x02.due_Date&#x2b;7&#x29; THEN 1&#xd;&#xa;		WHEN x02.finished_date&#x3c;&#x3d;&#x28;x02.due_date&#x2b;7&#x29; THEN 0&#xd;&#xa;        WHEN x02.finished_date&#x3e;&#x28;x02.due_date&#x2b;7&#x29; AND x02.cashin_firstd_all&#x3e;&#x28;x02.due_Date&#x2b;7&#x29; THEN 1&#xd;&#xa;        WHEN x02.finished_date&#x3e;&#x28;x02.due_date&#x2b;7&#x29; AND x02.cashin_firstd_all&#x3c;&#x3d;&#x28;x02.due_Date&#x2b;7&#x29; THEN 0&#xd;&#xa;        ELSE -1 END&#x29; FPD7,&#xd;&#xa;&#xd;&#xa;&#x28;CASE WHEN &#x28;x02.due_date&#x2b;10&#x29;&#x3e;current_Date then NULL&#xd;&#xa;		WHEN x02.cashin_firstd_all IS NULL THEN 1&#xd;&#xa;		WHEN x02.finished_date is NULL AND x02.cashin_firstd_all&#x3c;&#x3d;&#x28;x02.due_Date&#x2b;10&#x29; THEN 0&#xd;&#xa;        WHEN x02.finished_date is NULL AND x02.cashin_firstd_all&#x3e;&#x28;x02.due_Date&#x2b;10&#x29; THEN 1&#xd;&#xa;		WHEN x02.finished_date&#x3c;&#x3d;&#x28;x02.due_date&#x2b;10&#x29; THEN 0&#xd;&#xa;        WHEN x02.finished_date&#x3e;&#x28;x02.due_date&#x2b;10&#x29; AND x02.cashin_firstd_all&#x3e;&#x28;x02.due_Date&#x2b;10&#x29; THEN 1&#xd;&#xa;        WHEN x02.finished_date&#x3e;&#x28;x02.due_date&#x2b;10&#x29; AND x02.cashin_firstd_all&#x3c;&#x3d;&#x28;x02.due_Date&#x2b;10&#x29; THEN 0&#xd;&#xa;        ELSE -1 END&#x29; FPD10,&#xd;&#xa;&#xd;&#xa;&#x28;CASE WHEN &#x28;x02.due_date&#x2b;15&#x29;&#x3e;current_Date then NULL&#xd;&#xa;		WHEN x02.cashin_firstd_all IS NULL THEN 1&#xd;&#xa;		WHEN x02.finished_date is NULL AND x02.cashin_firstd_all&#x3c;&#x3d;&#x28;x02.due_Date&#x2b;15&#x29; THEN 0&#xd;&#xa;        WHEN x02.finished_date is NULL AND x02.cashin_firstd_all&#x3e;&#x28;x02.due_Date&#x2b;15&#x29; THEN 1&#xd;&#xa;		WHEN x02.finished_date&#x3c;&#x3d;&#x28;x02.due_date&#x2b;15&#x29; THEN 0&#xd;&#xa;        WHEN x02.finished_date&#x3e;&#x28;x02.due_date&#x2b;15&#x29; AND x02.cashin_firstd_all&#x3e;&#x28;x02.due_Date&#x2b;15&#x29; THEN 1&#xd;&#xa;        WHEN x02.finished_date&#x3e;&#x28;x02.due_date&#x2b;15&#x29; AND x02.cashin_firstd_all&#x3c;&#x3d;&#x28;x02.due_Date&#x2b;15&#x29; THEN 0&#xd;&#xa;        ELSE -1 END&#x29; FPD15,&#xd;&#xa;&#xd;&#xa;&#x28;CASE WHEN &#x28;x02.due_date&#x2b;30&#x29;&#x3e;current_Date then NULL&#xd;&#xa;		WHEN x02.cashin_firstd_all IS NULL THEN 1&#xd;&#xa;		WHEN x02.finished_date is NULL AND x02.cashin_firstd_all&#x3c;&#x3d;&#x28;x02.due_Date&#x2b;30&#x29; THEN 0&#xd;&#xa;        WHEN x02.finished_date is NULL AND x02.cashin_firstd_all&#x3e;&#x28;x02.due_Date&#x2b;30&#x29; THEN 1&#xd;&#xa;		WHEN x02.finished_date&#x3c;&#x3d;&#x28;x02.due_date&#x2b;30&#x29; THEN 0&#xd;&#xa;        WHEN x02.finished_date&#x3e;&#x28;x02.due_date&#x2b;30&#x29; AND x02.cashin_firstd_all&#x3e;&#x28;x02.due_Date&#x2b;30&#x29; THEN 1&#xd;&#xa;        WHEN x02.finished_date&#x3e;&#x28;x02.due_date&#x2b;30&#x29; AND x02.cashin_firstd_all&#x3c;&#x3d;&#x28;x02.due_Date&#x2b;30&#x29; THEN 0&#xd;&#xa;        ELSE -1 END&#x29; FPD30&#xd;&#xa;from x02&#xd;&#xa;&#xd;&#xa;--where r_loan_id &#x3d; &#x3f;</sql>
