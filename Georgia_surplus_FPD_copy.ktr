    <sql>&#xd;&#xa;&#xd;&#xa;with dd as &#x28;select d.r_loan_id,&#xd;&#xa;sum&#x28;d.amount&#x29; as amount, sum&#x28;d.amount_principal&#x29; as amount_principal, &#xd;&#xa;       sum&#x28;d.amount_commission&#x29; as amount_commission, sum&#x28;d.amount_penalty&#x29; as amount_penalty, sum&#x28;d.amount_surplus&#x29; as amount_surplus,&#xd;&#xa;        sum&#x28;d.amount_prolongation&#x29; as amount_prolongation, sum&#x28;d.amount_fees&#x29; as amount_fees&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;from &#xd;&#xa;r_payment_distr d join r_payment p on  &#x28;d.r_payment_id &#x3d; p.r_payment_id&#x29; &#xd;&#xa;where 1&#x3d;1 &#xd;&#xa;	&#xd;&#xa;	group by d.r_loan_id&#x29;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;,clci as &#x28;&#xd;&#xa;	 select &#x2a; from &#x28;select distinct r_client_id, id1, id2,&#xd;&#xa; decision, &#xd;&#xa; app_date,&#xd;&#xa; r_credit_info_id,&#xd;&#xa; rank&#x28;&#x29; over &#x28;partition by r_client_id, id1 order by r_client_id, id1, decision DESC, r_credit_info_id DESC &#x29; as rrow from&#xd;&#xa;&#x28;select  rra.r_client_id&#xd;&#xa;	,rra.r_application_id as id1&#xd;&#xa;	, raa.r_application_id as id2&#xd;&#xa;	,r_credit_info_id&#xd;&#xa;	&#xd;&#xa;	,case when decision &#x3d; &#x27;ERROR&#x27; then &#x27;ERROR&#x27;&#xd;&#xa;		 when decision is null then &#x27;no info&#x27;&#xd;&#xa;		else &#x27;ok&#x27; end as decision&#xd;&#xa;			,rra.app_date&#x3a;&#x3a;date&#xd;&#xa;	&#xd;&#xa;&#xd;&#xa;	from&#xd;&#xa;	r_application rra join r_application raa on &#x28;rra.r_client_id &#x3d; raa.r_client_id&#x29;&#xd;&#xa;	left outer join r_credit_info ci on &#x28;raa.r_application_id &#x3d; ci.r_application_id&#x29;&#xd;&#xa;&#xd;&#xa;	where 1&#x3d;1 &#xd;&#xa;	&#xd;&#xa;&#xd;&#xa;	and raa.r_application_id &#x3c;&#x3d; rra.r_application_id&#xd;&#xa;	and decision  &#x3c;&#x3e; &#x27;no info&#x27;&#xd;&#xa;	order by rra.r_client_id, rra.r_application_id, raa.r_application_id DESC&#x29;aa&#xd;&#xa;&#xd;&#xa;	order by id1, decision DESC&#x29;cc&#xd;&#xa;	where rrow &#x3d;1&#x29;&#xd;&#xa;	&#xd;&#xa;&#xd;&#xa;SELECT distinct l.r_loan_id, l.loan_Date&#x3a;&#x3a;date,l.status, substring&#x28;l.loan_no from &#x27;..&#x24;&#x27;&#x29;&#x3a;&#x3a;numeric loan_seq, l.loan_amount, l.loan_commission,&#xd;&#xa;&#xd;&#xa;dd.amount, dd.amount_principal, &#xd;&#xa;       dd.amount_commission, dd.amount_penalty, dd.amount_surplus, dd.amount_prolongation, dd.amount_fees&#xd;&#xa;&#xd;&#xa;       ,&#xd;&#xa;       a.amount as application_amount,&#xd;&#xa;       a.original_amount as original_application_amount,&#xd;&#xa;       a.term,&#xd;&#xa;       a.original_term,&#xd;&#xa;       a.ip_address,&#xd;&#xa;       a.score&#xd;&#xa;       &#xd;&#xa;&#xd;&#xa;&#xd;&#xa;,c.r_client_id&#xd;&#xa;,c.gender&#xd;&#xa;,c.dob&#xd;&#xa;,c.income_amount&#xd;&#xa;,c.liabilities_amount&#xd;&#xa;,c.employment_type&#xd;&#xa;,c.mobile_operator&#xd;&#xa;,c.actual_address_city&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;,ci.decision&#xd;&#xa;,ci.reason&#xd;&#xa;,ci.probability_of_default&#xd;&#xa;,ci.inquiries_last_1_month&#xd;&#xa;,ci.inquiries_last_3_month&#xd;&#xa;,ci.inquiries_last_6_month&#xd;&#xa;,ci.inquiries_last_12_month&#xd;&#xa;,ci.debtor_existing_operations&#xd;&#xa;,ci.debtor_terminated_operations&#xd;&#xa;,ci.guarantor_existing_operations&#xd;&#xa;,ci.guarantor_terminated_operations&#xd;&#xa;,ci.guarantor_unpaid_instalments&#xd;&#xa;,ci.debtor_total_overdue&#xd;&#xa;,ci.debtor_total_outstanding&#xd;&#xa;,ci.r_credit_info_id&#xd;&#xa;,ci.r_application_id as ci_application_id&#xd;&#xa;,a.r_application_id as loan_application_id&#xd;&#xa;,ci.created&#x3a;&#x3a;date&#xd;&#xa;&#xd;&#xa;,&#x28;l.loan_date&#x3a;&#x3a;date&#x2b;l.term_days&#x29;&#x3a;&#x3a;date due_Date&#xd;&#xa;,l.finished_Date&#x3a;&#x3a;date&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;  FROM r_loan l join  r_application a on &#x28;a.r_application_id &#x3d; l.r_application_id&#x29; left outer join clci clci on &#x28;a.r_application_id &#x3d; clci.id1&#x29;&#xd;&#xa;	left outer join r_credit_info ci on &#x28;clci.r_credit_info_id &#x3d; ci.r_credit_info_id&#x29; left outer join r_credit_info_negative cn on &#x28;ci.r_credit_info_id &#x3d; cn.r_credit_info_id&#x29;&#xd;&#xa;	join r_client c on &#x28;l.r_client_id &#x3d; c.r_client_id&#x29; &#xd;&#xa;	left outer join dd dd on &#x28;dd.r_loan_id &#x3d; l.r_loan_id&#x29;&#xd;&#xa;&#xd;&#xa;where 1&#x3d;1&#xd;&#xa;and l.r_loan_id &#x3d; &#x3f;&#xd;&#xa;&#xd;&#xa;  &#xd;&#xa;&#xd;&#xa;&#xd;&#xa;order by r_loan_id</sql>
