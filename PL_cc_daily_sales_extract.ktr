    <sql>-- CRM tasks&#xd;&#xa;SELECT DISTINCT ON &#x28;c.r_application_id&#x29; c.crm_task_id, c.crm_task_status_id, &#xd;&#xa;s.name as task_status,&#xd;&#xa;c.r_client_id, c.r_application_id, c.crm_user_group_id, &#xd;&#xa;c.um_user_id,&#xd;&#xa;u.first_name,&#xd;&#xa;u.last_name,&#xd;&#xa;&#xd;&#xa; c.task_date&#x3a;&#x3a;date&#xd;&#xa;&#xd;&#xa;,extract&#x28;hour from c.task_date&#x29;&#x7c;&#x7c;&#x27;&#x3a;&#x27;&#x7c;&#x7c;extract&#x28;minutes from c.task_date&#x29; as task_time&#xd;&#xa;,current_date&#x3a;&#x3a;date as extraction_date&#xd;&#xa;,extract&#x28;hour from current_timestamp&#x29;&#x7c;&#x7c;&#x27;&#x3a;&#x27;&#x7c;&#x7c;extract&#x28;minutes from current_timestamp&#x29; as extraction_time&#xd;&#xa;,a.finished_loan_cnt&#xd;&#xa;,l.loan_amount&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;FROM crm_task c join um_user u on &#x28;c.um_user_id &#x3d; u.um_user_id&#x29; join crm_task_status s on &#x28;c.crm_task_status_id &#x3d; s.crm_task_status_id&#x29;&#xd;&#xa;left outer join r_application a  on &#x28;c.r_application_id &#x3d; a.r_application_id&#x29; &#xd;&#xa;left outer join r_loan l on &#x28;l.r_application_id &#x3d; a.r_application_id&#x29;&#xd;&#xa;&#xd;&#xa;WHERE c.crm_task_type_id&#x3d;1 &#xd;&#xa;&#xd;&#xa;and c.um_user_id not in &#x28;10&#x29;&#xd;&#xa;ORDER BY c.r_application_id, c.task_date DESC NULLS LAST&#xd;&#xa;&#xd;&#xa;--limit 100</sql>
