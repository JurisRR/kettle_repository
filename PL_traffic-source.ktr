    <connection>Poland</connection>
    <sql>Select &#xd;&#xa;--&#x23;&#x23;&#x23;&#x23; data from applications&#xd;&#xa;a.r_application_id&#xd;&#xa;,a.app_date&#x3a;&#x3a;date&#xd;&#xa;,extract&#x28;week from a.app_date&#x29; as app_iso_week&#xd;&#xa;,to_char&#x28;a.app_date, &#x27;day&#x27;&#x29; as app_day&#xd;&#xa;,a.r_client_id&#xd;&#xa;,a.amount&#xd;&#xa;,a.status as app_status&#xd;&#xa;,c.status as client_status&#xd;&#xa;,a.reject_reason&#xd;&#xa;,a.score&#xd;&#xa;,a.finished_loan_cnt&#xd;&#xa;,extract&#x28;week from a.app_date&#x29; as week_nr&#xd;&#xa;,extract&#x28;isodow from a.app_date&#x29; as day_nr&#xd;&#xa;,to_char&#x28;a.app_date, &#x27;day&#x27;&#x29; as day&#xd;&#xa;,case when a.app_date&#x3a;&#x3a;date between current_date-7 and current_date-1 then &#x27;this week&#x27; &#xd;&#xa;	 when a.app_date&#x3a;&#x3a;date between current_date-14 and current_date-8 then &#x27;previous week&#x27; &#xd;&#xa;	else &#x27;history&#x27; end as week&#xd;&#xa;--&#x23;&#x23;&#x23;&#x23; data from loans&#xd;&#xa;,l.r_loan_id&#xd;&#xa;,substring&#x28;l.loan_no from &#x27;..&#x24;&#x27;&#x29;&#x3a;&#x3a;numeric loan_seq&#xd;&#xa;,case when substring&#x28;l.loan_no from &#x27;..&#x24;&#x27;&#x29;&#x3a;&#x3a;numeric &#x3d; 1 then &#x27;first&#x27; &#xd;&#xa;	when substring&#x28;l.loan_no from &#x27;..&#x24;&#x27;&#x29;&#x3a;&#x3a;numeric &#x3e; 1 then &#x27;repeated&#x27; else null end as loan_type&#xd;&#xa;&#xd;&#xa;,l.status as loan_status&#xd;&#xa;,l.term_days&#xd;&#xa;,l.loan_amount&#xd;&#xa;,l.loan_commission&#xd;&#xa;,case when l.prolongated &#x3d; 0 then 0 &#xd;&#xa;	when l.prolongated &#x3e;0 then 1 &#xd;&#xa;	else null end as prolongated&#xd;&#xa;--&#x23;&#x23;&#x23;&#x23; data from cms&#xd;&#xa;&#xd;&#xa;--,lp.name&#xd;&#xa;,case when c.cms_landing_page_id is null then &#x27;direct&#x27;&#xd;&#xa;	when lp.name in &#x28;&#x27;Affiliate44&#x27;, &#x27;CityAds&#x27;, &#x27;doAffiliate&#x27;, &#x27;Google&#x27;, &#x27;Tmefekt&#x27;, &#x27;Money.pl&#x27;, &#x27;Credy&#x27;, &#x27;Adtraction&#x27;&#x29; then lp.name&#xd;&#xa;else &#x27;other&#x27; end as traffic_source&#xd;&#xa;&#xd;&#xa;,case when lp.name in &#x28;&#x27;Affiliate44&#x27;, &#x27;CityAds&#x27;, &#x27;doAffiliate&#x27;,  &#x27;Tmefekt&#x27;, &#x27;Money.pl&#x27;, &#x27;Credy&#x27;, &#x27;Adtraction&#x27;&#x29; and a.status &#x3d; &#x27;FIN_CONFIRMED&#x27; then 87&#xd;&#xa;else 0 end as affiliate_costs_pln--&#x3a;&#x3a;numeric&#xd;&#xa;&#xd;&#xa;,case when c.cms_landing_page_id is null then &#x27;direct&#x27; else lp.name end as traffic_source_original 	&#xd;&#xa;&#xd;&#xa;,case when mod&#x28;substring&#x28;c.personal_no from 10 for 1&#x29;&#x3a;&#x3a;numeric, 2&#x29; &#x3d; 0 then &#x27;Female&#x27; &#xd;&#xa;	else &#x27;Male&#x27; end as gender&#xd;&#xa;,extract&#x28;years from age&#x28;c.dob&#x29;&#x29; as age  --&#x23;&#x23;&#x23; &#xd;&#xa;,div&#x28;extract&#x28;years from age&#x28;c.dob&#x29;&#x29;&#x3a;&#x3a;int-1,5&#x29;&#x2a;5&#x2b;1 &#x7c;&#x7c;&#x27;-&#x27;&#x7c;&#x7c; div&#x28;extract&#x28;years from age&#x28;c.dob&#x29;&#x29;&#x3a;&#x3a;int-1,5&#x29;&#x2a;5&#x2b;5 as age_group&#xd;&#xa;&#xd;&#xa;,&#x27;poland&#x27; as country&#xd;&#xa;&#xd;&#xa;, case when a.r_application_id &#x3d; fa.first_app then &#x27;first&#x27; else &#x27;repeated&#x27; end as app_type&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;from r_application a left outer join r_loan l on &#x28;a.r_application_id&#x3d; l.r_application_id&#x29; &#xd;&#xa;join r_client c on &#x28;a.r_client_id &#x3d; c.r_client_id&#x29;&#xd;&#xa;left outer join cms_landing_page lp on &#x28;c.cms_landing_page_id &#x3d; lp.cms_landing_page_id&#x29;&#xd;&#xa;join &#x28;select r_client_id, min&#x28;r_application_id&#x29; as first_app from r_application group by r_client_id&#x29;fa on &#x28;fa.r_client_id &#x3d; a.r_client_id&#x29;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;where a.app_date&#x3a;&#x3a;date between &#x27;2017-01-02&#x27; --current_date-14 &#x23;&#x23; changed on 26.02. due to request from Toms&#xd;&#xa;and current_date-1&#xd;&#xa;&#xd;&#xa;order by r_application_id</sql>
