    <sql>select &#x2a; from &#x28;SELECT &#xd;&#xa;a.created_at&#x3a;&#x3a;date as activity_date_old&#xd;&#xa;,case when la.created_at&#x3a;&#x3a;date is null &#xd;&#xa;	then ww.min_reg_time&#x3a;&#x3a;date&#xd;&#xa;&#xd;&#xa;	else la.created_at&#x3a;&#x3a;date end	as activity_date -- called this way so that Marcis doesn&#x27;t need to change in tableau&#xd;&#xa;&#xd;&#xa;,extract&#x28;hour from la.created_at&#x29;&#x7c;&#x7c;&#x27;&#x3a;&#x27;&#x7c;&#x7c;extract&#x28;minutes from la.created_at&#x29; as activity_time&#xd;&#xa;--,cast&#x28;la.created_at as time&#x29; as activity_time&#xd;&#xa;&#xd;&#xa;,a.&#x2a;&#xd;&#xa;,w.&#x2a;&#xd;&#xa;, lla.&#x2a;&#xd;&#xa;, t.&#x2a;&#xd;&#xa;,w.name as current_workflow&#xd;&#xa;--,bav.next_activity&#xd;&#xa;&#xd;&#xa;,rok.max_reg_ok_w_id&#xd;&#xa;,rok.count_ok_reg_w&#xd;&#xa;,wok.max_ok_w_id&#xd;&#xa;,wok.count_ok_w&#xd;&#xa;,case when aok.auto_ok is null then &#x27;no auto&#x27; else &#x27;autocomplete triggered&#x27; end as auto&#xd;&#xa;,pnok.first_personal_aid&#xd;&#xa;,pnok.count_personal&#xd;&#xa;,pc.cancelled_personal&#xd;&#xa;,pc.cancelled_personal_cnt&#xd;&#xa;,case when snok.snok_id is null then &#x27;false&#x27; else &#x27;true&#x27; end as sales_call_not_ok&#xd;&#xa;,att.selfie_status&#xd;&#xa;,la.close_reason as application_close_reason&#xd;&#xa;,l.status as loan_status&#xd;&#xa;,l.loans_paid&#xd;&#xa;,l.issue_date&#x3a;&#x3a;date as loan_issue_date&#xd;&#xa;&#xd;&#xa;--&#x23;&#x23; Marcis criteria to decrease file size, added on 12 may 2017 &#xd;&#xa;, case when &#x28;w.name in &#x28;&#x27;UnderwritingFirstLoan&#x27;, &#x27;UnderwritingFirstLoan_v2&#x27;&#x29; and a.id &#x3d; wok.max_ok_w_id &#x29; then 1&#xd;&#xa;		when &#x28;w.name in &#x28;&#x27;Registration&#x27;, &#x27;Registration_v2&#x27;&#x29; and &#x28;a.id &#x3d; rok.max_reg_ok_w_id or &#x28;a.id &#x3d; pnok.first_personal_aid and a.name &#x3d; &#x27;Personal&#x27; and a.status in &#x28;&#x27;CANCELLED&#x27;, &#x27;ACTIVE&#x27;&#x29;&#x29;&#x29;&#x29; then 1&#xd;&#xa; else 0 end as filter &#xd;&#xa;&#xd;&#xa;&#xd;&#xa;  FROM workflow.activity a join workflow.workflow w &#xd;&#xa;on &#x28;a.workflow_id&#x3d;w.id&#x29; &#xd;&#xa;&#xd;&#xa;left outer join &#x28;select client_id, count&#x28;client_id&#x29; as application_count, count&#x28;loan_id&#x29; as loan_count, max&#x28;loans_paid&#x29; as loans_paid&#xd;&#xa; from lending.loan_application &#xd;&#xa;--where created_at &#x3c;&#x3d; should I select those applications created before current&#x3f;&#xd;&#xa; group by client_id&#x29;lla on w.client_id &#x3d; lla.client_id&#xd;&#xa;&#xd;&#xa;left outer join task.task t on &#x28;t.activity_id &#x3d; a.id&#x29;&#xd;&#xa;&#xd;&#xa;left outer join &#x28;select max&#x28;a.id&#x29; as max_reg_ok_w_id, count&#x28;a.id&#x29; as count_ok_reg_w, a.workflow_id from workflow.activity a join workflow.workflow w on &#x28;a.workflow_id&#x3d;w.id&#x29; &#xd;&#xa;	where 1&#x3d;1&#xd;&#xa;	and w.name like &#x27;Registration&#x25;&#x27;&#xd;&#xa;	and a.actor &#x3d; &#x27;CLIENT&#x27;&#xd;&#xa;	and a.name not in &#x28;&#x27;ThreatMetrix&#x27;,&#x27;SalesCall&#x27;, &#x27;AutoCompleteRegistration&#x27;&#x29;&#xd;&#xa;	and a.resolution &#x3d; &#x27;OK&#x27;&#xd;&#xa;	group by a.workflow_id&#x29;rok on &#x28;rok.workflow_id &#x3d; a.workflow_id&#x29;&#xd;&#xa;&#xd;&#xa;left outer join &#x28;select max&#x28;a.id&#x29; as max_ok_w_id, count&#x28;a.id&#x29; as count_ok_w, a.workflow_id from workflow.activity a join workflow.workflow w on &#x28;a.workflow_id&#x3d;w.id&#x29; &#xd;&#xa;	where 1&#x3d;1&#xd;&#xa;	and a.actor &#x3d; &#x27;CLIENT&#x27;&#xd;&#xa;	and a.name not in &#x28;&#x27;ThreatMetrix&#x27;,&#x27;SalesCall&#x27;, &#x27;AutoCompleteRegistration&#x27;&#x29;&#xd;&#xa;	and a.resolution &#x3d; &#x27;OK&#x27;&#xd;&#xa;	group by a.workflow_id&#x29;wok on &#x28;wok.workflow_id &#x3d; a.workflow_id&#x29;&#xd;&#xa;&#xd;&#xa;left outer join &#x28;select a.id as auto_ok, a.workflow_id from workflow.activity a join workflow.workflow w on &#x28;a.workflow_id&#x3d;w.id&#x29; &#xd;&#xa;	where 1&#x3d;1&#xd;&#xa;	and a.name &#x3d; &#x28; &#x27;AutoCompleteRegistration&#x27;&#x29;&#xd;&#xa;	and a.status &#x3d; &#x27;COMPLETED&#x27;&#xd;&#xa;&#xd;&#xa;	&#x29;aok on &#x28;aok.workflow_id &#x3d; a.workflow_id&#x29;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;left outer join &#x28;select min&#x28;a.id&#x29; as first_personal_aid, count&#x28;a.id&#x29; as count_personal, w.client_id from workflow.activity a join workflow.workflow w on &#x28;a.workflow_id&#x3d;w.id&#x29; &#xd;&#xa;	where 1&#x3d;1&#xd;&#xa;	and w.name like &#x27;Registration&#x25;&#x27;&#xd;&#xa;	and a.name &#x3d; &#x27;Personal&#x27;&#xd;&#xa;	--and a.status in &#x28;&#x27;ACTIVE&#x27;,&#x27;CANCELLED&#x27;&#x29;&#xd;&#xa;	&#xd;&#xa;	group by w.client_id&#x29;pnok on &#x28;pnok.client_id &#x3d; w.client_id&#x29;&#xd;&#xa;&#xd;&#xa;left outer join &#x28;select min&#x28;a.id&#x29; as cancelled_personal, count&#x28;a.id&#x29; as cancelled_personal_cnt, w.client_id from workflow.activity a join workflow.workflow w on &#x28;a.workflow_id&#x3d;w.id&#x29; &#xd;&#xa;	where 1&#x3d;1&#xd;&#xa;	and w.name &#x3d; &#x27;Registration&#x27;&#xd;&#xa;	and a.name &#x3d; &#x27;Personal&#x27;&#xd;&#xa;	and a.status in &#x28;&#x27;ACTIVE&#x27;,&#x27;CANCELLED&#x27;&#x29;&#xd;&#xa;	&#xd;&#xa;	group by w.client_id&#x29;pc on &#x28;pc.client_id &#x3d; w.client_id&#x29;&#xd;&#xa;&#xd;&#xa;left outer join &#x28;select a.id as snok_id, a.workflow_id from workflow.activity a join workflow.workflow w on &#x28;a.workflow_id&#x3d;w.id&#x29; &#xd;&#xa;	where 1&#x3d;1&#xd;&#xa;	and a.name &#x3d; &#x28; &#x27;SalesCall&#x27;&#x29;&#xd;&#xa;	and a.status &#x3d; &#x27;COMPLETED&#x27;&#xd;&#xa;	and a.resolution &#x3d; &#x27;NOT_OK&#x27;&#xd;&#xa;&#xd;&#xa;	&#x29;snok on &#x28;snok.workflow_id &#x3d; a.workflow_id&#x29;&#xd;&#xa;&#xd;&#xa;left outer join &#x28;select client_id, status as selfie_status from crm.client_attachment where attachment_type &#x3d; &#x27;selfie&#x27;&#x29;att on &#x28;w.client_id &#x3d; att.client_id&#x29;&#xd;&#xa; &#xd;&#xa;&#xd;&#xa;--left outer join backoffice.loan_application_view bav on &#x28;bav.id &#x3d; w.application_id&#x29;&#xd;&#xa;&#xd;&#xa;left outer join lending.loan_application la on &#x28;la.id &#x3d; w.application_id&#x29;&#xd;&#xa;left outer join lending.loan l on &#x28;la.id &#x3d; l.loan_application_id&#x29;&#xd;&#xa;left outer join &#x28;select min&#x28;a.created_at&#x3a;&#x3a;Date&#x29; as min_reg_time, w.id, w.client_id from workflow.activity a join workflow.workflow w on&#x28;a.workflow_id&#x3d;w.id&#x29;&#xd;&#xa;		where w.name like &#x27;Registration&#x25;&#x27; group by w.id, w.client_id&#x29;ww on &#x28;ww.id &#x3d; w.id&#x29;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;where w.name not in &#x28;&#x27;Extension&#x27;, &#x27;ProfileRenewal&#x27;, &#x27;UnderwritingRepeatedLoan&#x27;, &#x27;UnderwritingRepeatedLoanWithRenewal&#x27;&#x29;&#xd;&#xa;--and a.name not in &#x28;&#x27;RemindAboutUnfinishedRegistration&#x27;&#x29;&#xd;&#xa;&#xd;&#xa;&#x29;file where filter &#x3d; 1&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;</sql>
