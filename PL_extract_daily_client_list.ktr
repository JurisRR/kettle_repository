    <connection>PL_Slave_Finotech</connection>
    <sql>Select email as blocked_email&#xd;&#xa;,case when stt.status is not null then &#x27;blocked&#x27;&#xd;&#xa;when str.status is not null then &#x27;blocked&#x27;&#xd;&#xa;when eca.email ilike &#x27;&#x25;blocked&#x25;&#x27; then &#x27;blocked&#x27;&#xd;&#xa;when bl.blacklist_entries is not null then &#x27;blocked&#x27;&#xd;&#xa;when pi.receive_communication &#x3d; FALSE then &#x27;blocked&#x27;&#xd;&#xa;when pi.receive_offers &#x3d; FALSE then &#x27;blocked&#x27;&#xd;&#xa;else null end as blocked_status&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;from crm.email_contact_audit eca-- crm.email_contact ec&#xd;&#xa;&#xd;&#xa;left outer join crm.client c on &#x28;eca.client_id &#x3d; c.id&#x29;&#xd;&#xa;left outer join crm.personal_info pi on &#x28;pi.client_id &#x3d; eca.id&#x29;&#xd;&#xa;left outer join &#x28;select distinct client_id, status from crm.status st where st.status &#x3d; &#x27;communicationBlocked&#x27;&#x29;stt on &#x28;stt.client_id &#x3d; eca.id&#x29;&#xd;&#xa;left outer join &#x28;select distinct client_id, status from crm.status st where st.status &#x3d; &#x27;resigned&#x27;&#x29;str on &#x28;str.client_id &#x3d; eca.id&#x29;&#xd;&#xa;left outer join &#x28;select client_id, count&#x28;id&#x29; as blacklist_entries, max&#x28;time_until&#x29;&#x3a;&#x3a;date as max_blacklist_due  from blacklist.blacklist_entry &#xd;&#xa;where active &#x3d; TRUE and &#x28;time_until&#x3a;&#x3a;date &#x3e; &#x28;current_date &#x2b; 30&#x29; or time_until is null&#x29; &#xd;&#xa;group by client_id&#x29;bl on &#x28;bl.client_id&#x3d;eca.client_id&#x29;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;union&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#x28;Select email as blocked_email&#xd;&#xa;,case when stt.status is not null then &#x27;blocked&#x27;&#xd;&#xa;when str.status is not null then &#x27;blocked&#x27;&#xd;&#xa;when eca.email ilike &#x27;&#x25;blocked&#x25;&#x27; then &#x27;blocked&#x27;&#xd;&#xa;when bl.blacklist_entries is not null then &#x27;blocked&#x27;&#xd;&#xa;when pi.receive_communication &#x3d; FALSE then &#x27;blocked&#x27;&#xd;&#xa;when pi.receive_offers &#x3d; FALSE then &#x27;blocked&#x27;&#xd;&#xa;else null end as blocked_status&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;from crm.email_contact eca-- crm.email_contact ec&#xd;&#xa;&#xd;&#xa;left outer join crm.client c on &#x28;eca.client_id &#x3d; c.id&#x29;&#xd;&#xa;left outer join crm.personal_info pi on &#x28;pi.client_id &#x3d; eca.id&#x29;&#xd;&#xa;left outer join &#x28;select distinct client_id, status from crm.status st where st.status &#x3d; &#x27;communicationBlocked&#x27;&#x29;stt on &#x28;stt.client_id &#x3d; eca.id&#x29;&#xd;&#xa;left outer join &#x28;select distinct client_id, status from crm.status st where st.status &#x3d; &#x27;resigned&#x27;&#x29;str on &#x28;str.client_id &#x3d; eca.id&#x29;&#xd;&#xa;left outer join &#x28;select client_id, count&#x28;id&#x29; as blacklist_entries, max&#x28;time_until&#x29;&#x3a;&#x3a;date as max_blacklist_due  from blacklist.blacklist_entry &#xd;&#xa;where active &#x3d; TRUE and &#x28;time_until&#x3a;&#x3a;date &#x3e; &#x28;current_date &#x2b; 30&#x29; or time_until is null&#x29; &#xd;&#xa;group by client_id&#x29;bl on &#x28;bl.client_id&#x3d;eca.client_id&#x29;&#xd;&#xa;&#x29;&#xd;&#xa;&#xd;&#xa;</sql>
