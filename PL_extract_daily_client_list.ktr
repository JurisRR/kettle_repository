    <connection>PL_Slave_Finotech</connection>
    <sql>select&#xd;&#xa;distinct&#xd;&#xa;c.id as client_id&#xd;&#xa;,c.client_number&#xd;&#xa;,ec.email&#xd;&#xa;,phcon.local_number as mobile&#xd;&#xa;,pi.first_name as &#x22;First Name&#x22;&#xd;&#xa;,pi.last_name  as &#x22;Last Name&#x22;&#xd;&#xa;, extract&#x28;years from age&#x28;pi.date_of_birth&#x29;&#x29; as age  &#xd;&#xa;,case when pi.date_of_birth&#x3a;&#x3a;date between &#x27;1900-01-01&#x27; and current_date then pi.date_of_birth&#x3a;&#x3a;date else null end as date_of_birth&#xd;&#xa;,coalesce&#x28;cl.loan_cnt, 0&#x29; as loan_cnt&#xd;&#xa;,coalesce&#x28;cal.act_loan_cnt,0&#x29; as act_loan_cnt&#xd;&#xa;,case when t.id is null then &#x27;Unidentified&#x27; else &#x27;Identified&#x27; end as cl_status&#xd;&#xa;,coalesce&#x28;cra.rej_app_cnt,0&#x29; as rej_app_cnt_2&#xd;&#xa;,coalesce&#x28;cra30.rej_last_month, 0&#x29; as rej_last_month&#xd;&#xa;,last_loan_date&#xd;&#xa;,lll.close_date&#x3a;&#x3a;Date as last_loan_close_date&#xd;&#xa;,last_loan_maturity_date&#xd;&#xa;,coalesce&#x28;case when lll.close_date is null and cal.act_loan_cnt is not null then current_date - last_loan_maturity_date&#xd;&#xa; else lll.close_date&#x3a;&#x3a;date - last_loan_maturity_date end,0&#x29; as last_loan_dpd&#xd;&#xa;,coalesce&#x28;max_loan_amount,0&#x29; as max_loan_amount&#xd;&#xa;,coalesce&#x28;aa.app_cnt,0&#x29; as app_cnt&#xd;&#xa;,aa.last_app_date&#xd;&#xa;,current_date - aa.last_app_date as days_since_last_app&#xd;&#xa;,current_date - lll.close_date&#x3a;&#x3a;date as days_since_last_paid_loan&#xd;&#xa;,current_date - c.created_at&#x3a;&#x3a;Date as days_since_registration&#xd;&#xa;,pi.receive_offers&#xd;&#xa;,ss.scoring as last_score&#xd;&#xa;,msa.score&#xd;&#xa;&#xd;&#xa;,lll.principal_disbursed as last_loan_amount&#xd;&#xa;,el.email as email_login&#xd;&#xa;,stt.status&#xd;&#xa;,pi.gender&#xd;&#xa;,ca.json_value&#x3a;&#x3a;json-&#x3e;&#x3e;&#x27;acceptPartnerMarketingPersonalData&#x27; as opoqa_flag&#xd;&#xa;,case when last_app_date &#x3e; &#x27;2017-12-01&#x27; then true else false end as opoqa_rep_flag&#xd;&#xa;,case when last_app_date &#x3e; &#x27;2017-12-01&#x27; then true&#xd;&#xa;	when ca.json_value&#x3a;&#x3a;json-&#x3e;&#x3e;&#x27;acceptPartnerMarketingPersonalData&#x27; &#x3d; &#x27;true&#x27; then true else false end as opoqa_combined_flag&#xd;&#xa;--,cras.rej_app_since_last_loan&#xd;&#xa;&#xd;&#xa;,case when aff.client_id is null then 0 else 1 end as affiliate_small_loan &#xd;&#xa;,case when last_app_date between &#x27;2017-11-01&#x27; and &#x27;2018-04-19&#x27; then 1 else 0 end as mandatory_checkbox&#xd;&#xa;,coalesce&#x28;cra2.rej_app_cnt,0&#x29; as rej_app_cnt&#xd;&#xa;,coalesce&#x28;ca.json_value&#x3a;&#x3a;json-&#x3e;&#x3e;&#x27;acceptMarketingPersonalData&#x27;,&#x27;true&#x27;&#x29; as acceptMarketingPersonalData&#xd;&#xa;,coalesce&#x28;ca.json_value&#x3a;&#x3a;json-&#x3e;&#x3e;&#x27;acceptAFUseTTEAndAutomationSystem&#x27;,&#x27;true&#x27;&#x29; as acceptAFUseTTEAndAutomationSystem&#xd;&#xa;,coalesce&#x28;ca.json_value&#x3a;&#x3a;json-&#x3e;&#x3e;&#x27;acceptMarketingProfiling&#x27;,&#x27;false&#x27;&#x29; as acceptMarketingProfiling&#xd;&#xa;,bl.max_blacklist_due&#xd;&#xa;&#xd;&#xa;,case when stt.status is not null then &#x27;blocked&#x27;&#xd;&#xa;when str.status is not null then &#x27;blocked&#x27;&#xd;&#xa;when ec.email like &#x27;&#x25;blocked&#x25;&#x27; then &#x27;blocked&#x27;&#xd;&#xa;when bl.blacklist_entries is not null then &#x27;blocked&#x27;&#xd;&#xa;when pi.receive_communication &#x3d; FALSE then &#x27;blocked&#x27;&#xd;&#xa;when pi.receive_offers &#x3d; FALSE then &#x27;blocked&#x27;&#xd;&#xa;else null end as blocked_status&#xd;&#xa;&#xd;&#xa;,case when ecst.rrso25 &#x3e;&#x3d; 1 then &#x27;true&#x27; else &#x27;false&#x27; end as rrso25&#xd;&#xa;&#xd;&#xa;,laat.value as last_app_purpose&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;from crm.client c&#xd;&#xa;&#xd;&#xa;left outer join transaction.transaction t on &#x28;c.id &#x3d; t.client_id and transaction_type &#x3d; &#x27;FEE_PAYMENT&#x27; and transaction_sub_type &#x3d; &#x27;REGISTRATION_FEE&#x27; and voided &#x3d; false&#x29;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;left outer join &#x28;select count&#x28;id&#x29; as loan_cnt, max&#x28;issue_date&#x3a;&#x3a;date&#x29; as last_loan_date &#xd;&#xa;	,max&#x28;id&#x29; as last_loan_id&#xd;&#xa;	,max&#x28;maturity_date&#x3a;&#x3a;date&#x29; as last_loan_maturity_date&#xd;&#xa;	,max&#x28;principal_disbursed&#x29; as max_loan_amount&#xd;&#xa;	--,max&#x28;close_date&#x3a;&#x3a;date&#x29; as last_loan_close_date&#xd;&#xa;, client_id from lending.loan group by client_id&#x29;cl on &#x28;cl.client_id &#x3d; c.id&#x29;&#xd;&#xa;&#xd;&#xa;left outer join &#x28;select count&#x28;id&#x29; as act_loan_cnt, client_id from lending.loan where status &#x3d; &#x27;ACTIVE&#x27; group by client_id&#x29;cal  on &#x28;cal.client_id &#x3d; c.id&#x29;&#xd;&#xa;left outer join &#x28;select count&#x28;id&#x29; as act_loan_cnt, client_id from lending.loan where status &#x3d; &#x27;PAID&#x27; group by client_id&#x29;cal2  on &#x28;cal2.client_id &#x3d; c.id&#x29;&#xd;&#xa;&#xd;&#xa;left outer join &#x28;select count&#x28;id&#x29; as rej_app_cnt, client_id from lending.loan_application where &#x28;close_reason is not null and status &#x3d; &#x27;REJECTED&#x27;--and close_reason not ilike &#x27;&#x25;BAD_SCORING&#x25;&#x27;&#xd;&#xa;&#x29; group by client_id&#x29;cra  on &#x28;cra.client_id &#x3d; c.id&#x29;&#xd;&#xa;left outer join &#x28;select count&#x28;id&#x29; as rej_last_month, client_id from lending.loan_application where close_reason is not null and status &#x3d; &#x27;REJECTED&#x27; and application_date&#x3a;&#x3a;date &#x3e; &#x28;current_date-31&#x29; group by client_id&#x29;cra30  on &#x28;cra30.client_id &#x3d; c.id&#x29;&#xd;&#xa;left outer join &#x28;select count&#x28;id&#x29; as app_cnt,max&#x28;id&#x29; as last_id, max&#x28;application_date&#x3a;&#x3a;date&#x29; as last_app_date, client_id from lending.loan_application group by client_id&#x29;aa  on &#x28;aa.client_id &#x3d; c.id&#x29;&#xd;&#xa;&#xd;&#xa;left outer join &#x28;select count&#x28;id&#x29; as rej_app_cnt, client_id from lending.loan_application where &#x28;status &#x3d; &#x27;REJECTED&#x27; --and close_reason not ilike &#x27;&#x25;BAD_SCORING&#x25;&#x27;&#xd;&#xa;&#x29; group by client_id&#x29;cra2  on &#x28;cra2.client_id &#x3d; c.id&#x29;&#xd;&#xa;&#xd;&#xa;join crm.personal_info pi on &#x28;pi.client_id &#x3d; c.id&#x29;&#xd;&#xa;left outer join crm.phone_contact phcon on &#x28;phcon.client_id&#x3d;c.id and phcon.is_primary &#x3d; TRUE and phcon.phone_type &#x3d; 0&#x29;&#xd;&#xa;left outer join crm.email_contact ec on &#x28;ec.client_id&#x3d;c.id and ec.is_primary &#x3d; TRUE&#x29;&#xd;&#xa;&#xd;&#xa;left outer join &#x28;select max&#x28;id&#x29; as max_app_id, client_id from lending.loan_application group by client_id&#x29;map on &#x28;c.id &#x3d; map.client_id&#x29;&#xd;&#xa;&#xd;&#xa;left outer join lending.loan_application_attribute laat on &#x28;laat.application_id &#x3d; map.max_app_id and laat.key &#x3d; &#x27;loanPurpose&#x27;&#x29;&#xd;&#xa;&#xd;&#xa;left outer join crm.email_login el on &#x28;el.client_id&#x3d;c.id&#x29;&#xd;&#xa;&#xd;&#xa;left outer join &#x28;select client_id, count&#x28;id&#x29; as blacklist_entries, max&#x28;time_until&#x29;&#x3a;&#x3a;date as max_blacklist_due  from blacklist.blacklist_entry where active &#x3d; TRUE  group by client_id&#x29;bl on &#x28;bl.client_id&#x3d;c.id&#x29; --and time_until &#x3e; current_timestamp  removed as worked incorrectly&#xd;&#xa;&#xd;&#xa;left outer join &#x28;select &#x2a; from &#x28;select distinct loan_application_id, scoring, rank&#x28;&#x29; over &#x28;partition by loan_application_id &#xd;&#xa;  order by loan_application_id ASC, e.id DESC &#x29; as rrow  from scoring.scorecard_evaluation e join scoring.scorecard s on &#x28;e.scorecard_id &#x3d;  s.id and s.name like &#x27;CREDIT_BUREAU&#x25;&#x27;&#x29; &#xd;&#xa;where e.split_run_selected_result &#x3d; TRUE&#x29;aa where rrow &#x3d; 1&#xd;&#xa;--order by e.created_at desc limit 1&#xd;&#xa;&#x29;ss on &#x28;ss.loan_application_id&#x3d;aa.last_id&#x29;&#xd;&#xa;&#xd;&#xa;left outer join migration_source.r_application msa on &#x28;aa.last_id&#x3d;msa.r_application_id&#x29;&#xd;&#xa;left outer join lending.loan lll on &#x28;lll.id &#x3d; cl.last_loan_id&#x29;&#xd;&#xa;left outer join &#x28;select distinct client_id, status from crm.status st where st.status &#x3d; &#x27;communicationBlocked&#x27;&#x29;stt on &#x28;stt.client_id &#x3d; c.id&#x29;&#xd;&#xa;left outer join &#x28;select distinct client_id, status from crm.status st where st.status &#x3d; &#x27;resigned&#x27;&#x29;str on &#x28;str.client_id &#x3d; c.id&#x29;&#xd;&#xa;left outer join crm.client_attribute ca on &#x28;ca.key &#x3d; &#x27;registrationData&#x27; and ca.client_id &#x3d; c.id&#x29;&#xd;&#xa;&#xd;&#xa;left outer join &#x28;SELECT distinct client_id FROM scoring.scorecard_evaluation e join scoring.scorecard_variable_score s on &#x28;s.attribute_score_id &#x3d; e.id&#x29;&#xd;&#xa; where 1&#x3d;1  and e.scorecard_id &#x3d; 10  and s.name &#x3d; &#x27;AffiliateSmallLoan&#x27;  and s.scoring  &#x3d; -100&#x29;aff on &#x28;c.id &#x3d; aff.client_id&#x29;&#xd;&#xa;&#xd;&#xa;left outer join &#x28;select distinct client_id, count&#x28;id&#x29; as rrso25  from crm.status where status &#x3d; &#x27;existingClient&#x27; group by 1&#x29;ecst on &#x28;c.id &#x3d; ecst.client_id&#x29; &#xd;&#xa;&#xd;&#xa;&#xd;&#xa;--left outer join &#x28;select count&#x28;id&#x29; as rej_app_cnt_since_last_loan, client_id from lending.loan_application where &#x28;status &#x3d; &#x27;REJECTED&#x27; and application_date&#x3a;&#x3a;date &#x3e;&#x3d; lll.close_date&#x3a;&#x3a;Date --and close_reason not ilike &#x27;&#x25;BAD_SCORING&#x25;&#x27;&#xd;&#xa;--&#x29; group by client_id&#x29;cras  on &#x28;cras.client_id &#x3d; c.id&#x29;&#xd;&#xa;&#xd;&#xa;where 1&#x3d;1&#xd;&#xa;--and pi.receive_communication &#x3d; TRUE &#xd;&#xa;--and pi.receive_offers &#x3d; TRUE&#xd;&#xa;--and stt.status is null -- added to remove blocked communication&#xd;&#xa;--and str.status is null -- added to remove resigned clients&#xd;&#xa;--and cal2.act_loan_cnt is null &#xd;&#xa;--and c.id not in &#x28;23, 51&#x29; --test clients&#xd;&#xa;--and bl.blacklist_entries is null  --remove blacklisted&#xd;&#xa;--and ec.email not like &#x27;&#x25;blocked&#x25;&#x27;   --remove manually blocked clients&#xd;&#xa;&#xd;&#xa;order by c.id&#xd;&#xa;&#xd;&#xa;--limit 100</sql>
