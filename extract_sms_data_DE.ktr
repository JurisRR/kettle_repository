    <sql>-- &#x23;&#x23; Extract data on loan level including credit info, client, application data&#xd;&#xa;&#xd;&#xa;--&#x23;&#x23; aggregation of payments_distr table on loan level&#xd;&#xa;with dd as &#x28;select d.r_loan_id,&#xd;&#xa;sum&#x28;d.amount&#x29; as amount, sum&#x28;d.amount_principal&#x29; as amount_principal, &#xd;&#xa;       sum&#x28;d.amount_commission&#x29; as amount_commission, sum&#x28;d.amount_penalty&#x29; as amount_penalty, sum&#x28;d.amount_surplus&#x29; as amount_surplus,&#xd;&#xa;        sum&#x28;d.amount_prolongation&#x29; as amount_prolongation, sum&#x28;d.amount_fees&#x29; as amount_fees&#xd;&#xa;	&#xd;&#xa;from &#xd;&#xa;r_payment_distr d join r_payment p on  &#x28;d.r_payment_id &#x3d; p.r_payment_id&#x29; &#xd;&#xa;where 1&#x3d;1 &#xd;&#xa;	--and p.cl_bank_acct_id &#x3c;&#x3e; 18&#xd;&#xa;	--and p.cl_bank_acct_id &#x3c;&#x3e; 12&#xd;&#xa;	group by d.r_loan_id&#x29;&#xd;&#xa;&#xd;&#xa;,pay as &#x28;select d.r_loan_id, min&#x28;payment_date&#x29;&#x3a;&#x3a;date as first_payment_date &#xd;&#xa;	from r_payment_distr d join r_payment p on  &#x28;d.r_payment_id &#x3d; p.r_payment_id&#x29; &#xd;&#xa;		where p.type &#x3d; &#x27;IN_Loan&#x27; and p.cl_bank_acct_id &#x3c;&#x3e; 12 and cl_bank_acct_id &#x3c;&#x3e;  18&#xd;&#xa;		group by d.r_loan_id&#x29;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;-- &#x23;&#x23;&#x23;&#x23;&#x23; add also subquery to attach info on virtual transactions&#xd;&#xa;-- &#x23;&#x23; in case when surplus &#x3c; loan_amount, seems like loan defaulted, otherwise seems like a good loan&#xd;&#xa;&#xd;&#xa;---&#x23;&#x23;&#x23;&#x23;&#x23;&#xd;&#xa;	 &#xd;&#xa;&#xd;&#xa;,clci as &#x28;&#xd;&#xa;	 select &#x2a; from &#x28;select distinct r_client_id, id1, id2,&#xd;&#xa; decision, &#xd;&#xa; app_date,&#xd;&#xa; r_credit_info_id,&#xd;&#xa; rank&#x28;&#x29; over &#x28;partition by r_client_id, id1 order by r_client_id, id1, decision DESC, r_credit_info_id DESC &#x29; as rrow from&#xd;&#xa;&#x28;select  rra.r_client_id&#xd;&#xa;	,rra.r_application_id as id1&#xd;&#xa;	, raa.r_application_id as id2&#xd;&#xa;	,r_credit_info_id&#xd;&#xa;	--,decision&#xd;&#xa;	,case when decision &#x3d; &#x27;ERROR&#x27; then &#x27;ERROR&#x27;&#xd;&#xa;		 when decision is null then &#x27;no info&#x27;&#xd;&#xa;		else &#x27;ok&#x27; end as decision&#xd;&#xa;			,rra.app_date&#x3a;&#x3a;date&#xd;&#xa;	--,raa.app_date&#x3a;&#x3a;date&#xd;&#xa;&#xd;&#xa;	from&#xd;&#xa;	r_application rra join r_application raa on &#x28;rra.r_client_id &#x3d; raa.r_client_id&#x29;-- and raa.app_date &#x3c; rra.app_date&#x29;&#xd;&#xa;	left outer join r_credit_info ci on &#x28;raa.r_application_id &#x3d; ci.r_application_id&#x29;&#xd;&#xa;&#xd;&#xa;	where 1&#x3d;1 &#xd;&#xa;	--and rra.r_client_id &#x3d; 45254&#xd;&#xa;	--in &#x28;&#xd;&#xa;&#xd;&#xa;	and raa.r_application_id &#x3c;&#x3d; rra.r_application_id&#xd;&#xa;	and decision  &#x3c;&#x3e; &#x27;no info&#x27;&#xd;&#xa;	order by rra.r_client_id, rra.r_application_id, raa.r_application_id DESC&#x29;aa&#xd;&#xa;&#xd;&#xa;	order by id1, decision DESC&#x29;cc&#xd;&#xa;	where rrow &#x3d;1&#x29;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;, neg as 	&#xd;&#xa;	&#x28;with&#xd;&#xa;xx as &#x28;select r_credit_info_id, status, count&#x28;status&#x29; as count_isnegative from r_credit_info_negative where status &#x3d; &#x27;IsNegative&#x27; group by 1,2&#x29;,&#xd;&#xa;&#xd;&#xa;yy as &#x28;select r_credit_info_id, status, count&#x28;status&#x29; as count_wasnegative from r_credit_info_negative where status &#x3d; &#x27;WasNegative&#x27; group by 1,2&#x29;&#xd;&#xa;&#xd;&#xa;SELECT r.r_credit_info_id, r.debtor_existing_operations, &#xd;&#xa;       r.debtor_terminated_operations ,&#xd;&#xa;        xx.count_isnegative, yy.count_wasnegative&#xd;&#xa;FROM r_credit_info r&#xd;&#xa;    left outer  join xx xx&#xd;&#xa;  on &#x28;r.r_credit_info_id &#x3d; xx.r_credit_info_id&#x29; &#xd;&#xa;  left outer join yy yy on &#x28;r.r_credit_info_id &#x3d; yy.r_credit_info_id&#x29;&#x29;&#xd;&#xa;-- need to add count of credit calls for errors&#xd;&#xa;---&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23;	&#xd;&#xa;&#xd;&#xa;SELECT distinct l.r_loan_id, l.loan_Date&#x3a;&#x3a;date,l.status, substring&#x28;l.loan_no from &#x27;..&#x24;&#x27;&#x29;&#x3a;&#x3a;numeric loan_seq, l.loan_amount, l.loan_commission,&#xd;&#xa;&#xd;&#xa;dd.amount, dd.amount_principal, &#xd;&#xa;       dd.amount_commission, dd.amount_penalty, dd.amount_surplus, dd.amount_prolongation, dd.amount_fees&#xd;&#xa;      -- d.is_early_repayment, d.ad_client_id, d.isactive, p.type&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;       ,&#xd;&#xa;       a.amount as application_amount,&#xd;&#xa;       a.original_amount as original_application_amount,&#xd;&#xa;       a.term,&#xd;&#xa;       a.original_term,&#xd;&#xa;       a.ip_address,&#xd;&#xa;       a.score,&#xd;&#xa;       &#x28;a.answered_time&#x3a;&#x3a;date-a.app_date&#x3a;&#x3a;date&#x29; as days_to_answer&#xd;&#xa;       &#xd;&#xa;&#xd;&#xa;&#xd;&#xa;,c.r_client_id&#xd;&#xa;,c.gender&#xd;&#xa;,c.dob&#xd;&#xa;,c.income_amount&#xd;&#xa;,c.liabilities_amount&#xd;&#xa;,c.employment_type&#xd;&#xa;,c.mobile_operator&#xd;&#xa;,c.actual_address_city&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;,ci.decision&#xd;&#xa;,ci.reason&#xd;&#xa;,ci.probability_of_default&#xd;&#xa;,ci.inquiries_last_1_month&#xd;&#xa;,ci.inquiries_last_3_month&#xd;&#xa;,ci.inquiries_last_6_month&#xd;&#xa;,ci.inquiries_last_12_month&#xd;&#xa;,ci.debtor_existing_operations&#xd;&#xa;,ci.debtor_terminated_operations&#xd;&#xa;,ci.guarantor_existing_operations&#xd;&#xa;,ci.guarantor_terminated_operations&#xd;&#xa;,ci.guarantor_unpaid_instalments&#xd;&#xa;,ci.debtor_total_overdue&#xd;&#xa;,ci.debtor_total_outstanding&#xd;&#xa;,ci.r_credit_info_id&#xd;&#xa;,ci.r_application_id as ci_application_id&#xd;&#xa;,a.r_application_id as loan_application_id&#xd;&#xa;&#xd;&#xa;,neg.count_isnegative&#xd;&#xa;,neg.co