riod	&#xd;&#xa;&#xd;&#xa;-- surpluses should be correct &#xd;&#xa;--&#x23;&#x23;&#x23;&#x23;&#x23; FPDs  &#xd;&#xa;-- &#x23;&#x23;&#x23;&#x23;&#x23;&#x23;&#x23; &#xd;&#xa;,case when l.status &#x3d; &#x27;Completed&#x27; then &#x28;l.loan_date&#x3a;&#x3a;date&#x2b;l.term_days&#x29;&#x3a;&#x3a;date - pay.last_payment_date &#xd;&#xa;	else null end &#xd;&#xa;	as finished_n_days_earlier&#xd;&#xa;&#xd;&#xa;, case when &#x28;&#x28;l.loan_date&#x3a;&#x3a;date&#x2b;l.term_days&#x29;&#x3a;&#x3a;date&#x2b;0&#x29; &#x3e; current_date and pay.first_payment_date is NULL then 6 --&#xd;&#xa;	when pay.first_payment_date is null then 1&#xd;&#xa;	WHEN  pay.first_payment_date&#x3a;&#x3a;date &#x3c;&#x3d; &#x28;&#x28;l.loan_date&#x3a;&#x3a;date&#x2b;l.term_days&#x29;&#x3a;&#x3a;date&#x2b;0&#x29; THEN 0&#xd;&#xa;     WHEN pay.first_payment_date&#x3a;&#x3a;date &#x3e;  &#x28;&#x28;l.loan_date&#x3a;&#x3a;date&#x2b;l.term_days&#x29;&#x3a;&#x3a;date&#x2b;0&#x29; THEN 1&#xd;&#xa;	else -1 end as FPD0&#xd;&#xa;&#xd;&#xa;, case when &#x28;&#x28;l.loan_date&#x3a;&#x3a;date&#x2b;l.term_days&#x29;&#x3a;&#x3a;date&#x2b;5&#x29; &#x3e; current_date and pay.first_payment_date is NULL then 6 --&#xd;&#xa;	when pay.first_payment_date is null then 1&#xd;&#xa;	WHEN  pay.first_payment_date&#x3a;&#x3a;date &#x3c;&#x3d; &#x28;&#x28;l.loan_date&#x3a;&#x3a;date&#x2b;l.term_days&#x29;&#x3a;&#x3a;date&#x2b;5&#x29; THEN 0&#xd;&#xa;     WHEN pay.first_payment_date&#x3a;&#x3a;date &#x3e;  &#x28;&#x28;l.loan_date&#x3a;&#x3a;date&#x2b;l.term_days&#x29;&#x3a;&#x3a;date&#x2b;5&#x29; THEN 1&#xd;&#xa;	else -1 end as FPD5&#xd;&#xa;&#xd;&#xa;, case when &#x28;&#x28;l.loan_date&#x3a;&#x3a;date&#x2b;l.term_days&#x29;&#x3a;&#x3a;date&#x2b;7&#x29; &#x3e; current_date and pay.first_payment_date is NULL then 6 --&#xd;&#xa;	when pay.first_payment_date is null then 1&#xd;&#xa;	WHEN  pay.first_payment_date&#x3a;&#x3a;date &#x3c;&#x3d; &#x28;&#x28;l.loan_date&#x3a;&#x3a;date&#x2b;l.term_days&#x29;&#x3a;&#x3a;date&#x2b;7&#x29; THEN 0&#